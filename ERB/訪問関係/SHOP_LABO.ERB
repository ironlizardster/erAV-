	CALL MODIFY_BONYU_ERASE
ELSEIF RESULT == 11
	CALL MODIFY_OMORASHI_ERASE
ELSEIF RESULT == 12
	CALL SHOJO_SAISEI
ELSEIF RESULT == 30
