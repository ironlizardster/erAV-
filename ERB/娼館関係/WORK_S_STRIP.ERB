;===========================================================
;ストリップショー関係の処理はここで
;===========================================================
;eraIm@s_ver.0.17βdのダンスショーより流用して改造(pawapokya-)
;===========================================================
@STRIPTEASE
DRAWLINE

;特殊チケットフラグ初期化
FLAG:特殊営業で夜の部を開催フラグ = 0

;ショーの形式を初期化
FLAG:特殊営業タイプ詳細 = 0

;夜の場合、通常ストリップショーか特殊ストリップショーかを選択
PRINTL どのチケットを販売しますか？
PRINTL  [0] - 通常チケット
SIF TIME == 1
	PRINTL  [1] - 特殊チケット
PRINTL  [100] - やめる
	
$INPUT_LOOP
INPUT

IF RESULT == 0
	PRINTFORMW 通常チケットを販売します
ELSEIF RESULT == 1 && TIME == 1
	PRINTFORMW 特殊チケットを販売します。ストリップショー後にお楽しみがあります
	FLAG:特殊営業で夜の部を開催フラグ = 1
	;コンドーム使用フラグ外す
	CFLAG:営業コンドーム = 0
ELSEIF RESULT == 100
	RETURN 100
ELSE
	GOTO INPUT_LOOP
ENDIF

DRAWLINE

;ローカル変数初期化
A = 0
B = 0
C = 0
D = 0
E = 0
F = 0

G = 0
H = 0
I = 0
J = 0
N = 0
O = 0
P = 0
Q = 0
R = 0
S = 0
;L = 呼び出し元で代入される参加人数

;COUNT用待避変数
X = 0

REPEAT 101
	TCVAR:COUNT = 0
	U:COUNT = 0
REND


;調教対象と助手を空に
TARGET = -1
ASSI = -1
;参加人数が一人の場合はTARGETにそのキャラを代入
IF FLAG:特殊営業参加人数 == 1
	REPEAT CHARANUM
		SIF CFLAG:COUNT:特殊営業参加
			TARGET = COUNT
	REND
;参加人数が二人の場合は調教自慰経験の高い方をTARGETに、もう一人をASSIに代入
ELSEIF FLAG:特殊営業参加人数 == 2
	REPEAT CHARANUM
		SIF CFLAG:COUNT:特殊営業参加 == 0
			CONTINUE
		IF TARGET == -1
			TARGET = COUNT
		ELSEIF EXP:COUNT:調教自慰経験 > EXP:TARGET:調教自慰経験
			ASSI = TARGET
			TARGET = COUNT
		ELSE
			ASSI = COUNT
		ENDIF
	REND
;参加人数が３人以上の場合は、最も調教自慰経験の高い者をTARGETに代入
ELSEIF FLAG:特殊営業参加人数 >= 3
	E = -1
	REPEAT CHARANUM
		SIF CFLAG:COUNT:特殊営業参加 == 0
			CONTINUE
		IF E < EXP:COUNT:調教自慰経験
			TARGET = COUNT
			E = EXP:COUNT:調教自慰経験
		ENDIF
	REND
	E = 0
ENDIF

;参加者が２人の場合はキャラ同志の近親チェック
IF FLAG:特殊営業参加人数 == 2
	PLAYER = ASSI
	TFLAG:近親相姦 = 0
	CALL INCEST
ENDIF

;FLAG:53でショーの形式を決定
FLAG:特殊営業タイプ詳細 = 0
;&1:オナニー &2:レズ &4:セックス &8:近親
;参加者2人で近親関係なら近親ショー
SIF FLAG:特殊営業参加人数 == 2 && ((TALENT:オトコ || TALENT:ふたなり) || (TALENT:ASSI:オトコ || TALENT:ASSI:ふたなり))
	FLAG:特殊営業タイプ詳細 |= 4
E = 0
;参加者全員が自慰中毒3以上か自慰狂いならオナニーショー
E = 1
REPEAT CHARANUM
	SIF CFLAG:COUNT:特殊営業参加 == 0
		CONTINUE
	SIF ABL:COUNT:自慰中毒 < 3 && TALENT:COUNT:自慰狂い == 0
		E = 0
REND
SIF E
	FLAG:特殊営業タイプ詳細 |= 1
E = 0
;参加者複数で全員がレズっ気2以上ならレズショー
E = 1
REPEAT CHARANUM
	SIF CFLAG:COUNT:特殊営業参加 == 0
		CONTINUE
	SIF ABL:COUNT:レズっ気 < 2
		E = 0
REND
SIF E && FLAG:特殊営業参加人数 >= 2
	FLAG:特殊営業タイプ詳細 |= 2
E = 0

;特殊営業参加キャラの相性をチェック
CALL WORK_SUB_RELATION

;客の数を計算
;参加するキャラの人気の最大値に応じて客が来ます
E = 0
REPEAT CHARANUM
	SIF E < EXP:COUNT:人気 && CFLAG:COUNT:特殊営業参加
		E = EXP:COUNT:人気
REND
C = E / 4
G = C / 2
IF C
	IF C < 13
		REPEAT C
			G += RAND:2
		REND
	ELSEIF C < 16
		TIMES G , 1.10
		REPEAT C
			G += RAND:2
		REND
	ELSEIF C < 20
		TIMES G , 1.30
		REPEAT C
			G += RAND:2
		REND
	ELSEIF C < 24
		TIMES G , 1.50
		REPEAT C
			G += RAND:2
		REND
	ELSE
		TIMES G , 2.00
		REPEAT C
			G += RAND:3
		REND
	ENDIF
ENDIF

;娼館の人気に応じてさらにプラス
C = (EXP:MASTER:人気 / 5)
IF C
	REPEAT C
		G += RAND:2
	REND
ENDIF

;参加者が特定の素質を持っていた場合ボーナス
REPEAT CHARANUM
	SIF CFLAG:COUNT:特殊営業参加 == 0
		CONTINUE
	;社会的ステータスが影響
	;人気者
	SIF TALENT:COUNT:人気者
		G += 10
	;アイドル
	SIF TALENT:COUNT:アイドル
		G += 5
	;娼婦
	SIF TALENT:COUNT:風俗嬢
		G += 2
	;傾城
	SIF TALENT:COUNT:高級娼婦
		G += 3
	;饒舌
	SIF TALENT:COUNT:饒舌
		G += 2
	;常連客
	SIF TALENT:COUNT:常連客
		G += 2
	;求愛
	SIF TALENT:COUNT:彼氏あり
		G += 1
	;歌姫
	SIF TALENT:COUNT:歌姫
		G += 5
	;舞姫
	SIF TALENT:COUNT:舞姫
		G += 15
	;美姫
	SIF TALENT:COUNT:美姫
		G += 10
	;優等生
	SIF TALENT:COUNT:優等生
		G += 5
	;人気ＡＶ女優
	SIF TALENT:COUNT:人気ＡＶ女優
		G += 3
	;ファッションモデル
	SIF TALENT:COUNT:ファッションモデル
		G += 3
		
	;専用素質(まあ(差別化する必要もあるし)多少はね？)
	;極道の娘
	SIF TALENT:COUNT:極道の娘
		G += 20
	;淫魔
	SIF TALENT:COUNT:淫魔
		G += 30
	;声優
	SIF TALENT:COUNT:声優
		G += 5
	;はるうられ
	SIF TALENT:COUNT:はるうられ
		G += 10
	;ColorfulPureGirls
	SIF TALENT:COUNT:ColorfulPureGirls
		G += 15
	;神巫
	SIF TALENT:COUNT:神巫
		G += 25
	;天使
	SIF TALENT:COUNT:天使
		G += 25
	;グランドマスター
	SIF TALENT:COUNT:グランドマスター
		G += 20
	;極道の妻
	SIF TALENT:COUNT:極道の妻
		G += 15
	;Mermaid girl
	SIF TALENT:COUNT:[[TALENT:Mermaid girl]]
		G += 20
	;FallenFallAngel
	;Triptych
	SIF TALENT:COUNT:Triptych
		G += 30
	;Emerald Queen
	SIF TALENT:COUNT:[[TALENT:Emerald Queen]]
		G += 25
	;堕ちた歌姫
	SIF TALENT:COUNT:[[TALENT:平和の歌姫？]] || TALENT:COUNT:堕ちた歌姫
		G += 20
	;ルーンマスター 
	SIF TALENT:COUNT:ルーンマスター || TALENT:COUNT:奇蹟
		G += 20
	;ONLY ONE IDOL
	SIF TALENT:COUNT:[[TALENT:ONLY ONE IDOL]] || TALENT:COUNT:ILMATAR || TALENT:COUNT:AVALOKITESVARA || TALENT:COUNT:QueenB
		G += 40
	;SUPER HEROINE
	SIF TALENT:COUNT:[[TALENT:SUPER HEROINE]]
		G += 25
REND

	;警備員の数に応じて観客数の限界が伸びます。500000Pをケチってはいけない(戒め)
	IF G > 50 && FLAG:警備員人数 < 10
		IF (50 + (FLAG:警備員人数 * 5)) < G
			G = (50 + (FLAG:警備員人数 * 5))
		ENDIF
	ENDIF

;通常チケットなら2/3程度の人数になる
SIF FLAG:特殊営業で夜の部を開催フラグ == 0
	G = G * 2 / 3

;10人以下だった場合、10人にします
SIF G < 10
	G = 10

;次に成功か失敗かの判定を計算
;参加者の調教自慰経験の平均値によって基礎値が決定
E = 0
REPEAT CHARANUM
	SIF CFLAG:COUNT:特殊営業参加 == 0
		CONTINUE
	E += EXP:COUNT:調教自慰経験
REND
E /= FLAG:特殊営業参加人数
;調教自慰経験によって基礎値が決定
IF E < 10
	D = 0 + RAND:5
ELSEIF E < 20
	D = 1 + RAND:5
ELSEIF E < 30
	D = 2 + RAND:6
ELSEIF E < 40
	D = 4 + RAND:6
ELSEIF E < 50
	D = 7 + RAND:7
ELSEIF E < 75
	D = 10 + RAND:8
ELSEIF E < 100
	D = 13 + RAND:9
ELSEIF E < 150
	D = 16 + RAND:10
ELSEIF E < 200
	D = 20 + RAND:12
ELSEIF E < 300
	D = 25 + RAND:14
ELSEIF E < 500
	D = 30 + RAND:16
ELSEIF E < 1000
	D = 40 + RAND:18
ELSE
	D = 50 + RAND:20
ENDIF
E = 0

;参加者全員の技能・素質による修正値を加算
REPEAT CHARANUM
	SIF CFLAG:COUNT:特殊営業参加 == 0
		CONTINUE

	;全員の露出癖をプラス
	D += ABL:COUNT:露出癖

	;公開セックスがある場合は全員の性交中毒の1/2をプラス
	SIF FLAG:特殊営業タイプ詳細 & 4
		D += ABL:COUNT:性交中毒 / 2
	;レズショーがある場合は全員のレズ中毒の1/2をプラス
	SIF FLAG:特殊営業タイプ詳細 & 2
		D += ABL:COUNT:レズ中毒 / 2
	;公開オナニーがある場合は全員の自慰中毒の1/2をプラス
	SIF FLAG:特殊営業タイプ詳細 & 1
		D += ABL:COUNT:自慰中毒 / 2

	;技巧と舞踏技能は1/3をプラス
	D += ABL:COUNT:技巧 / 3
	D += ABL:COUNT:舞踊技能 / 3

	;全員の美容感覚をプラス
	D += ABL:COUNT:美容感覚

	;素質に応じて加減
	;性格的な素質が影響
	;臆病
	SIF TALENT:COUNT:臆病
		D -= 1
	;大人しい
	SIF TALENT:COUNT:大人しい
		D -= 1
	;プライド高い
	SIF TALENT:COUNT:プライド高い
		D += 2
	;プライド低い
	SIF TALENT:COUNT:プライド低い
		D -= 2
	;感情乏しい
	SIF TALENT:COUNT:感情乏しい
		D -= 1
	;好奇心
	SIF TALENT:COUNT:好奇心
		D += 1
	;目立ちたがり
	SIF TALENT:COUNT:目立ちたがり
		D += 3
	;抑圧
	SIF TALENT:COUNT:抑圧
		D -= 2
	;解放
	SIF TALENT:COUNT:解放
		D += 2
	;抵抗
	SIF TALENT:COUNT:抵抗
		D -= 2
	;恥じらい
	SIF TALENT:COUNT:恥じらい
		D -= 2
	;恥薄い
	SIF TALENT:COUNT:恥薄い
		D += 2
	;自慰しやすい
	SIF TALENT:COUNT:自慰しやすい
		D += 5
	;快感に素直
	SIF TALENT:COUNT:快感に素直
		D += 3
	;淫乱
	SIF TALENT:COUNT:淫乱
		D += 10
	;倒錯的
	SIF TALENT:COUNT:倒錯的
		D += 2
	;露出狂
	SIF TALENT:COUNT:露出狂
		D += 10
	;謎の魅力
	SIF TALENT:COUNT:謎の魅力
		D += 15

	;公開セックスがある場合
	IF FLAG:特殊営業タイプ詳細 & 4
		;セックス狂い
		SIF TALENT:COUNT:セックス狂
			D += 10
		;性別が同じで両刀
		SIF TALENT:COUNT:両刀 && (TALENT:TARGET:オトコ == TALENT:ASSI:オトコ)
			D += 5
		;片方が女で女嫌い
		SIF TALENT:COUNT:ボーイッシュ && (TALENT:TARGET:オトコ == 0 || TALENT:ASSI:オトコ == 0)
			D -= 3
		;片方が男で男嫌い
		SIF TALENT:COUNT:男性不信 && (TALENT:TARGET:オトコ || TALENT:ASSI:オトコ)
			D -= 3
	ENDIF

	;レズショーがある場合
	IF FLAG:特殊営業タイプ詳細 & 2
		;両刀
		SIF TALENT:COUNT:両刀
			D += 5
		;女嫌い
		SIF TALENT:COUNT:ボーイッシュ
			D -= 5
	ENDIF

	;公開オナニーがある場合
	IF FLAG:特殊営業タイプ詳細 & 1
		;自慰しやすい
		SIF TALENT:COUNT:自慰しやすい
			D += 2
		;自慰狂い
		SIF TALENT:COUNT:自慰狂い
			D += 10
	ENDIF

	;肥満があると満足度半減
	SIF TALENT:COUNT:肥満
		D /= 2

	;狂気・崩壊があると満足度激減
	SIF TALENT:COUNT:狂気 || TALENT:COUNT:崩壊
		D /= 5
REND

;相性による修正
IF FLAG:特殊営業時のキャラの相性 >= 200
	TIMES D , 2.00
ELSEIF FLAG:特殊営業時のキャラの相性 >= 150
	TIMES D , 1.50
ELSEIF FLAG:特殊営業時のキャラの相性 >= 120
	TIMES D , 1.20
ELSEIF FLAG:特殊営業時のキャラの相性 < 100
	TIMES D , 0.50
ENDIF

;満足度Dが目標値Pを上回れば成功、下回れば失敗
;２週目以降は相性値込でなければまず超成功は狙えないでしょう。
IF FLAG:周回数 > 1
	P = 41 + RAND:10
ELSE
	P = 21 + RAND:5
ENDIF

;特殊チケットの場合は客の目的が目的なのでちょっと下がる
SIF FLAG:特殊営業で夜の部を開催フラグ == 1
	P -= 5

;満足度DがPの50%未満だったら大失敗
IF D < (P / 2)
	A = 0
;P未満だったら小失敗
ELSEIF D < P
	A = 1
;Pの150%未満だったら普通
ELSEIF D < (P * 3) / 2
	A = 2
;Pの200%未満だったら小成功
ELSEIF D < P * 2
	A = 3
;Pの400%未満だったら大成功
ELSEIF D < P * 4
	A = 4
;それ以上だったら超成功
ELSE
	A = 5
ENDIF
Z = 0
Z = A

;成功値が低いと一部のショーは行われない
SIF A < 2
	FLAG:特殊営業タイプ詳細 -= FLAG:特殊営業タイプ詳細 & 1
SIF A < 3
	FLAG:特殊営業タイプ詳細 -= FLAG:特殊営業タイプ詳細 & 2
SIF A < 4
	FLAG:特殊営業タイプ詳細 -= FLAG:特殊営業タイプ詳細 & 4

IF FLAG:特殊営業参加人数 == 1
	PRINTFORM %NAME:TARGET%
ELSEIF FLAG:特殊営業参加人数 == 2
	PRINTFORM %NAME:TARGET%と%NAME:ASSI%
ELSE
	S = 0
	REPEAT CHARANUM
		SIF CFLAG:COUNT:特殊営業参加 == 0
			CONTINUE
		SIF S
			PRINT と
		PRINTFORM %NAME:COUNT%
		S = 1
	REND
ENDIF

IF FLAG:特殊営業タイプ詳細 & 8
	IF TFLAG:近親相姦 == 2 && TALENT:PLAYER:オトコ == 0 && TALENT:ASSI:オトコ == 0
		PRINT による母と娘の
	ELSEIF TFLAG:近親相姦 == 2 && TALENT:オトコ == 0 && TALENT:ASSI:オトコ
		PRINT による母と息子の
	ELSEIF TFLAG:近親相姦 == 4 && TALENT:PLAYER:オトコ == 0 && TALENT:ASSI:オトコ == 0
		PRINT による姉と妹の
	ELSEIF TFLAG:近親相姦 == 4 && TALENT:オトコ == 0 && TALENT:ASSI:オトコ
		PRINT による姉と弟の
	ELSEIF TFLAG:近親相姦 == 4 && TALENT:オトコ && TALENT:ASSI:オトコ == 0
		PRINT による兄と妹の
	ELSE
		PRINT による近親
	ENDIF
ELSE
	PRINT の
ENDIF
IF FLAG:特殊営業タイプ詳細 & 4
	PRINT 公開セックス
ELSEIF FLAG:特殊営業タイプ詳細 & 2
	PRINT レズビアン
ELSEIF FLAG:特殊営業タイプ詳細 & 1
	PRINT 公開オナニー
ELSE
	PRINT ストリップ
ENDIF
PRINTL ショーを開いた……
PRINTFORML 今回のストリップショーには{G}人の観客が集まった
WAIT

;通常ストリップ
PRINT スポットライトを浴びながら、
CALL WORK_SUB_PRINT_MEMBER
PRINTL は観客に裸身を晒している……

WAIT

;公開オナニー
IF FLAG:特殊営業タイプ詳細 & 1
	PRINT そして
	CALL WORK_SUB_PRINT_MEMBER
	PRINTL は客席に向かって足を開いて秘所をさらけ出し、
	PRINTL いやらしい音を立てて性器をまさぐり始めた……
	WAIT
	;近親オナニーショー
	IF FLAG:特殊営業タイプ詳細 & 8
		PRINT 無数の目が見守る中で並んでオナニーを続けている
		IF TFLAG:近親相姦 == 2  && TALENT:オトコ
			PRINT 父
		ELSEIF TFLAG:近親相姦 == 2  && TALENT:オトコ == 0
			PRINT 母
		ELSEIF TFLAG:近親相姦 == 4  && TALENT:オトコ
			PRINT 兄
		ELSEIF TFLAG:近親相姦 == 4  && TALENT:オトコ == 0
			PRINT 姉
		ENDIF
		PRINT と
		IF TFLAG:近親相姦 == 2  && TALENT:ASSI:オトコ
			PRINT 息子
		ELSEIF TFLAG:近親相姦 == 2  && TALENT:ASSI:オトコ == 0
			PRINT 娘
		ELSEIF TFLAG:近親相姦 == 4  && TALENT:ASSI:オトコ
			PRINT 弟
		ELSEIF TFLAG:近親相姦 == 4  && TALENT:ASSI:オトコ == 0
			PRINT 妹
		ENDIF
		PRINTL に、
		PRINTL 客席から卑猥な野次が飛んだ……
		WAIT
	ENDIF
ENDIF

;レズショー
IF FLAG:特殊営業タイプ詳細 & 2
	PRINT やがて
	CALL WORK_SUB_PRINT_MEMBER
	PRINTL は互いの裸身を淫らに絡めあい始めた……
	WAIT
	;近親レズビアンショー
	IF FLAG:特殊営業タイプ詳細 & 8
		IF TFLAG:近親相姦 == 2
			PRINTL Ｍ字に開かれた母親の股間に顔をうずめ、
			PRINT 自分が生まれてきた場所に舌を這わせる
			IF TALENT:ASSI:未熟
				PRINT 幼女
			ELSEIF TALENT:ASSI:幼稚
				PRINT 少女
			ELSE
				PRINT 娘
			ENDIF
		ELSEIF TFLAG:近親相姦 == 4
			PRINT 互いにさかさまになって相手の性器を舐めあう姉と妹
		ENDIF
		PRINTL の姿を、
		PRINTL 観客は固唾を呑んで見守っている……
		WAIT
	ENDIF
ENDIF

;公開セックス
IF FLAG:特殊営業タイプ詳細 & 4
	E = 0
	PRINTW 本番行為が始まった……
	;TARGETとASSI双方がふたなりでＶ使用可
	IF TALENT:TARGET:ふたなり && CFLAG:TARGET:売春Ｖ不能 == 0 && TALENT:ASSI:ふたなり && CFLAG:ASSI:売春Ｖ不能 == 0
		PRINTFORM %CALLNAME:TARGET%と%CALLNAME:ASSI%は
		PRINTFORMW お互いのペニスでお互いのヴァギナを交互に貫きあった
		SIF TALENT:処女
			E |= 1
		SIF TALENT:童貞
			E |= 2
		SIF TALENT:ASSI:処女
			E |= 4
		SIF TALENT:ASSI:童貞
			E |= 8
		;助手→奴隷の妊娠フラグを加算
		SIF CFLAG:営業コンドーム == 0
			CFLAG:助奴膣内精子量 += G / 10 + 1
		;奴隷→助手の妊娠フラグを加算
		SIF CFLAG:営業コンドーム == 0
