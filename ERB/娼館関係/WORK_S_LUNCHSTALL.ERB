	SIF FLAG:特殊営業で夜の部を開催フラグ && TARGET != X && TALENT:乳狂い == 0
		TIMES U:100 , 3.00
	
	DRAWLINE
	
	Y = COUNT
	CALL WORK_EXP
	COUNT = Y

	;反発刻印取得の判定
	;従順の200倍+1000まで許容
	O = ABL:信頼 * 200 + 1000
	
	;反発刻印LV2はLV1の√3倍、LV3は√5倍で獲得
	I = O
	TIMES I , 2.23
	J = O
	TIMES J , 1.73
	
	IF MARK:反発刻印 != 3 && JUEL:否定 > I
		PRINTL  反発刻印LV3を取得
		MARK:反発刻印 = 3
	ELSEIF MARK:反発刻印 != 2 && JUEL:否定 > J
		PRINTL  反発刻印LV2を取得
		MARK:反発刻印 = 2
	ELSEIF MARK:反発刻印 != 1 && JUEL:否定 > O
		PRINTL  反発刻印LV1を取得
		MARK:反発刻印 = 1
	ENDIF
	
	DRAWLINE
	
	;体力減少の計算
	LOSEBASE:体力 = G * 20
	
	;小柄だと疲労増加
	SIF TALENT:小柄
		TIMES LOSEBASE:体力 , 1.20

	;おっぱい屋台なら接客係はかなり疲れる
	SIF FLAG:特殊営業で夜の部を開催フラグ && TARGET != X 
		TIMES LOSEBASE:体力 , 4.00

	;接客係なら人数に応じて疲労軽減
	SIF TARGET != X && FLAG:特殊営業参加人数 >= 2
		LOSEBASE:体力 /= (FLAG:特殊営業参加人数 - 1)

	;厨房担当は料理技能に応じて疲労減
	IF TARGET == X
		IF ABL:料理技能 == 0
			TIMES LOSEBASE:体力 , 1.00
		ELSEIF ABL:料理技能 == 1
			TIMES LOSEBASE:体力 , 0.90
		ELSEIF ABL:料理技能 == 2
			TIMES LOSEBASE:体力 , 0.80
		ELSEIF ABL:料理技能 == 3
			TIMES LOSEBASE:体力 , 0.70
		ELSEIF ABL:料理技能 == 4
			TIMES LOSEBASE:体力 , 0.60
		ELSEIF ABL:料理技能 >= 5
			TIMES LOSEBASE:体力 , 0.50
		ENDIF
	ENDIF

	;運動能力に応じて疲労軽減
	IF ABL:運動能力 == 1
		TIMES LOSEBASE:体力 , 0.95
	ELSEIF ABL:運動能力 == 2
		TIMES LOSEBASE:体力 , 0.90
	ELSEIF ABL:運動能力 == 3
		TIMES LOSEBASE:体力 , 0.85
	ELSEIF ABL:運動能力 == 4
		TIMES LOSEBASE:体力 , 0.80
	ELSEIF ABL:運動能力 == 5
		TIMES LOSEBASE:体力 , 0.75
	ELSEIF ABL:運動能力 == 6
		TIMES LOSEBASE:体力 , 0.70
	ELSEIF ABL:運動能力 == 7
		TIMES LOSEBASE:体力 , 0.65
	ELSEIF ABL:運動能力 == 8
		TIMES LOSEBASE:体力 , 0.60
	ELSEIF ABL:運動能力 == 9
		TIMES LOSEBASE:体力 , 0.55
	ELSEIF ABL:運動能力 >= 10
		TIMES LOSEBASE:体力 , 0.50
	ENDIF
	
	SIF LOSEBASE:体力 >= BASE:体力
		LOSEBASE:体力 = BASE:体力
	
	PRINT  体力消費　　:
	N = BASE:体力
	CALL FIGURE_INDENT
	PRINTV BASE:体力
	PRINT  - 
	N = LOSEBASE:体力
	CALL FIGURE_INDENT
	PRINTV LOSEBASE:体力
	PRINT  = 
	N = BASE:体力 - LOSEBASE:体力
	CALL FIGURE_INDENT
	PRINTVL BASE:体力 - LOSEBASE:体力
	
	BASE:体力 -= LOSEBASE:体力
	
	PRINT  体力　　　　:
	BAR BASE:体力, MAXBASE:体力, 32
	PRINTFORML ({BASE:体力}/{MAXBASE:体力})
	
	;体力が0以下、または500以下だった場合の処理
	IF BASE:体力 <= 0
		PRINTFORML %CALLNAME:TARGET%は、屋台を閉めた直後に突っ伏すように倒れた
		PRINTFORML 何をやっても反応がない……
		WAIT
		;キャラの死亡確認
		CALL CHARADEAD_CHECK
	ELSEIF BASE:体力 <= 500
		PRINTFORML 体力が限界に来ています。休ませてあげてください
	ENDIF
	
	DRAWLINE
	WAIT
REND
