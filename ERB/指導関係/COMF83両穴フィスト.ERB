@COM83
;両穴フィスト
PRINTL 両穴フィスト
SAVESTR:0 = 両穴フィスト
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 800
LOSEBASE:気力 += 500

SOURCE:痛覚 = 1800
SOURCE:愛液分泌 = 2800
SOURCE:欲情 = 1500
SOURCE:露出 = 2500
SOURCE:屈従 = 2500
SOURCE:逸脱 = 2500
SOURCE:恭順 = 2500

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 40
	SOURCE:情愛 = 50
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 150
	SOURCE:情愛 = 150
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 400
	SOURCE:情愛 = 250
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 1000
	SOURCE:情愛 = 350
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 1700
	SOURCE:情愛 = 600
ELSE
	SOURCE:快Ｖ = 2200
	SOURCE:情愛 = 850
ENDIF


;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 10
	SOURCE:情愛 = 10
	SOURCE:屈従 = 100
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 30
	SOURCE:情愛 = 30
	SOURCE:屈従 = 700
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 500
	SOURCE:情愛 = 100
	SOURCE:屈従 = 1500
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 1000
	SOURCE:情愛 = 200
	SOURCE:屈従 = 3000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 1700
	SOURCE:情愛 = 450
	SOURCE:屈従 = 5000
ELSE
	SOURCE:快Ａ = 2200
	SOURCE:情愛 = 750
	SOURCE:屈従 = 8000
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛覚 += 20000
	SOURCE:欲情 += 2000

	IF HAVE_VAGINA(PLAYER) 
		EXP:異常経験 += 1
		PRINTFORML %EXPNAME:[[EXP:異常経験]]%＋１
	ENDIF
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.60
	SOURCE:痛覚 += 300
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
	SOURCE:痛覚 += 50
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
	SOURCE:痛覚 += 10
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.40
	SOURCE:痛覚 += 0
ELSE
	TIMES SOURCE:快Ｖ , 1.60
	SOURCE:痛覚 = 0
ENDIF


;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:快Ａ , 0.10
	SOURCE:痛覚 += 5000
	SOURCE:欲情 += 1000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:快Ａ , 0.30
	SOURCE:痛覚 += 2000
	SOURCE:欲情 += 1000
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:快Ａ , 0.50
	SOURCE:痛覚 += 2000
	SOURCE:欲情 += 1000
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛覚 += 2000
	SOURCE:欲情 += 1000
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛覚 += 1000
	SOURCE:欲情 += 200
ELSE
	TIMES SOURCE:快Ｖ , 1.60
	SOURCE:痛覚 += 600
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.20
	TIMES SOURCE:快Ａ , 0.20
	SOURCE:欲情 += 1000

	SOURCE:痛覚 += 1900
	TIMES SOURCE:痛覚 , 9.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:欲情 += 800

	SOURCE:痛覚 += 1250
	TIMES SOURCE:痛覚 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:快Ａ , 0.60
	SOURCE:欲情 += 600

	SOURCE:痛覚 += 1000
	TIMES SOURCE:痛覚 , 1.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:欲情 += 200

	SOURCE:痛覚 += 200
	TIMES SOURCE:痛覚 , 0.30
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:快Ａ , 1.30

	TIMES SOURCE:痛覚 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:快Ａ , 0.60
	TIMES SOURCE:屈従 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:快Ａ , 0.80
	TIMES SOURCE:屈従 , 0.80
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:快Ａ , 1.00
	TIMES SOURCE:屈従 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:屈従 , 1.20
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.40
	TIMES SOURCE:快Ａ , 1.40
	TIMES SOURCE:屈従 , 1.40
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:快Ａ , 0.70
	TIMES SOURCE:情愛 , 0.60
	TIMES SOURCE:欲情, 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:快Ａ , 0.90
	TIMES SOURCE:情愛 , 0.80
	TIMES SOURCE:欲情, 1.20
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:快Ａ , 1.00
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:欲情, 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:快Ａ , 1.10
	TIMES SOURCE:情愛 , 1.20
	TIMES SOURCE:欲情, 0.60
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:快Ｖ , 1.40
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:情愛 , 1.40
	TIMES SOURCE:欲情, 0.30
ELSE
	TIMES SOURCE:快Ｖ , 1.70
	TIMES SOURCE:快Ａ , 1.30
	TIMES SOURCE:情愛 , 1.60
	TIMES SOURCE:欲情, 0.10
ENDIF

;大柄
SIF IS_大柄(TARGET)
	TIMES SOURCE:痛覚 , 0.80
;小柄体型
SIF IS_小柄(TARGET)
	TIMES SOURCE:痛覚 , 3.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:痛覚 , 4.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF IS_Ａ鈍感(TARGET)
	TIMES SOURCE:痛覚 , 1.50
	TIMES SOURCE:欲情 , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF IS_Ａ敏感(TARGET)
	TIMES SOURCE:痛覚 , 0.60
	TIMES SOURCE:欲情 , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

EXP:Ａ経験 += 25
PRINTL Ａ経験＋２５

;貞操観念
IF IS_貞操観念(TARGET)
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:情愛 , 0.60
		TIMES SOURCE:欲情, 5.00
	ELSE
		TIMES SOURCE:情愛 , 0.60
		TIMES SOURCE:欲情, 1.80
	ENDIF
;貞操無頓着
ELSEIF IS_貞操無頓着(TARGET)
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:欲情, 0.50
	ELSE
		TIMES SOURCE:欲情, 0.30
	ENDIF
ENDIF

EXP:Ｖ経験 += 25
PRINTL Ｖ経験＋２５

IF EXP:Ｖ拡張経験 == 0 
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

EXP:Ｖ拡張経験 += 1
PRINTL Ｖ拡張経験＋1

IF EXP:Ａ拡張経験 == 0 
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

EXP:Ａ拡張経験 += 3
PRINTL Ａ拡張経験＋3

RETURN 1
;
