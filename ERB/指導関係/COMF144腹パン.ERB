;-------------------------------------------------
;腹パン
;SM系コマンド
;-------------------------------------------------
@COM144

PRINTL 腹パン
SAVESTR:0 = 腹パン
CALL TRAIN_MESSAGE_B

;実際には苦痛があるためもっと体力気力をもっていかれる
LOSEBASE:体力 += 0
LOSEBASE:気力 += 100

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
SOURCE:逸脱 = 1000

;PALAM:苦痛をみる
IF PALAM:苦痛 < PALAMLV:1
	SOURCE:痛覚 = 5000
ELSEIF PALAM:苦痛 < PALAMLV:2
	SOURCE:痛覚 = 5300
ELSEIF PALAM:苦痛 < PALAMLV:3
	SOURCE:痛覚 = 5600
ELSEIF PALAM:苦痛 < PALAMLV:4
	SOURCE:痛覚 = 6000
ELSEIF PALAM:苦痛 >= PALAMLV:4
	SOURCE:痛覚 = 6500
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF HAVE_VAGINA(TARGET) && HAVE_VAGINA(PLAYER) 
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF IS_MALE(TARGET) && IS_MALE(PLAYER) 
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

;屈服刻印３に相当
TFLAG:屈服刻印2 = 3

SIF ASSIPLAY == 0 && ABL:マゾっ気 >= 3
	TFLAG:主人経験 += 1

RETURN 1
