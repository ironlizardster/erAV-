;-------------------------------------------------
;浣腸セット
;SM系コマンド
;-------------------------------------------------
@COM46

IF TEQUIP:触手召喚
	PRINTL 触手浣腸
	SAVESTR:0 = 触手浣腸
ELSE
	PRINTL 浣腸器＋プラグ
	SAVESTR:0 = 浣腸セット
ENDIF
CALL TRAIN_MESSAGE_B

LOSEBASE:体力 += 60
LOSEBASE:気力 += 150

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 80
	SOURCE:屈従 = 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 250
	SOURCE:屈従 = 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 600
	SOURCE:屈従 = 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 1000
	SOURCE:屈従 = 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 1300
	SOURCE:屈従 = 2100
ELSE
	SOURCE:快Ａ = 1700
	SOURCE:屈従 = 2400
ENDIF

;ABL8,マゾっ気をみる
IF ABL:マゾっ気 < 1
	SOURCE:痛覚 = 2000
	SOURCE:不潔 = 1000
	SOURCE:屈従 = 200
	SOURCE:逸脱 = 1000
	SOURCE:反感 = 2000
ELSEIF ABL:マゾっ気 <2
	SOURCE:痛覚 = 1600
	SOURCE:不潔 = 2000
	SOURCE:屈従 = 500
	SOURCE:逸脱 = 1000
	SOURCE:反感 = 1000
ELSEIF ABL:マゾっ気 < 3
	SOURCE:痛覚 = 1200
	SOURCE:不潔 = 1000
	SOURCE:屈従 = 800
	SOURCE:逸脱 = 1000
	SOURCE:反感 = 500
ELSEIF ABL:マゾっ気 < 4
	SOURCE:痛覚 = 800
	SOURCE:不潔 = 1000
	SOURCE:屈従 = 1200
	SOURCE:逸脱 = 1000
	SOURCE:反感 = 100
ELSEIF ABL:マゾっ気 < 5
	SOURCE:痛覚 = 600
	SOURCE:不潔 = 1000
	SOURCE:屈従 = 1500
	SOURCE:逸脱 = 1000
	SOURCE:反感 = 0
ELSE
	SOURCE:痛覚 = 400
	SOURCE:不潔 = 1000
	SOURCE:屈従 = 2000
	SOURCE:逸脱 = 1000
	SOURCE:反感 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛覚 += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛覚 += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛覚 += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛覚 += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛覚 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	TIMES SOURCE:快Ａ , 1.30
ENDIF

;大柄
SIF IS_大柄(TARGET)
	TIMES SOURCE:痛覚 , 0.80
;小柄体型
SIF IS_小柄(TARGET)
	TIMES SOURCE:痛覚 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:痛覚 , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF IS_Ａ鈍感(TARGET)
	TIMES SOURCE:痛覚 , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF IS_Ａ敏感(TARGET)
	TIMES SOURCE:痛覚 , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

;貞操観念
IF EXP:Ｖ経験 == 0 && IS_貞操観念(TARGET)
	SOURCE:屈従 /= 3
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
EXP:Ａ経験 += 5
PRINTL Ａ経験＋5

;調教時の排泄が始めてだった場合、
IF TEQUIP:浣腸セット && CFLAG:浣腸経験 == 0
	X = 1
	;ビデオ録画中なら異常経験+2、そうでなければ+1
	IF TEQUIP:撮影中
		X += 1
		CFLAG:浣腸経験 = 2
	ELSE
		CFLAG:浣腸経験 = 1
	ENDIF
	PRINTFORML 異常経験+{X}
	EXP:異常経験 += X
;初めてではないが、ビデオ録画中だった場合
ELSEIF TEQUIP:浣腸セット && CFLAG:浣腸経験 == 1 && TEQUIP:撮影中
	PRINTL 異常経験+1
	EXP:異常経験 += 1
	CFLAG:浣腸経験 = 2
ENDIF

;触手浣腸処理
SIF TEQUIP:触手召喚
	T = 0
IF TEQUIP:浣腸セット == 0 && TEQUIP:触手召喚 
	STAIN:アナル |= 2
	STAIN:アナル |= 4
ENDIF

;着衣おもらし処理
SIF TEQUIP:浣腸セット && FLAG:着衣使用
	CALL SOILING_CLOTH_NO2

;浣腸プラグの着脱
TEQUIP:浣腸セット = 1 - TEQUIP:浣腸セット

RETURN 1

;-------------------------------------------------
;-------------------------------------------------
;浣腸＋アナルプラグ挿入中
;-------------------------------------------------
@EQUIP_COM46
IF TEQUIP:触手召喚
	PRINTL ＜浣腸触手挿入中＞
ELSE
	PRINTL ＜浣腸＋アナルプラグ挿入中＞
ENDIF

LOSEBASE:体力 += 100
LOSEBASE:気力 += 80

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 80
	B = 300
ELSEIF ABL:Ａ感覚 == 1
	A = 250
	B = 800
ELSEIF ABL:Ａ感覚 == 2
	A = 600
	B = 1400
ELSEIF ABL:Ａ感覚 == 3
	A = 1000
	B = 1800
ELSEIF ABL:Ａ感覚 == 4
	A = 1300
	B = 2100
ELSE
	A = 1700
	B = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2 / 2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3 / 2
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4 / 2
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5 / 2
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES A , 0.80
ELSEIF ABL:信頼 == 1
	TIMES A , 0.90
ELSEIF ABL:信頼 == 2
	TIMES A , 1.00
ELSEIF ABL:信頼 == 3
	TIMES A , 1.10
ELSEIF ABL:信頼 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF
;大柄
SIF IS_大柄(TARGET)
	TIMES C , 0.80
;小柄体型
SIF IS_小柄(TARGET)
	TIMES C , 2.00
;未熟
SIF TALENT:未熟
	TIMES C , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF IS_Ａ鈍感(TARGET)
	TIMES SOURCE:痛覚 , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF IS_Ａ敏感(TARGET)
	TIMES SOURCE:痛覚 , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:快Ａ += A
SOURCE:屈従 += B
SOURCE:痛覚 += C
SOURCE:逸脱 += B

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && IS_貞操観念(TARGET)
	SOURCE:屈従 /= 3
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
EXP:Ａ経験 += 3
PRINTL Ａ経験＋３

IF HAVE_VAGINA(TARGET) && HAVE_VAGINA(PLAYER) 
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF IS_MALE(TARGET) && IS_MALE(PLAYER) 
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:触手召喚
	T += 1
