;-------------------------------------------------
;正常位ＳＰ
;セックス系コマンド：正常位・キスと正常位・胸愛撫と子宮責めの合わせ技
;発動までに時間がかかる上、必要能力も高めだが効果は非常に高い
;愛があると補正が大きい
;-------------------------------------------------
@COM130

SELECTCOM = 130

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0


PRINTL 正常位ＳＰ
SAVESTR:0 = 正常位ＳＰ
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0
	TFLAG:主人による処女喪失 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 70
LOSEBASE:気力 += 130

SOURCE:12 = 400

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 50
	SOURCE:5 = 10
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 150
	SOURCE:5 = 50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 200
	SOURCE:5 = 100
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 250
	SOURCE:5 = 180
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 300
	SOURCE:5 = 300
ELSE
	SOURCE:4 = 350
	SOURCE:5 = 500
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:4 , 4.00
	TIMES SOURCE:5 , 4.00
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 20
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 100
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 500
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 1200
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 2000
ELSE
	SOURCE:0 = 2800
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 10
	SOURCE:3 = 250
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 50
	SOURCE:3 = 500
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 450
	SOURCE:3 = 550
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 800
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 2800
	SOURCE:3 = 1200
ELSE
	SOURCE:1 = 4000
	SOURCE:3 = 1800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5500
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 300
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 50
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 300
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:ふたなり
		TIMES SOURCE:1 , 2.50
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:6 , 4.00

;貞操観念
IF TALENT:貞操観念
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:貞操無頓着
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:Ｖ経験 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:17 = 20
	SOURCE:3 = 50
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:17 = 100
	SOURCE:3 = 100
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:17 = 500
	SOURCE:3 = 160
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:17 = 1200
	SOURCE:3 = 200
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:17 = 2000
	SOURCE:3 = 230
ELSE
	SOURCE:17 = 2800
	SOURCE:3 = 250
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;TALENT:愛をみる
IF TALENT:恋心
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 2.00
	TIMES SOURCE:17 , 1.50
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者の口の汚れが移動
STAIN:0 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:0

;奴隷のＢ⇔調教者の指の汚れが移動
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
;
;
