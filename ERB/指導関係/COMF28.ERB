;-------------------------------------------------
;対面座位アナル
;セックス系コマンド：調教者の%CSTR:男性器呼び方%を調教対象のアナルに挿入
;-------------------------------------------------
@COM28
;アナル処女を奪うかどうかの確認
CALL CONFIRM_LOST_ANALVIRGIN
SIF RESULT == 0
	RETURN 0


PRINTL 対面座位アナル
SAVESTR:0 = 対面座位アナル
CALL TRAIN_MESSAGE_B

;A経験を伴うコマンドのフラグ
TFLAG:Ａ挿入判定 = 1
;調教者の射精チェック
CALL COM_EJAC_PLAYER_ANALSEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOCAL = 60
LOCAL:1 = 120

LOCAL:2 = ABL:Ａ感覚 * 3
SIF LOCAL:2 <= 0
	LOCAL:2 = 1
LOCAL:3 = ABL:Ａ感覚 * 2
SIF LOCAL:3 <= 0
	LOCAL:3 = 1

LOCAL -= LOCAL:2
SIF LOCAL <= 10
	LOCAL = 10
LOCAL:1 -= LOCAL:3
SIF LOCAL:1 <= 10
	LOCAL:1 = 10

LOSEBASE:体力 += LOCAL
LOSEBASE:気力 += LOCAL:1

SOURCE:3 = 100
SOURCE:7 = 100
SOURCE:12 = 100

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:2 += 10
	SOURCE:3 += 10
	SOURCE:13 += 80
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:2 += 30
	SOURCE:3 += 130
	SOURCE:13 += 300
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:2 += 200
	SOURCE:3 += 500
	SOURCE:13 += 700
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:2 += 500
	SOURCE:3 += 1000
	SOURCE:13 += 1500
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:2 += 900
	SOURCE:3 += 1500
	SOURCE:13 += 2600
ELSE
	SOURCE:2 += 1400
	SOURCE:3 += 2000
	SOURCE:13 += 4000
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:6 = 18000
	LOSEBASE:体力 += 40
	LOSEBASE:気力 += 60
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:2 , 0.30
	SOURCE:6 = 10000
	LOSEBASE:体力 += 30
	LOSEBASE:気力 += 50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 4500
	LOSEBASE:体力 += 20
	LOSEBASE:気力 += 40
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 1500
	LOSEBASE:体力 += 10
	LOSEBASE:気力 += 30
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 700
	LOSEBASE:体力 += 0
	LOSEBASE:気力 += 20
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 300
	LOSEBASE:体力 += 0
	LOSEBASE:気力 += 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 10000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 3600
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 1200
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 200
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 50
	SOURCE:5 = 10
	SOURCE:16 = 100
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 150
	SOURCE:5 = 50
	SOURCE:16 = 300
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 300
	SOURCE:5 = 100
	SOURCE:16 = 700
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 400
	SOURCE:5 = 200
	SOURCE:16 = 1200
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 500
	SOURCE:5 = 300
	SOURCE:16 = 1800
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 800
	SOURCE:5 = 500
	SOURCE:16 = 2500
	TIMES SOURCE:8 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:ふたなり
		TIMES SOURCE:1 , 2.50
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:6 , 2.00

;貞操観念
IF TALENT:貞操観念
	IF EXP:Ａ経験 == 0
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:16 , 1.10
		SOURCE:15 = 500
	ELSE
		TIMES SOURCE:3 , 1.20
		TIMES SOURCE:16 , 1.10
		SOURCE:15 = 0
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:13 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:13 , 1.20
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:13 , 1.40
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;プレイヤーのABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	SOURCE:3 += 100
	SOURCE:10 += 0
	A = 0
ELSEIF ABL:PLAYER:技巧 == 1
	SOURCE:3 += 150
	SOURCE:10 += 50
	A = 0
ELSEIF ABL:PLAYER:技巧 == 2
	SOURCE:3 += 200
	SOURCE:10 += 100
	A = 0
ELSEIF ABL:PLAYER:技巧 == 3
	SOURCE:3 += 300
	SOURCE:10 += 150
	A = 50
ELSEIF ABL:PLAYER:技巧 == 4
	SOURCE:3 += 500
	SOURCE:10 += 250
	A = 100
ELSE
	SOURCE:3 += 800
	SOURCE:10 += 400
	A = 300
ENDIF

SOURCE:0 += A

;TALENT:愛をみる
IF TALENT:恋心
	TIMES SOURCE:3 , 3.00
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:16 , 2.00
ENDIF

;アナルセックス後の処理
CALL COM_AFTER_ANAL_SEX

RETURN 1
;
