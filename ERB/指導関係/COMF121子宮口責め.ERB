;-------------------------------------------------
;挿入子宮口責め
;-------------------------------------------------
@COM121

TFLAG:３Ｐ継続中 = 0
IF SELECTCOM == [[TRAIN:騎乗位]]
	PRINTL 騎乗子宮口責め
	SAVESTR:0 = 騎乗子宮口責め
ELSE
	PRINTL 挿入子宮口責め
	SAVESTR:0 = 挿入子宮口責め
ENDIF

SELECTCOM = 121
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0
	TFLAG:主人による処女喪失 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
IF PREVCOM == [[TRAIN:騎乗位]]
	LOSEBASE:体力 += 60
	LOSEBASE:気力 += 150
	SOURCE:露出 = 900
ELSE
	LOSEBASE:体力 += 50
	LOSEBASE:気力 += 100
	SOURCE:露出 = 400
ENDIF

SOURCE:快Ｃ = 0

;前方向挿入の場合（正常位・対面座位・騎乗位）
IF PREVCOM == [[TRAIN:正常位]] || PREVCOM == [[TRAIN:対面座位]] || PREVCOM == [[TRAIN:騎乗位]]
	;ABL:Ｃ感覚をみる
	IF ABL:Ｃ感覚 == 0
		SOURCE:快Ｃ = 20
	ELSEIF ABL:Ｃ感覚 == 1
		SOURCE:快Ｃ = 100
	ELSEIF ABL:Ｃ感覚 == 2
		SOURCE:快Ｃ = 400
	ELSEIF ABL:Ｃ感覚 == 3
		SOURCE:快Ｃ = 900
	ELSEIF ABL:Ｃ感覚 == 4
		SOURCE:快Ｃ = 1400
	ELSE
		SOURCE:快Ｃ = 2100
	ENDIF
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 10
	SOURCE:情愛 = 100
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 30
	SOURCE:情愛 = 180
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 400
	SOURCE:情愛 = 250
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 800
	SOURCE:情愛 = 380
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 2700
	SOURCE:情愛 = 500
ELSE
	SOURCE:快Ｖ = 3800
	SOURCE:情愛 = 800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛覚 = 300
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.60
	SOURCE:痛覚 = 100
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
	SOURCE:痛覚 = 10
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
	SOURCE:痛覚 = 0
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.30
	SOURCE:痛覚 = 0
ELSE
	TIMES SOURCE:快Ｖ , 1.80
	SOURCE:痛覚 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｃ , 0.50
	TIMES SOURCE:痛覚 , 0.10
	SOURCE:痛覚 += 1000
	TIMES SOURCE:痛覚 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｃ , 0.60
	TIMES SOURCE:痛覚, 0.40
	SOURCE:痛覚 += 300
	TIMES SOURCE:痛覚, 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:痛覚 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.40
	TIMES SOURCE:痛覚 , 0.20
ELSE
	TIMES SOURCE:快Ｖ , 1.80
	TIMES SOURCE:痛覚 , 0.10
ENDIF

;対面座位だと効果が高い
IF PREVCOM == [[TRAIN:対面座位]]
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:情愛 , 1.10
;背面座位だと効果が低い
ELSEIF PREVCOM == [[TRAIN:背面座位]]
	TIMES SOURCE:快Ｖ , 0.70
	TIMES SOURCE:情愛 , 0.90
ENDIF

;調教者が[オトコ]
SIF TALENT:PLAYER:オトコ
	TIMES SOURCE:快Ｖ , 2.50

;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:痛覚 , 2.00

;貞操観念
IF TALENT:貞操観念
	IF TALENT:処女 == 1
		TIMES SOURCE:情愛 , 0.60
		TIMES SOURCE:逸脱, 5.00
		SOURCE:反感 = 2000
	ELSE
		TIMES SOURCE:情愛 , 0.60
		SOURCE:反感 = 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:貞操無頓着
	IF TALENT:処女 == 1
		TIMES SOURCE:情愛 , 0.60
		SOURCE:反感 = 300
	ENDIF
ELSE
	SIF TALENT:処女 == 1
		SOURCE:反感 = 3000
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｃ , 0.60
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:情愛 , 0.30
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｃ , 0.80
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:情愛 , 1.50
ELSE
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:快Ｖ , 1.50
	TIMES SOURCE:情愛 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:快Ｃ , 0.50
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:情愛 , 0.60
	TIMES SOURCE:反感 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:快Ｃ , 0.80
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.80
	TIMES SOURCE:反感 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:反感 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:快Ｃ , 1.20
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:情愛 , 1.20
	TIMES SOURCE:反感 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:快Ｃ , 1.40
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:情愛 , 1.40
	TIMES SOURCE:反感 , 0.60
ELSE
	TIMES SOURCE:快Ｃ , 1.60
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:情愛 , 1.60
	TIMES SOURCE:反感 , 0.30
ENDIF

IF PREVCOM == [[TRAIN:騎乗位]]
	;ABL:奉仕精神をみる
	IF ABL:奉仕技術 == 0
		SOURCE:性行動 = 200
		SOURCE:達成 = 50
		SOURCE:逸脱 = 300
	ELSEIF ABL:奉仕技術 == 1
		SOURCE:性行動 = 250
		SOURCE:達成 = 200
		SOURCE:逸脱 = 100
	ELSEIF ABL:奉仕技術 == 2
		SOURCE:性行動 = 350
		SOURCE:達成 = 550
		SOURCE:逸脱 = 30
	ELSEIF ABL:奉仕技術 == 3
		SOURCE:性行動 = 450
		SOURCE:達成 = 900
		SOURCE:逸脱 = 0
	ELSEIF ABL:奉仕技術 == 4
		SOURCE:性行動 = 600
		SOURCE:達成 = 1500
		SOURCE:逸脱 = 0
	ELSE
		SOURCE:性行動 = 750
		SOURCE:達成 = 2200
		SOURCE:逸脱 = 0
	ENDIF
	;ABL:技巧をみる
	IF ABL:技巧 == 0
		TIMES SOURCE:快Ｖ , 0.30
		TIMES SOURCE:情愛 , 0.30
	ELSEIF ABL:技巧 == 1
		TIMES SOURCE:快Ｖ , 0.60
		TIMES SOURCE:情愛 , 0.60
	ELSEIF ABL:技巧 == 2
		TIMES SOURCE:快Ｖ , 1.00
		TIMES SOURCE:情愛 , 1.00
	ELSEIF ABL:技巧 == 3
		TIMES SOURCE:快Ｖ , 1.10
		TIMES SOURCE:情愛 , 1.10
	ELSEIF ABL:技巧 == 4
		TIMES SOURCE:快Ｖ , 1.30
		TIMES SOURCE:情愛 , 1.20
	ELSE
		TIMES SOURCE:快Ｖ , 1.50
		TIMES SOURCE:情愛 , 1.40
	ENDIF
ENDIF

;乱れ牡丹からの派生
IF PREVCOM == 612
	;ABL:Ｂ感覚をみる
	IF ABL:Ｂ感覚 == 0
		SOURCE:快Ｂ = 50
		SOURCE:情愛 += 50
		SOURCE:性行動 += 50
	ELSEIF ABL:Ｂ感覚 == 1
		SOURCE:快Ｂ = 200
		SOURCE:情愛 += 200
		SOURCE:性行動 += 200
	ELSEIF ABL:Ｂ感覚 == 2
		SOURCE:快Ｂ = 500
		SOURCE:情愛 += 500
		SOURCE:性行動 += 400
	ELSEIF ABL:Ｂ感覚 == 3
		SOURCE:快Ｂ = 1000
		SOURCE:情愛 += 600
		SOURCE:性行動 += 600
	ELSEIF ABL:Ｂ感覚 == 4
		SOURCE:快Ｂ = 1600
		SOURCE:情愛 += 1000
		SOURCE:性行動 += 1000
	ELSE
		SOURCE:快Ｂ = 2100
		SOURCE:情愛 += 1400
		SOURCE:性行動 += 1400
	ENDIF

	;ABL:Ｃ感覚をみる
	IF ABL:Ｃ感覚 == 0
		SOURCE:快Ｃ += 40
		SOURCE:性行動 += 50
	ELSEIF ABL:Ｃ感覚 == 1
		SOURCE:快Ｃ += 160
		SOURCE:性行動 += 200
	ELSEIF ABL:Ｃ感覚 == 2
		SOURCE:快Ｃ += 700
		SOURCE:性行動 += 400
	ELSEIF ABL:Ｃ感覚 == 3
		SOURCE:快Ｃ += 1500
		SOURCE:性行動 += 600
	ELSEIF ABL:Ｃ感覚 == 4
		SOURCE:快Ｃ += 2400
		SOURCE:性行動 += 1000
	ELSE
		SOURCE:快Ｃ += 3600
		SOURCE:性行動 += 1400
	ENDIF

	;プレイヤーのABL:技巧をみる
	IF ABL:PLAYER:技巧 == 0
		SOURCE:快Ｃ += 100
		SOURCE:快Ｖ += 100
		SOURCE:快Ｂ += 50
		SOURCE:情愛 += 100
		SOURCE:性行動 += 50
		SOURCE:愛液分泌 += 0
		SOURCE:欲情 += 0
		A = 0
	ELSEIF ABL:PLAYER:技巧 == 1
		SOURCE:快Ｃ += 200
		SOURCE:快Ｖ += 200
		SOURCE:快Ｂ += 150
		SOURCE:情愛 += 150
		SOURCE:性行動 += 100
		SOURCE:愛液分泌 += 50
		SOURCE:欲情 += 100
		A = 0
	ELSEIF ABL:PLAYER:技巧 == 2
		SOURCE:快Ｃ += 300
		SOURCE:快Ｖ += 300
		SOURCE:快Ｂ += 250
		SOURCE:情愛 += 200
		SOURCE:性行動 += 200
		SOURCE:愛液分泌 += 100
		SOURCE:欲情 += 200
		A = 50
	ELSEIF ABL:PLAYER:技巧 == 3
		SOURCE:快Ｃ += 500
		SOURCE:快Ｖ += 400
		SOURCE:快Ｂ += 500
		SOURCE:情愛 += 300
		SOURCE:性行動 += 300
		SOURCE:愛液分泌 += 150
		SOURCE:欲情 += 300
		A = 100
	ELSEIF ABL:PLAYER:技巧 == 4
		SOURCE:快Ｃ += 800
		SOURCE:快Ｖ += 500
		SOURCE:快Ｂ += 800
		SOURCE:情愛 += 500
		SOURCE:性行動 += 400
		SOURCE:愛液分泌 += 250
		SOURCE:欲情 += 400
		A = 300
	ELSE
		SOURCE:快Ｃ += 1200
		SOURCE:快Ｖ += 600
		SOURCE:快Ｂ += 1200
		SOURCE:情愛 += 800
		SOURCE:性行動 += 600
		SOURCE:愛液分泌 += 400
		SOURCE:欲情 += 500
		A = 500
	ENDIF

	;ABL:露出癖をみる
	IF ABL:露出癖 == 0
		SOURCE:中毒充足 += 100
		SOURCE:愛液分泌 += 100
		TIMES A, 0.60
	ELSEIF ABL:露出癖 == 1
		SOURCE:中毒充足 += 200
		SOURCE:愛液分泌 += 200
		TIMES A, 1.00
	ELSEIF ABL:露出癖 == 2
		SOURCE:中毒充足 += 400
		SOURCE:愛液分泌 += 400
		TIMES A, 1.50
	ELSEIF ABL:露出癖 == 3
		SOURCE:中毒充足 += 700
		SOURCE:愛液分泌 += 700
		TIMES A, 2.40
	ELSEIF ABL:露出癖 == 4
		SOURCE:中毒充足 += 1200
		SOURCE:愛液分泌 += 1200
		TIMES A, 3.60
	ELSE
		SOURCE:中毒充足 += 2000
		SOURCE:愛液分泌 += 2000
		TIMES A, 5.80
	ENDIF

	SOURCE:快Ｃ += A
	SOURCE:快Ｖ += A
	SOURCE:情愛 += A

	SIF EXP:Ｖ経験 >= EXPLV:3
		SOURCE:快Ｖ += A

	;TALENT:恋心をみる
	SIF TALENT:恋心
		TIMES SOURCE:情愛 , 2.00
ENDIF

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
