@COM100
;触手召喚

PRINTL 触手召喚
SAVESTR:0 = 触手召喚
CALL TRAIN_MESSAGE_B

IF TEQUIP:触手召喚
	TEQUIP:触手召喚 = 0

	;触手挿入(バイブ)
	TEQUIP:バイブ = 0
	;アナル触手(アナルバイブ)
	TEQUIP:アナルバイブ = 0
	;触手クリ責め(クリキャップ)
	TEQUIP:クリキャップ = 0
	;触手乳首責め(ニプルキャップ)
	TEQUIP:ニプルキャップ = 0
	;触手搾乳(搾乳器)
	TEQUIP:搾乳機 = 0
	;触手ペニス責め(オナホ)
	TEQUIP:オナホール = 0
	;触手緊縛(縄)
	TEQUIP:縄 = 0
	;触手浣腸(浣腸器)
	TEQUIP:[[TEQUIP:浣腸器＋プラグ]] = 0
	;触手口辱
	TEQUIP:触手口辱 = 0

ELSE
	TEQUIP:触手召喚 = 1

	A = 100
	IF EXP:触手経験 < EXPLV:1
		TIMES A , 3.00
	ELSEIF EXP:触手経験 < EXPLV:2 / 2
		TIMES A , 2.50
	ELSEIF EXP:触手経験 < EXPLV:3 / 2
		TIMES A , 2.00
	ELSEIF EXP:触手経験 < EXPLV:4 / 2
		TIMES A , 1.00
	ELSEIF EXP:触手経験 < EXPLV:5 / 2
		TIMES A , 0.80
	ELSE
		TIMES A , 0.60
	ENDIF

	;臆病
	SIF TALENT:臆病
		TIMES A , 2.00
	;感情乏しい
	SIF TALENT:感情乏しい
		TIMES A , 0.60

	LOSEBASE:体力 += A
	LOSEBASE:気力 += A * 2

	UP:[[UP:恐怖]] += A * 20
	SOURCE:逸脱 += A * 5
ENDIF
T = 0
RETURN 1


@EQUIP_COM100
;触手調教中
PRINTL ＜触手調教中＞

A = 100
IF EXP:触手経験 < EXPLV:1
	TIMES A , 3.00
ELSEIF EXP:触手経験 < EXPLV:2
	TIMES A , 2.50
ELSEIF EXP:触手経験 < EXPLV:3
	TIMES A , 2.00
ELSEIF EXP:触手経験 < EXPLV:4
	TIMES A , 1.00
ELSEIF EXP:触手経験 < EXPLV:5
	TIMES A , 0.80
ELSE
	TIMES A , 0.60
ENDIF

;臆病
SIF TALENT:臆病
	TIMES A , 2.00

;感情乏しい
SIF TALENT:感情乏しい
	TIMES A , 0.60

;淫乱
SIF TALENT:淫乱
	TIMES A , 0.50

;苗床なら効果増大
IF TALENT:苗床
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成 , 1.20
	TIMES SOURCE:中毒充足 , 2.00
	TIMES SOURCE:愛液分泌 , 3.00
ENDIF

IF ABL:触手中毒 <= 1
	TIMES A , 1.00
ELSEIF ABL:触手中毒 <= 2
	TIMES A , 0.90
ELSEIF ABL:触手中毒 <= 3
	TIMES A , 0.80
ELSEIF ABL:触手中毒 <= 4
	TIMES A , 0.70
ELSEIF ABL:触手中毒 <= 5
	TIMES A , 0.60
ELSE
	TIMES A , 0.50
ENDIF

LOSEBASE:体力 += A
LOSEBASE:気力 += A * 2

UP:[[UP:恐怖]] += A * 20
SOURCE:不潔 += A * 10
SOURCE:逸脱 += A * 5

;触手のぬめりと判断
SOURCE:液体追加 += 2000

TIMES SOURCE:快Ｃ , 2.00
TIMES SOURCE:快Ｖ , 2.00
TIMES SOURCE:快Ａ , 2.00
TIMES SOURCE:快Ｂ , 2.00
TIMES SOURCE:屈従 , 1.80

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
IF MAXBASE:PLAYER:異種射精ゲージ != 0

	B = 0
	;ABL:技巧をみる
	IF ABL:技巧 == 0
		B = 500
	ELSEIF ABL:技巧 == 1
		B = 600
	ELSEIF ABL:技巧 == 2
		B = 800
	ELSEIF ABL:技巧 == 3
		B = 1000
	ELSEIF ABL:技巧 == 4
		B = 1400
	ELSE
		B = 2000
	ENDIF

	;ABL:従順をみる
	IF ABL:信頼 == 0
		TIMES B , 0.80
	ELSEIF ABL:信頼 == 1
		TIMES B , 0.90
	ELSEIF ABL:信頼 == 2
		TIMES B , 1.00
	ELSEIF ABL:信頼 == 3
		TIMES B , 1.10
	ELSEIF ABL:信頼 == 4
		TIMES B , 1.20
	ELSE
		TIMES B , 1.30
	ENDIF

	;PALAM:欲情をみる
	IF PALAM:欲情 < PALAMLV:1
		TIMES B , 1.00
	ELSEIF PALAM:欲情 < PALAMLV:2
		TIMES B , 1.10
	ELSEIF PALAM:欲情 < PALAMLV:3
		TIMES B , 1.20
	ELSEIF PALAM:欲情 < PALAMLV:4
		TIMES B , 1.30
	ELSEIF PALAM:欲情 < PALAMLV:5
		TIMES B , 1.40
	ELSE
		TIMES B , 1.50
	ENDIF
	;【苗床】だとイかせやすくなる
	SIF TALENT:苗床
		TIMES B , 2.50
	
	
	;触手挿入
	SIF TEQUIP:バイブ
		TIMES B , 1.50
	;アナル触手
	SIF TEQUIP:アナルバイブ
		TIMES B , 1.50
	;触手クリ責め
	SIF TEQUIP:クリキャップ
		TIMES B , 1.20
	;触手乳首責め
	SIF TEQUIP:ニプルキャップ
		TIMES B , 1.20
	;触手搾乳
	SIF TEQUIP:搾乳機
		TIMES B , 1.30
	;触手ペニス責め
	SIF TEQUIP:オナホール
		TIMES B , 1.10
	;触手緊縛
	SIF TEQUIP:縄
		TIMES B , 1.20
	;触手浣腸
	SIF TEQUIP:[[TEQUIP:浣腸器＋プラグ]]
		TIMES B , 1.30
	;触手口辱
	SIF TEQUIP:触手口辱
		TIMES B , 1.50

	BASE:PLAYER:異種射精ゲージ += B

	S = BASE:PLAYER:異種射精ゲージ
	EJAC = MAXBASE:PLAYER:異種射精ゲージ

	IF  S > EJAC * 2
		E = 2
	ELSEIF S > EJAC
		E = 1
	ELSE
		E = 0
	ENDIF


	;大量射精
	IF E == 2
		EXP:精液経験 += 3
						;****追加ここから****
		SIF TEQUIP:バイブ
			CFLAG:触奴膣内精子量 += 3
						;****追加ここまで****
		PRINTL 触手大量射精
		PRINTL 精液経験＋３
		T += 1

		BASE:PLAYER:異種射精ゲージ -= EJAC*2
		SIF BASE:PLAYER:異種射精ゲージ >= EJAC
			BASE:PLAYER:異種射精ゲージ = EJAC-1

	;通常の射精
	ELSEIF E == 1
		EXP:精液経験 += 1
						;****追加ここから****
		SIF TEQUIP:バイブ
			CFLAG:触奴膣内精子量 += 1
						;****追加ここまで****
		PRINTL 触手射精
		PRINTL 精液経験＋１
		T += 1

		BASE:PLAYER:異種射精ゲージ -= EJAC
		SIF BASE:PLAYER:異種射精ゲージ >= EJAC
			BASE:PLAYER:異種射精ゲージ = EJAC-1
	ENDIF


ENDIF

IF EXP:触手経験 == 0
	EXP:異常経験 += 1
	PRINTL 異常経験＋1
ENDIF

T += 1
PRINT 触手経験＋
PRINTVL T
EXP:触手経験 += T
T = 0
RETURN 1


