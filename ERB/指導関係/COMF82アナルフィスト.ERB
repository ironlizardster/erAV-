@COM82
;アナルフィスト
PRINTL アナルフィスト
SAVESTR:0 = アナルフィスト
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 600
LOSEBASE:気力 += 300

SOURCE:6 = 1200
SOURCE:12 = 1200
SOURCE:13 = 1200
SOURCE:14 = 1200
SOURCE:16 = 1200

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:2 = 10
	SOURCE:3 = 10
	SOURCE:13 = 100
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:2 = 30
	SOURCE:3 = 30
	SOURCE:13 = 700
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:2 = 500
	SOURCE:3 = 100
	SOURCE:13 = 1500
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:2 = 1000
	SOURCE:3 = 200
	SOURCE:13 = 3000
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:2 = 1700
	SOURCE:3 = 450
	SOURCE:13 = 5000
ELSE
	SOURCE:2 = 2200
	SOURCE:3 = 750
	SOURCE:13 = 8000
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:2 , 0.10
	SOURCE:6 = 20000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:2 , 0.30
	SOURCE:6 = 12000
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:2 , 0.50
	SOURCE:6 = 5000
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:2 , 1.00
	SOURCE:6 = 1800
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:2 , 1.40
	SOURCE:6 = 1000
ELSE
	TIMES SOURCE:2 , 1.60
	SOURCE:6 = 600
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:2 , 0.40
	SOURCE:6 += 10000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	SOURCE:6 += 3600
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	SOURCE:6 += 1200
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:2 , 1.40
	SOURCE:6 += 200
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:2 , 1.80
	SOURCE:6 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:2 , 0.60
	TIMES SOURCE:13 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:2 , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:2 , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:2 , 1.20
	TIMES SOURCE:13 , 1.20
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:2 , 1.40
	TIMES SOURCE:13 , 1.40
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:6 , 4.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	SOURCE:13 /= 3
ENDIF

TFLAG:Ａ挿入判定 = 1

EXP:Ａ経験 += 25
PRINTL Ａ経験＋２５

IF EXP:Ａ拡張経験 == 0 
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

EXP:Ａ拡張経験 += 1
PRINTL Ａ拡張経験＋1

RETURN 1
