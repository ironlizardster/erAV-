@COM57
;羞恥プレイ

PRINTL 羞恥プレイ
SAVESTR:0 = 羞恥プレイ
CALL TRAIN_MESSAGE_B

;鏡のセット
;終了時は修正なし
IF TEQUIP:羞恥大鏡プレイ
	TEQUIP:羞恥大鏡プレイ = 0
	RETURN 1
ENDIF

LOSEBASE:体力 += 0
LOSEBASE:気力 += 200


A = 500
B = 500

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 1.10
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.20
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.30
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.40
ENDIF

;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:中毒充足 += 0
	SOURCE:愛液分泌 += 0
	SOURCE:屈従 += 600
	SOURCE:逸脱 += 1000
	SOURCE:反感 += 1000
	TIMES A, 0.60
ELSEIF ABL:露出癖 == 1
	SOURCE:中毒充足 += 150
	SOURCE:愛液分泌 += 150
	SOURCE:屈従 += 500
	TIMES A, 1.00
ELSEIF ABL:露出癖 == 2
	SOURCE:中毒充足 += 300
	SOURCE:愛液分泌 += 300
	SOURCE:屈従 += 400
	TIMES A, 1.60
ELSEIF ABL:露出癖 == 3
	SOURCE:中毒充足 += 600
	SOURCE:愛液分泌 += 600
	SOURCE:屈従 += 300
	TIMES A, 2.00
ELSEIF ABL:露出癖 == 4
	SOURCE:中毒充足 += 1000
	SOURCE:愛液分泌 += 1000
	SOURCE:屈従 += 200
	TIMES A, 2.60
ELSE
	SOURCE:中毒充足 += 1800
	SOURCE:愛液分泌 += 1800
	SOURCE:屈従 += 100
	TIMES A, 3.80
ENDIF
;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES A , 1.00
ELSEIF ABL:マゾっ気 == 1
	TIMES A , 1.20
ELSEIF ABL:マゾっ気 == 2
	TIMES A , 1.40
ELSEIF ABL:マゾっ気 == 3
	TIMES A , 1.60
ELSEIF ABL:マゾっ気 == 4
	TIMES A , 1.80
ELSE
	TIMES A , 2.00
ENDIF

;無関心
SIF TALENT:クール
	TIMES A , 0.80
;感情乏しい
SIF TALENT:感情乏しい
	TIMES A , 0.80
;好奇心
SIF TALENT:好奇心
	TIMES A , 2.50
;解放
SIF IS_解放(TARGET)
	TIMES A , 1.50

;臆病
SIF TALENT:臆病
	TIMES B , 1.50
;恥じらい
SIF IS_恥じらい(TARGET)
	TIMES B , 3.00

;倒錯的
SIF TALENT:倒錯的
	TIMES A , 1.50
	TIMES B , 1.20
;魅力
SIF TALENT:魅力
	SOURCE:情愛 += 500
	SOURCE:恭順 += 500
	TIMES A , 1.50
	TIMES B , 1.20

SOURCE:露出 += A
SOURCE:逸脱 += B
SOURCE:恭順 += A/2

TEQUIP:羞恥大鏡プレイ = 1

;愛情経験
E = 1
IF CFLAG:好感度 >= 1000 && ABL:露出癖 >= 3 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:[[EXP:愛情経験]]%+{E}
	EXP:愛情経験 += E
ENDIF
E = 0

RETURN 1


