;-------------------------------------------------
;調教中のユーザーコマンド判定
;-------------------------------------------------
@USERCOM
IF RESULT == 120
	CALL SHOW_EQUIP
	RETURN 1
ELSEIF RESULT == 121
	CALL STAIN_INFO
	RETURN 1
ELSEIF RESULT == 122
	CALL SHOW_INFO
	WAIT
	RETURN 1
						;****スライムパッチ追加ここから****
ELSEIF RESULT == 200 && ASSI > 0 && TEQUIP:触手召喚 == 0 && TEQUIP:スライム生成 == 0
						;****スライムパッチ追加ここまで****
	IF ASSIPLAY
		ASSIPLAY = 0
		PLAYER = MASTER
	ELSE
		ASSIPLAY = 1
		PLAYER = ASSI
	ENDIF
	RETURN 1
ELSEIF RESULT == 202
	T = TARGET
	TARGET = PLAYER
	CALL SHOW_INFO
	WAIT
	TARGET = T
	RETURN 1
ELSEIF RESULT == 300 && TALENT:PLAYER:具現 && TALENT:PLAYER:ふたなり == 0
	PRINTFORM 《%CALLNAME:PLAYER%はペニスを具現化させた》
	SIF CFLAG:PLAYER:初キス相手 == 0
		TALENT:PLAYER:童貞 = 1
	TALENT:PLAYER:ふたなり = 1
	WAIT
	RETURN 1
ELSEIF RESULT == 300 && TALENT:PLAYER:具現 && TALENT:PLAYER:ふたなり == 1
	PRINTFORM 《%CALLNAME:PLAYER%は具現化したペニスを消去した》
	SIF CFLAG:PLAYER:初キス相手 == 0
		TALENT:PLAYER:童貞 = 0
	TALENT:PLAYER:ふたなり = 0
	WAIT
	RETURN 1
ELSEIF RESULT == 400
	CALL SHOOT_POSITION
ELSEIF RESULT == 500 && ITEM:コンドーム >= 1 && TEQUIP:主人コンドーム == 0 && (TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ)
	CALL SET_CONDOM
ELSEIF RESULT == 990
	CALL COMSEQ_REGISTER_BEGIN
	RETURN 1
ELSEIF RESULT == 991
	CALL SHOW_COMSEQ_REGISTER
	RETURN 1
ELSEIF RESULT == 992
	CALL COMSEQ_REGISTER_TRAIN
	RETURN 1

ELSEIF RESULT == 999

;終了時に失神していた場合は口上を表示しない
	IF TFLAG:失神中終了 >= 1 && FLAG:口上使用 > 0
		IF FLAG:口上使用 == 1
			FLAG:口上使用 = 0
			TFLAG:失神回復 = 1
		ELSEIF FLAG:口上使用 == 2
			FLAG:口上使用 = 0
			TFLAG:失神回復 = 2
		ENDIF
	ENDIF
	BEGIN AFTERTRAIN
	RETURN 1
ENDIF
RETURN 0

