@COM73
;乳首あわせ
;エラー出て発見。消してもよかったけどうん
;全力で未完成。

PRINTL 乳首あわせ
SAVESTR:0 = 乳首あわせ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:気力 += 30
LOSEBASE:気力 += 90

;△中毒充足の中毒度分岐
SOURCE:7 = 500
SOURCE:12 = 250
SOURCE:13 = 400
SOURCE:14 = 300

;ABL:従順をみる
IF ABL:信頼 == 0
	SOURCE:11 = 200
ELSEIF ABL:信頼 == 1
	SOURCE:11 = 120
ELSEIF ABL:信頼 == 2
	SOURCE:11 = 60
ELSEIF ABL:信頼 == 3
	SOURCE:11 = 20
ELSEIF ABL:信頼 == 4
	SOURCE:11 = 0
ELSE
	SOURCE:11 = 0
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:17 = 20
	SOURCE:4 = 0
	SOURCE:5 = 0
	SOURCE:13 = 20
	TIMES SOURCE:12 , 0.80
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:17 = 80
	SOURCE:4 = 10
	SOURCE:5 = 50
	SOURCE:13 = 20
	TIMES SOURCE:12 , 0.90
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:17 = 350
	SOURCE:4 = 50
	SOURCE:5 = 100
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.00
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:17 = 750
	SOURCE:4 = 100
	SOURCE:5 = 300
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.10
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:17 = 1200
	SOURCE:4 = 700
	SOURCE:5 = 600
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.20
ELSE
	SOURCE:17 = 1750
	SOURCE:4 = 2000
	SOURCE:5 = 1000
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.30
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 0.60
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 0.60
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 0.60
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 0.60
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 0.60
	TIMES SOURCE:13 , 0.50
ELSE
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 0.60
	TIMES SOURCE:13 , 0.50
ENDIF

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:奉仕精神 == 1
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:奉仕精神 == 2
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:奉仕精神 == 3
	TIMES SOURCE:5 , 1.40
ELSEIF ABL:奉仕精神 == 4
	TIMES SOURCE:5 , 1.70
ELSE
	TIMES SOURCE:5 , 2.00
ENDIF


;調教者のABL:Ｂ感覚をみる
IF ABL:PLAYER:Ｂ感覚 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:PLAYER:Ｂ感覚 == 1
	TIMES SOURCE:4 , 0.90
	TIMES SOURCE:5 , 0.70
ELSEIF ABL:PLAYER:Ｂ感覚 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:PLAYER:Ｂ感覚 == 3
	TIMES SOURCE:4 , 1.10
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:PLAYER:Ｂ感覚 == 4
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.40
ELSE
	TIMES SOURCE:4 , 1.30
	TIMES SOURCE:5 , 1.70
ENDIF


;奴隷のＢ⇔調教者のＢの汚れが移動
STAIN:5 |= STAIN:PLAYER:5
STAIN:PLAYER:5 |= STAIN:5

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +8
	EXP:レズ経験 += 8
ENDIF

SIF ASSIPLAY == 0 && ABL:Ｃ感覚 >= 3
		TFLAG:主人経験 += 1

TFLAG:快楽経験 = 1

RETURN 1

