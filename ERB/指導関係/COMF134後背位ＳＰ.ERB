;-------------------------------------------------
;後背位ＳＰ
;セックス系コマンド：立ちバック＋子宮責め
;-------------------------------------------------
@COM134

SELECTCOM = 134

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0



PRINTL 後背位ＳＰ
SAVESTR:0 = 後背位ＳＰ
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0 && TEQUIP:獣姦プレイ == 0
	TFLAG:主人による処女喪失 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 60
LOSEBASE:気力 += 120

SOURCE:露出 = 800

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 50
	SOURCE:達成 = 10
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 150
	SOURCE:達成 = 50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 200
	SOURCE:達成 = 100
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 250
	SOURCE:達成 = 180
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 300
	SOURCE:達成 = 300
ELSE
	SOURCE:性行動 = 350
	SOURCE:達成 = 500
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.50
	TIMES SOURCE:達成 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.50
	TIMES SOURCE:達成 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 2.50
	TIMES SOURCE:達成 , 2.50
ELSE
	TIMES SOURCE:性行動 , 4.00
	TIMES SOURCE:達成 , 4.00
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 10
	SOURCE:情愛 = 250
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 50
	SOURCE:情愛 = 500
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 450
	SOURCE:情愛 = 550
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 1000
	SOURCE:情愛 = 800
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 2800
	SOURCE:情愛 = 1200
ELSE
	SOURCE:快Ｖ = 4000
	SOURCE:情愛 = 1800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛覚 = 5000

	IF ASSIPLAY && HAVE_VAGINA(PLAYER) 
		EXP:異常経験 += 1
		PRINTFORML %EXPNAME:[[EXP:異常経験]]%＋１
	ENDIF
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.60
	SOURCE:痛覚 = 220
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
	SOURCE:痛覚 = 30
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
	SOURCE:痛覚 = 5
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.30
	SOURCE:痛覚 = 0
ELSE
	TIMES SOURCE:快Ｖ , 1.80
	SOURCE:痛覚 = 0
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:快Ｂ = 20
	SOURCE:情愛 += 50
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:快Ｂ = 100
	SOURCE:情愛 += 100
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:快Ｂ = 600
	SOURCE:情愛 += 200
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:快Ｂ = 1400
	SOURCE:情愛 += 300
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:快Ｂ = 2200
	SOURCE:情愛 += 600
ELSE
	SOURCE:快Ｂ = 3200
	SOURCE:情愛 += 1000
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.10
	SOURCE:痛覚 += 900
	TIMES SOURCE:痛覚 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.40
	SOURCE:痛覚 += 250
	TIMES SOURCE:痛覚 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:痛覚 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.40
	TIMES SOURCE:痛覚 , 0.20
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.80
	TIMES SOURCE:痛覚 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF IS_FUTA(ASSI)
		TIMES SOURCE:快Ｖ , 2.50
ENDIF

;大柄
SIF IS_大柄(TARGET)
	TIMES SOURCE:痛覚 , 1.80
;小柄体形
SIF IS_小柄(TARGET)
	TIMES SOURCE:痛覚 , 2.00

;貞操観念
IF IS_貞操観念(TARGET)
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:情愛 , 0.60
		SOURCE:反感 = 10000
	ELSE
		TIMES SOURCE:情愛 , 0.60
		SOURCE:反感 = 1000
	ENDIF
;貞操無頓着
ELSEIF IS_貞操無頓着(TARGET)
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:情愛 , 0.60
		SOURCE:反感 = 300
	ENDIF
ELSE
	IF EXP:Ｖ経験 == 0
		SOURCE:反感 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:情愛 , 0.30
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:情愛 , 1.50
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.50
	TIMES SOURCE:情愛 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:情愛 , 0.60
	TIMES SOURCE:反感 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.80
	TIMES SOURCE:反感 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:反感 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:情愛 , 1.20
	TIMES SOURCE:反感 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:情愛 , 1.40
	TIMES SOURCE:反感 , 0.60
ELSE
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:情愛 , 1.60
	TIMES SOURCE:反感 , 0.30
ENDIF

;愛
IF TALENT:恋心
	TIMES SOURCE:快Ｖ , 1.50
	TIMES SOURCE:情愛 , 2.00
	TIMES SOURCE:快Ｂ , 1.50
ENDIF

;後背位スパンキングを挟んでいた場合、マゾっ気補正がさらにかかる
IF TFLAG:前々回のコマンド == [[TRAIN:後背位スパンキング]] || PREVCOM == [[TRAIN:後背位スパンキング]]
	;ABL:マゾっ気をみる
	IF ABL:マゾっ気 == 0
		TIMES SOURCE:快Ｖ , 0.60
		TIMES SOURCE:愛液分泌 , 0.80
		TIMES SOURCE:屈従 , 0.80
	ELSEIF ABL:マゾっ気 == 1
		TIMES SOURCE:快Ｖ , 0.80
		TIMES SOURCE:愛液分泌 , 1.00
		TIMES SOURCE:屈従 , 1.00
	ELSEIF ABL:マゾっ気 == 2
		TIMES SOURCE:快Ｖ , 1.00
		TIMES SOURCE:愛液分泌 , 1.20
		TIMES SOURCE:屈従 , 1.50
	ELSEIF ABL:マゾっ気 == 3
		TIMES SOURCE:快Ｖ , 1.10
		TIMES SOURCE:愛液分泌 , 1.40
		TIMES SOURCE:屈従 , 3.00
	ELSEIF ABL:マゾっ気 == 4
		TIMES SOURCE:快Ｖ , 1.20
		TIMES SOURCE:愛液分泌 , 2.00
		TIMES SOURCE:屈従 , 5.00
	ELSE
		TIMES SOURCE:快Ｖ , 1.30
		TIMES SOURCE:愛液分泌 , 3.00
		TIMES SOURCE:屈従 , 8.00
	ENDIF
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者の口の汚れが移動
STAIN:口 |= STAIN:PLAYER:口
STAIN:PLAYER:口 |= STAIN:口

;奴隷のＢ⇔調教者の指の汚れが移動
STAIN:胸 |= STAIN:PLAYER:手
STAIN:PLAYER:手 |= STAIN:胸

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
