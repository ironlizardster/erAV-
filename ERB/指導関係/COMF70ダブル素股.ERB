;-------------------------------------------------
;ダブル素股
;助手系コマンド:奴隷と助手に同時に調教者の%CSTR:男性器呼び方%を素股させる
;-------------------------------------------------
@COM70

TFLAG:近親相姦 = 0
PLAYER = ASSI
CALL INCEST
PLAYER = MASTER
IF (TFLAG:近親相姦 == 1 || TFLAG:近親相姦 == 2) && TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINT 母娘
ELSEIF TFLAG:近親相姦 == 1 || TFLAG:近親相姦 == 2
	PRINT 親子
ELSEIF  (TFLAG:近親相姦 == 3 || TFLAG:近親相姦 == 4) && TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINT 姉妹
ELSEIF  (TFLAG:近親相姦 == 3 || TFLAG:近親相姦 == 4) && TALENT:オトコ && TALENT:ASSI:オトコ
	PRINT 兄弟
ELSEIF  TFLAG:近親相姦 == 3 || TFLAG:近親相姦 == 4
	PRINT 兄妹
ELSEIF  TFLAG:近親相姦 == 5 || TFLAG:近親相姦 == 6
	PRINT 従姉弟
ENDIF
PRINTL ダブル素股

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 2
	PRINTS ABLNAME:[[ABL:欲望]]
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
	S = 1
ENDIF

;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINTS ABLNAME:[[ABL:奉仕精神]]
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF

;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 1
	PRINTS ABLNAME:[[ABL:精液中毒]]
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	S = 1
ENDIF

;快楽刻印
IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 2
	PRINTS MARKNAME:[[MARK:快楽刻印]]
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 2,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:[[PALAM:欲情]]
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:[[TALENT:恥じらい]]
	PRINTV '(,1,')
	S = 1
ENDIF
;快感の否定
IF TALENT:快感の否定
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:[[TALENT:快感の否定]]
	PRINTV '(,3,')
	S = 1
ENDIF
;男嫌い
IF TALENT:男性不信 && TALENT:PLAYER:オトコ
	PRINT  - 
	A -= 7
	PRINTS TALENTNAME:[[TALENT:男性不信]]
	PRINTV '(,7,')
	S = 1
ENDIF
;愛
IF TALENT:恋心 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:[[TALENT:恋心]]
	PRINTV '(,3,')
	S = 3
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:[[TALENT:ふたなり]]
	PRINTV '(,8,')
	S = 1
ENDIF

;しあわせ草
IF TEQUIP:媚薬効果
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:[[ITEM:プリンセスポイズン]]
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2

;素股では汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		PRINT 汚れあり、
		PRINTS TALENTNAME:[[TALENT:汚臭鈍感]]
	;汚臭敏感
	ELSEIF TALENT:汚臭敏感
		PRINT 汚れあり、
		PRINTS TALENTNAME:[[TALENT:汚臭敏感]]
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(25以上で実行)
PRINT  = 
PRINTV A

V = 25
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

SELECTCOM = 70
SAVESTR:0 = ダブル素股
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 40
LOSEBASE:気力 += 160

SOURCE:屈従 = 1500
SOURCE:逸脱 = 600

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*10 + 60

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 200
	SOURCE:達成 = 100
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 250
	SOURCE:達成 = 180
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 300
	SOURCE:達成 = 250
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 350
	SOURCE:達成 = 350
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 400
	SOURCE:達成 = 500
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 450
	SOURCE:達成 = 800
	TIMES SOURCE:不潔 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.70
	TIMES SOURCE:達成 , 0.70
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.90
	TIMES SOURCE:達成 , 0.90
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 1.40
	TIMES SOURCE:達成 , 1.40
ELSE
	TIMES SOURCE:性行動 , 1.60
	TIMES SOURCE:達成 , 1.60
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:快Ｃ = 20
	TIMES SOURCE:性行動 , 0.80
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:快Ｃ = 80
	TIMES SOURCE:性行動 , 0.90
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:快Ｃ = 350
	TIMES SOURCE:性行動 , 1.00
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:快Ｃ = 750
	TIMES SOURCE:性行動 , 1.10
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:快Ｃ = 1200
	TIMES SOURCE:性行動 , 1.20
ELSE
	SOURCE:快Ｃ = 1750
	TIMES SOURCE:性行動 , 1.30
ENDIF

;PALAM:潤滑
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｃ , 0.30
	TIMES SOURCE:性行動 , 0.60
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｃ , 0.60
	TIMES SOURCE:性行動 , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｃ , 1.00
	TIMES SOURCE:性行動 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｃ , 1.50
	TIMES SOURCE:性行動 , 1.20
ELSEIF PALAM:潤滑 < PALAMLV:5
	TIMES SOURCE:快Ｃ , 2.00
	TIMES SOURCE:性行動 , 1.40
ELSE
	TIMES SOURCE:快Ｃ , 2.50
	TIMES SOURCE:性行動 , 1.60
ENDIF

;助手のABL:Ｃ感覚をみる
IF ABL:ASSI:Ｃ感覚 == 0
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成 , 0.50
ELSEIF ABL:ASSI:Ｃ感覚 == 1
	TIMES SOURCE:性行動 , 0.90
	TIMES SOURCE:達成 , 0.70
ELSEIF ABL:ASSI:Ｃ感覚 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成 , 1.00
ELSEIF ABL:ASSI:Ｃ感覚 == 3
	TIMES SOURCE:性行動 , 1.10
	TIMES SOURCE:達成 , 1.20
ELSEIF ABL:ASSI:Ｃ感覚 == 4
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成 , 1.40
ELSE
	TIMES SOURCE:性行動 , 1.30
	TIMES SOURCE:達成 , 1.70
ENDIF

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:技巧 == 0
	B = 500
ELSEIF ABL:技巧 == 1
	B = 1100
ELSEIF ABL:技巧 == 2
	B = 2000
ELSEIF ABL:技巧 == 3
	B = 3000
ELSEIF ABL:技巧 == 4
	B = 3900
ELSE
	B = 4600
ENDIF

;PALAM:潤滑
IF PALAM:潤滑 < PALAMLV:1
	B += 0
ELSEIF PALAM:潤滑 < PALAMLV:2
	B += 0
ELSEIF PALAM:潤滑 < PALAMLV:3
	B += 0
ELSEIF PALAM:潤滑 < PALAMLV:4
	B += 300
ELSEIF PALAM:潤滑 < PALAMLV:5
	B += 600
ELSE
	B += 1000
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES B , 0.80
ELSEIF ABL:信頼 == 1
	TIMES B , 0.90
ELSEIF ABL:信頼 == 2
	TIMES B , 1.00
ELSEIF ABL:信頼 == 3
	TIMES B , 1.10
ELSEIF ABL:信頼 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	TIMES B , 0.50
ELSEIF ABL:奉仕精神 == 1
	TIMES B , 0.80
ELSEIF ABL:奉仕精神 == 2
	TIMES B , 1.20
ELSEIF ABL:奉仕精神 == 3
	TIMES B , 1.50
ELSEIF ABL:奉仕精神 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:Ｃ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
	BASE:PLAYER:射精ゲージ += B

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:射精ゲージ
EJAC = MAXBASE:PLAYER:射精ゲージ

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:性行動 , 2.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 = 0
		TIMES SOURCE:達成 , 1.50
		TIMES SOURCE:屈従, 1.40
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 = 200
		TIMES SOURCE:達成 , 2.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 = 400
		TIMES SOURCE:達成 , 2.50
		TIMES SOURCE:屈従, 0.80
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 = 700
		TIMES SOURCE:達成 , 3.00
		TIMES SOURCE:屈従, 0.50
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 = 1000
		TIMES SOURCE:達成 , 4.00
		TIMES SOURCE:屈従, 0.20
	ELSE
		SOURCE:中毒充足 = 1500
		TIMES SOURCE:達成 , 5.00
		TIMES SOURCE:屈従, 0.00
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:中毒充足 , 1.50
	TIMES SOURCE:達成 , 1.20

	EXP:PLAYER:射精経験 += 2
	EXP:レズ経験 += 2
	PRINTL 大量射精
	PRINTL 精液経験+2
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:射精ゲージ -= EJAC*2
	SIF BASE:PLAYER:射精ゲージ >= EJAC
		BASE:PLAYER:射精ゲージ = EJAC-1

	;素股で射精フラグ
	TFLAG:素股で射精 = 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:射精経験 += 1
	EXP:レズ経験 += 1
	PRINTL 射精
	PRINTL 精液経験+1
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:射精ゲージ -= EJAC
	SIF BASE:PLAYER:射精ゲージ >= EJAC
		BASE:PLAYER:射精ゲージ = EJAC-1

	;素股で射精フラグ
	TFLAG:素股で射精 = 1
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＶ⇔調教者のＰの汚れが移動
STAIN:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:3

;助手のＶ⇔調教者のＰの汚れが移動
STAIN:ASSI:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:ASSI:3

;奴隷のＶ⇔助手のＶの汚れが移動
STAIN:3 |= STAIN:ASSI:3
STAIN:ASSI:3 |= STAIN:3

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;マスターと奴隷のレズorホモ
IF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +7
	EXP:レズ経験 += 7
ELSEIF TALENT:オトコ == 1 && TALENT:MASTER:オトコ == 1
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +7
	EXP:ＢＬ経験 += 7
ENDIF

;助手と奴隷のレズorホモ
IF TALENT:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +2
	EXP:レズ経験 += 2
ELSEIF TALENT:オトコ == 1 && TALENT:ASSI:オトコ == 1
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +2
	EXP:ＢＬ経験 += 2
ENDIF

;愛情経験
E = 1
IF CFLAG:好感度 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:[[EXP:奉仕快楽経験]]%+{E}
	EXP:奉仕快楽経験 += E
ENDIF
E = 0

;------------------------------------------------
;その他の処理
;------------------------------------------------
;快楽経験
TFLAG:快楽経験 = 1

;屈服刻印１に相当
TFLAG:屈服刻印2 = 1

;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SOURCE:屈従 /= 2
ENDIF


RETURN 1
