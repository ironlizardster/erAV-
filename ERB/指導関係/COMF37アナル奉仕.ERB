;-------------------------------------------------
;アナル奉仕
;奉仕系コマンド：調教対象が調教者のアナルを口で刺激
;-------------------------------------------------
@COM37

IF (ASSIPLAY && TFLAG:前回の調教者が助手) || (ASSIPLAY == 0 && TFLAG:前回の調教者が助手 == 0)
	IF PREVCOM == [[TRAIN:手淫]]
		CALL COM_ABLE138
		SIF RESULT == 1
			JUMP COM138
	ENDIF
ENDIF

PRINTL アナル奉仕

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINTS ABLNAME:[[ABL:欲望]]
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 3
	PRINTS ABLNAME:[[ABL:奉仕精神]]
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 3,')
	S = 1
ENDIF

IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 2
	PRINTS MARKNAME:[[MARK:快楽刻印]]
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 2,')
	S = 1
ENDIF

IF MARK:屈服刻印
	SIF S
		PRINT  + 
	A += MARK:屈服刻印 * 3
	PRINTS MARKNAME:[[MARK:屈服刻印]]
	PRINTV 'LV,MARK:屈服刻印,'(,MARK:屈服刻印 * 3,')
	S = 1
ENDIF

IF MARK:反発刻印
	SIF S
		PRINT  - 
	A -= MARK:反発刻印 * 5
	PRINTS MARKNAME:[[MARK:反発刻印]]
	PRINTV 'LV,MARK:反発刻印,'(,MARK:反発刻印 * 5,')
	S = 1
ENDIF

;反抗的
IF TALENT:ナマイキ
	PRINT  - 
	A -= 5
	PRINTS TALENTNAME:[[TALENT:ナマイキ]]
	PRINTV '(,5,')
	S = 1
ENDIF
;プライド高い
IF IS_プライド高い(TARGET)
	PRINT  - 
	A -= 12
	PRINTS GET_STR_プライド(TARGET)
	PRINTV '(,12,')
	S = 1
ENDIF
;プライド低い
IF IS_プライド低い(TARGET)
	PRINT  + 
	A += 2
	PRINTS GET_STR_プライド(TARGET)
	PRINTV '(,2,')
	S = 1
ENDIF
;抵抗
IF TALENT:抵抗
	PRINT  - 
	A -= 20
	PRINTS TALENTNAME:[[TALENT:抵抗]]
	PRINTV '(,20,')
	S = 1
ENDIF
;恥じらい
IF IS_恥じらい(TARGET)
	PRINT  - 
	A -= 1
	PRINTS GET_STR_恥じらい(TARGET)
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF IS_汚臭鈍感(TARGET)
	SIF S
		PRINT  + 
	A += 2
	PRINTS GET_STR_汚臭耐性(TARGET)
	PRINTV '(,2,')
	S = 1
ENDIF
;汚臭敏感
IF IS_汚臭敏感(TARGET)
	PRINT  - 
	A -= 5
	PRINTS GET_STR_汚臭耐性(TARGET)
	PRINTV '(,5,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:[[TALENT:献身的]]
	PRINTV '(,4,')
	S = 1
ENDIF
;汚れ無視
IF TALENT:汚れ無視
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:[[TALENT:汚れ無視]]
	PRINTV '(,3,')
	S = 1
ENDIF
;快感の否定
IF IS_快感の否定(TARGET)
	PRINT  - 
	A -= 1
	PRINTS GET_STR_快感に素直(TARGET)
	PRINTV '(,1,')
	S = 1
ENDIF
;倒錯的
IF TALENT:倒錯的
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:[[TALENT:倒錯的]]
	PRINTV '(,3,')
	S = 1
ENDIF
;サド
IF TALENT:PLAYER:サド
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:[[TALENT:サド]]
	PRINTV '(,3,')
	S = 1
ENDIF
;愛
IF TALENT:恋心 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:[[TALENT:恋心]]
	PRINTV '(,5,')
	S = 1
ENDIF
;獣姦
IF TEQUIP:獣姦プレイ && TALENT:牝犬 == 0
	SIF S
		PRINT  - 
	A -= 20
	PRINTS ITEMNAME:[[ITEM:犬]]
	PRINTV '(,20,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:アナル & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:アナル & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:アナル & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:アナル & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:アナル & 32
	Y += 3

;獣姦の場合は汚れ7で固定
SIF TEQUIP:獣姦プレイ
	Y = 7

SIF IS_汚臭鈍感(TARGET)
	Y /= 3
SIF IS_汚臭敏感(TARGET)
	Y *= 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF IS_汚臭鈍感(TARGET)
		PRINT 汚れあり、
		PRINTS GET_STR_汚臭耐性(TARGET)
	ELSEIF IS_汚臭敏感(TARGET)
		PRINT 汚れあり、
		PRINTS GET_STR_汚臭耐性(TARGET)
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF


;合計を表示(23以上で実行)
PRINT  = 
PRINTV A

V = 23
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

SAVESTR:0 = アナル奉仕強制
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:気力 += 10
LOSEBASE:気力 += 100

SOURCE:屈従 = 3000
SOURCE:逸脱 = 5000

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*80 + 50

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 420
	SOURCE:達成 = 150
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 500
	SOURCE:達成 = 300
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 580
	SOURCE:達成 = 600
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 660
	SOURCE:達成 = 900
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 740
	SOURCE:達成 = 1500
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 820
	SOURCE:達成 = 2200
	TIMES SOURCE:不潔 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.50
	TIMES SOURCE:達成 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.50
	TIMES SOURCE:達成 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 2.50
	TIMES SOURCE:達成 , 2.50
ELSE
	TIMES SOURCE:性行動 , 4.00
	TIMES SOURCE:達成 , 4.00
ENDIF

;獣姦の場合はここで終了
IF TEQUIP:獣姦プレイ
	;犬のアナルと初キス
	IF !CFLAG:初キス相手
		TFLAG:初キス口以外 = 1
		;屈服刻印３に相当
		TFLAG:屈服刻印2 = 3
		CALL 初キス喪失実行(TARGET,初相手_犬,CFLAG:実年齢,DAY:日付_月,DAY:日付_日,初場所_スタジオ,初状況_肛門,"犬")
	ENDIF

	RETURN 1
ENDIF

;-------------------------------------------------
;調教者の射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:技巧 == 0
	B = 100
ELSEIF ABL:技巧 == 1
	B = 300
ELSEIF ABL:技巧 == 2
	B = 500
ELSEIF ABL:技巧 == 3
	B = 800
ELSEIF ABL:技巧 == 4
	B = 1200
ELSE
	B = 1600
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES B , 0.80
ELSEIF ABL:信頼 == 1
	TIMES B , 0.90
ELSEIF ABL:信頼 == 2
	TIMES B , 1.00
ELSEIF ABL:信頼 == 3
	TIMES B , 1.10
ELSEIF ABL:信頼 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕技術をみる
IF ABL:奉仕技術 == 0
	TIMES B , 0.50
ELSEIF ABL:奉仕技術 == 1
	TIMES B , 0.80
ELSEIF ABL:奉仕技術 == 2
	TIMES B , 1.20
ELSEIF ABL:奉仕技術 == 3
	TIMES B , 1.50
ELSEIF ABL:奉仕技術 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;TALENT:舌使いをみる
SIF TALENT:舌使い
	TIMES B , 2.00

;プレイヤーのABL:Ａ感覚をみる
IF ABL:PLAYER:Ａ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ａ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ａ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ａ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ａ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF HAVE_PENIS(PLAYER)
	BASE:PLAYER:射精ゲージ += B

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:射精ゲージ
EJAC = MAXBASE:PLAYER:射精ゲージ

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:性行動 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:充足 = 0
		TIMES SOURCE:達成 , 2.00
		TIMES SOURCE:屈従, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:充足 = 200
		TIMES SOURCE:達成 , 2.50
		TIMES SOURCE:屈従, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:充足 = 500
		TIMES SOURCE:達成 , 3.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:充足 = 1200
		TIMES SOURCE:達成 , 4.50
		TIMES SOURCE:屈従, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:充足 = 2500
		TIMES SOURCE:達成 , 6.00
		TIMES SOURCE:屈従, 0.40
	ELSE
		SOURCE:充足 = 5000
		TIMES SOURCE:達成 , 8.00
		TIMES SOURCE:屈従, 0.10
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:充足 , 2.00
	TIMES SOURCE:達成 , 1.50

	EXP:PLAYER:射精経験 += 2
	EXP:精液経験 += 4
	PRINTL 大量射精
	PRINTL 精液経験＋４
	;精液汚れ
	STAIN:PLAYER:ペニス |= 4

	BASE:PLAYER:射精ゲージ -= EJAC*2
	SIF BASE:PLAYER:射精ゲージ >= EJAC
		BASE:PLAYER:射精ゲージ = EJAC-1

	;クンニで射精させたフラグ
	TFLAG:クンニで射精 = 2
;通常の射精
ELSEIF E == 1
	EXP:PLAYER:射精経験 += 1
	EXP:精液経験 += 1
	PRINTL 射精
	PRINTL 精液経験＋１
	;精液汚れ
	STAIN:PLAYER:ペニス |= 4

	BASE:PLAYER:射精ゲージ -= EJAC
	SIF BASE:PLAYER:射精ゲージ >= EJAC
		BASE:PLAYER:射精ゲージ = EJAC-1

	;クンニで射精させたフラグ
	TFLAG:クンニで射精 = 1
ENDIF

;-------------------------------------------------
;汚れ
;-------------------------------------------------
;奴隷の口⇔調教者のＡの汚れが移動
STAIN:口 |= STAIN:PLAYER:アナル
STAIN:PLAYER:アナル |= STAIN:口

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF HAVE_VAGINA(TARGET) && HAVE_VAGINA(PLAYER) 
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +6
	EXP:レズ経験 += 6
ENDIF

TFLAG:快楽経験 = 1

;屈服刻印３に相当
TFLAG:屈服刻印2 = 3

;初キス
;標準の初キス判定を行うように
TFLAG:621 |= 1
TFLAG:621 |= 4
