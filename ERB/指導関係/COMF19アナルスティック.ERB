;-------------------------------------------------
;アナルビーズ
;道具使用コマンド：調教対象のアナルをアナルビーズで刺激
;-------------------------------------------------
@COM19

PRINTL アナルスティック
SAVESTR:0 = アナルスティック
CALL TRAIN_MESSAGE_B

LOSEBASE:体力 += 60
LOSEBASE:気力 += 150

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ = 80
	SOURCE:屈従 = 300
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ = 250
	SOURCE:屈従 = 800
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ = 600
	SOURCE:屈従 = 1400
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ = 1000
	SOURCE:屈従 = 1800
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ = 1300
	SOURCE:屈従 = 2100
ELSE
	SOURCE:快Ａ = 1700
	SOURCE:屈従 = 2400
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:快Ａ , 0.50
	SOURCE:痛覚 = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛覚 = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:快Ａ , 1.10
	SOURCE:痛覚 = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:快Ａ , 1.20
	SOURCE:痛覚 = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛覚 = 0
ELSE
	TIMES SOURCE:快Ａ , 1.60
	SOURCE:痛覚 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛覚 += 1200
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛覚 += 700
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛覚 += 400
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛覚 += 150
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛覚 += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:快Ａ , 0.80
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:快Ａ , 0.90
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:快Ａ , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:快Ａ , 1.10
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:快Ａ , 1.20
ELSE
	TIMES SOURCE:快Ａ , 1.30
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:痛覚 , 0.80
;小柄
SIF TALENT:小柄
	TIMES SOURCE:痛覚 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:痛覚 , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛覚 , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛覚 , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	SOURCE:屈従 /= 3
ENDIF

SIF TEQUIP:アナルビーズ == 0
	TIMES SOURCE:快Ａ , 0.80

SIF TEQUIP:アナルビーズ == 1
	TIMES SOURCE:快Ａ , 3.00

						;****スライムパッチ追加ここから****
IF ITEM:魔術式アナルビーズ >= 1 && TEQUIP:触手召喚 == 0
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 0.80
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:痛覚 , 0.80
	TIMES SOURCE:屈従 , 1.20
	TIMES SOURCE:逸脱 , 0.80
ENDIF
						;****スライムパッチ追加ここまで****
;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF TEQUIP:アナルビーズ == 0
EXP:Ａ経験 += 1
PRINTL Ａ経験＋１
ELSEIF TEQUIP:アナルビーズ == 1
EXP:Ａ経験 += 2
PRINTL Ａ経験＋２
ENDIF

;-------------------------------------------------
;アナルビーズの着脱
;-------------------------------------------------
TEQUIP:アナルビーズ = 1 - TEQUIP:アナルビーズ

RETURN 1

;-------------------------------------------------
;-------------------------------------------------
;アナルビーズ
;-------------------------------------------------
@EQUIP_COM19

PRINTL ＜アナルビーズ挿入中＞

LOSEBASE:体力 += 10
LOSEBASE:気力 += 30

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	A = 40
	B = 100
ELSEIF ABL:Ａ感覚 == 1
	A = 80
	B = 300
ELSEIF ABL:Ａ感覚 == 2
	A = 100
	B = 500
ELSEIF ABL:Ａ感覚 == 3
	A = 150
	B = 700
ELSEIF ABL:Ａ感覚 == 4
	A = 200
	B = 900
ELSE
	A = 300
	B = 1200
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES A , 0.50
	C = 2000
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES A , 1.00
	C = 300
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES A , 1.10
	C = 50
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES A , 1.20
	C = 10
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES A , 1.40
	C = 0
ELSE
	TIMES A , 1.60
	C = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.40
	C += 800
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.80
	C += 500
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	C += 300
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	C += 120
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES A , 1.80
	C += 100
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES A , 0.80
ELSEIF ABL:信頼 == 1
	TIMES A , 0.90
ELSEIF ABL:信頼 == 2
	TIMES A , 1.00
ELSEIF ABL:信頼 == 3
	TIMES A , 1.10
ELSEIF ABL:信頼 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF

;大柄
SIF TALENT:大柄
	TIMES C , 0.80
;小柄
SIF TALENT:小柄
	TIMES C , 1.50
;未熟
SIF TALENT:未熟
	TIMES C , 2.00

;Ａ敏感、鈍感をみる
;快Ａ自体のチェックは後でまとめてやる
IF TALENT:Ａ鈍感
	TIMES SOURCE:痛覚 , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ａ敏感
	TIMES SOURCE:痛覚 , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

SOURCE:快Ａ += A
SOURCE:屈従 += B
SOURCE:痛覚 += C

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	SOURCE:屈従 /= 3
ENDIF

						;****スライムパッチ追加ここから****
IF ITEM:魔術式アナルビーズ >= 1 && TEQUIP:触手召喚 == 0
	LOSEBASE:体力 -= 2
	LOSEBASE:気力 -= 6
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:痛覚 , 0.80
	TIMES SOURCE:屈従 , 1.20
	TIMES SOURCE:逸脱 , 0.80
ENDIF
						;****スライムパッチ追加ここまで****
;-------------------------------------------------
;経験上昇
;-------------------------------------------------
EXP:Ａ経験 += 1
PRINTL Ａ経験＋１

IF HAVE_VAGINA(TARGET) && HAVE_VAGINA(PLAYER) 
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF IS_MALE(TARGET) && IS_MALE(PLAYER) 
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF TEQUIP:触手召喚
	T += 1

RETURN 1
;
