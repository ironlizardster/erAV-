;------------------------------------------------------------
;野外プレイ
;奴隷を野外に連れて行く
;------------------------------------------------------------
@COM54

PRINTL 撮影セット変更
SAVESTR:0 = 撮影セット変更


;屋外への出入り
;終了時は修正なし
IF TEQUIP:野外プレイ
	TEQUIP:野外プレイ = 0
	RETURN 1
ENDIF

IF ABL:露出癖 <= 4
	LOSEBASE:気力 += 50
ELSEIF ABL:露出癖 == 5
	LOSEBASE:気力 += 40
ELSEIF ABL:露出癖 == 6
	LOSEBASE:気力 += 30
ELSEIF ABL:露出癖 == 7
	LOSEBASE:気力 += 20
ELSEIF ABL:露出癖 == 8
	LOSEBASE:気力 += 10
ELSEIF ABL:露出癖 >= 9
	LOSEBASE:気力 += 0
ENDIF



A = 500
B = 500

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF
;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:中毒充足 += 0
	SOURCE:愛液分泌 += 0
	TIMES A, 1.00
ELSEIF ABL:露出癖 == 1
	SOURCE:中毒充足 += 50
	SOURCE:愛液分泌 += 50
	TIMES A, 1.20
ELSEIF ABL:露出癖 == 2
	SOURCE:中毒充足 += 150
	SOURCE:愛液分泌 += 150
	TIMES A, 1.40
ELSEIF ABL:露出癖 == 3
	SOURCE:中毒充足 += 400
	SOURCE:愛液分泌 += 400
	TIMES A, 1.60
ELSEIF ABL:露出癖 == 4
	SOURCE:中毒充足 += 750
	SOURCE:愛液分泌 += 750
	TIMES A, 2.00
ELSE
	SOURCE:中毒充足 += 1300
	SOURCE:愛液分泌 += 1300
	TIMES A, 3.00
ENDIF
;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES A , 0.80
ELSEIF ABL:マゾっ気 == 1
	TIMES A , 1.00
ELSEIF ABL:マゾっ気 == 2
	TIMES A , 1.30
ELSEIF ABL:マゾっ気 == 3
	TIMES A , 1.60
ELSEIF ABL:マゾっ気 == 4
	TIMES A , 2.00
ELSE
	TIMES A , 3.00
ENDIF

;目立ちたがり
SIF TALENT:目立ちたがり
	TIMES A , 1.50
;解放
SIF IS_解放(TARGET)
	TIMES A , 1.50

;臆病
SIF TALENT:臆病
	TIMES B , 2.00
;恥じらい
SIF IS_恥じらい(TARGET)
	TIMES B , 2.00

;露出狂
IF TALENT:露出狂
	TIMES A , 2.00
	TIMES B , 0.50
ENDIF

SOURCE:露出 += A
SOURCE:逸脱 += B
SOURCE:恭順 += A/2

$INPUT_LOOP_00
IF TEQUIP:野外プレイ == 0
	PRINTFORML 何処に行きますか？
	PRINTL  [1] - 野外
	PRINTL  [2] - 教室のセット
	PRINTL  [3] - プール
	INPUT
	IF RESULT == 1
		TEQUIP:野外プレイ = 1
	ELSEIF RESULT == 2
		TEQUIP:野外プレイ = 2
	ELSEIF RESULT == 3
		TEQUIP:野外プレイ = 3
	ELSE
		GOTO INPUT_LOOP_00
	ENDIF
ENDIF	

CALL TRAIN_MESSAGE_B

EXP:野外露出経験 += 1
PRINTFORML 野外露出経験＋1

;野外プレイが始めての場合
IF TEQUIP:野外プレイ && CFLAG:野外露出済 == 0
	PRINTFORML 異常経験＋１
	EXP:異常経験 += 1
	CFLAG:野外露出済 = 1
ENDIF

RETURN 1

;野外プレイ
@EQUIP_COM54

IF TEQUIP:野外プレイ == 1 
	PRINTL ＜野外プレイ中＞
ELSEIF TEQUIP:野外プレイ == 2
	IF TIME == 0
		PRINTL ＜教室プレイ中(昼)＞
	ELSE
		PRINTL ＜教室プレイ中(放課後)＞
	ENDIF
ELSEIF TEQUIP:野外プレイ == 3
	PRINTL ＜野外プレイ中(プール)＞
ENDIF

IF ABL:露出癖 <= 4
	LOSEBASE:気力 += 50
ELSEIF ABL:露出癖 == 5
	LOSEBASE:気力 += 40
ELSEIF ABL:露出癖 == 6
	LOSEBASE:気力 += 30
ELSEIF ABL:露出癖 == 7
	LOSEBASE:気力 += 20
ELSEIF ABL:露出癖 == 8
	LOSEBASE:気力 += 10
ELSEIF ABL:露出癖 >= 9
	LOSEBASE:気力 += 0
ENDIF


IF TEQUIP:野外プレイ == 1
	EXP:野外露出経験 += 1
	PRINTFORML 野外露出経験＋1
ENDIF

A = 500
B = 500

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF
;ABL:露出癖をみる
IF ABL:露出癖 == 0
	SOURCE:中毒充足 += 0
	SOURCE:愛液分泌 += 0
	TIMES A, 1.00
ELSEIF ABL:露出癖 == 1
	SOURCE:中毒充足 += 50
	SOURCE:愛液分泌 += 50
	TIMES A, 1.20
ELSEIF ABL:露出癖 == 2
	SOURCE:中毒充足 += 150
	SOURCE:愛液分泌 += 150
	TIMES A, 1.40
ELSEIF ABL:露出癖 == 3
	SOURCE:中毒充足 += 400
	SOURCE:愛液分泌 += 400
	TIMES A, 1.60
ELSEIF ABL:露出癖 == 4
	SOURCE:中毒充足 += 750
	SOURCE:愛液分泌 += 750
	TIMES A, 2.00
ELSE
	SOURCE:中毒充足 += 1300
	SOURCE:愛液分泌 += 1300
	TIMES A, 3.00
ENDIF
;ABL:マゾっ気をみる
IF ABL:マゾっ気 == 0
	TIMES A , 0.80
ELSEIF ABL:マゾっ気 == 1
	TIMES A , 1.00
ELSEIF ABL:マゾっ気 == 2
	TIMES A , 1.30
ELSEIF ABL:マゾっ気 == 3
	TIMES A , 1.60
ELSEIF ABL:マゾっ気 == 4
	TIMES A , 2.00
ELSE
	TIMES A , 3.00
ENDIF

;目立ちたがり
SIF TALENT:目立ちたがり
	TIMES A , 1.50
;解放
SIF IS_解放(TARGET)
	TIMES A , 1.50

;臆病
SIF TALENT:臆病
	TIMES B , 2.00
;恥じらい
SIF IS_恥じらい(TARGET)
	TIMES B , 2.00
;露出狂
IF TALENT:露出狂
	TIMES A , 2.00
	TIMES B , 0.50
ENDIF

SOURCE:露出 += A
SOURCE:逸脱 += B
SOURCE:恭順 += A/2

;---------------------------------------------
;レズ・ホモ経験の追加
;---------------------------------------------
IF HAVE_VAGINA(TARGET) && HAVE_VAGINA(PLAYER) 
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF IS_MALE(TARGET) && IS_MALE(PLAYER) 
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

SIF ASSIPLAY == 0 && ABL:露出癖 >= 3
	TFLAG:主人経験 += 1

RETURN 1
