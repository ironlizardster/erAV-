;-------------------------------------------------
;後背位ＳＰ
;セックス系コマンド：立ちバック＋子宮責め
;-------------------------------------------------
@COM134

SELECTCOM = 134

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0



PRINTL 後背位ＳＰ
SAVESTR:0 = 後背位ＳＰ
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0 && TEQUIP:獣姦プレイ == 0
	TFLAG:主人による処女喪失 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 60
LOSEBASE:気力 += 120

SOURCE:12 = 800

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:4 = 50
	SOURCE:5 = 10
ELSEIF ABL:奉仕精神 == 1
	SOURCE:4 = 150
	SOURCE:5 = 50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:4 = 200
	SOURCE:5 = 100
ELSEIF ABL:奉仕精神 == 3
	SOURCE:4 = 250
	SOURCE:5 = 180
ELSEIF ABL:奉仕精神 == 4
	SOURCE:4 = 300
	SOURCE:5 = 300
ELSE
	SOURCE:4 = 350
	SOURCE:5 = 500
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:4 , 4.00
	TIMES SOURCE:5 , 4.00
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 10
	SOURCE:3 = 250
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 50
	SOURCE:3 = 500
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 450
	SOURCE:3 = 550
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 800
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 2800
	SOURCE:3 = 1200
ELSE
	SOURCE:1 = 4000
	SOURCE:3 = 1800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5000

	IF ASSIPLAY && TALENT:PLAYER:オトコ == 0
		EXP:異常経験 += 1
		PRINTFORML %EXPNAME:[[EXP:異常経験]]%＋１
	ENDIF
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 220
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:17 = 20
	SOURCE:3 += 50
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:17 = 100
	SOURCE:3 += 100
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:17 = 600
	SOURCE:3 += 200
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:17 = 1400
	SOURCE:3 += 300
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:17 = 2200
	SOURCE:3 += 600
ELSE
	SOURCE:17 = 3200
	SOURCE:3 += 1000
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 900
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 250
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:ふたなり
		TIMES SOURCE:1 , 2.50
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 1.80
;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00

;貞操観念
IF TALENT:貞操観念
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:貞操無頓着
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:Ｖ経験 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;愛
IF TALENT:恋心
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 2.00
	TIMES SOURCE:17 , 1.50
ENDIF

;後背位スパンキングを挟んでいた場合、マゾっ気補正がさらにかかる
IF TFLAG:前々回のコマンド == 132 || PREVCOM == 132
	;ABL:マゾっ気をみる
	IF ABL:マゾっ気 == 0
		TIMES SOURCE:1 , 0.60
		TIMES SOURCE:10 , 0.80
		TIMES SOURCE:13 , 0.80
	ELSEIF ABL:マゾっ気 == 1
		TIMES SOURCE:1 , 0.80
		TIMES SOURCE:10 , 1.00
		TIMES SOURCE:13 , 1.00
	ELSEIF ABL:マゾっ気 == 2
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:10 , 1.20
		TIMES SOURCE:13 , 1.50
	ELSEIF ABL:マゾっ気 == 3
		TIMES SOURCE:1 , 1.10
		TIMES SOURCE:10 , 1.40
		TIMES SOURCE:13 , 3.00
	ELSEIF ABL:マゾっ気 == 4
		TIMES SOURCE:1 , 1.20
		TIMES SOURCE:10 , 2.00
		TIMES SOURCE:13 , 5.00
	ELSE
		TIMES SOURCE:1 , 1.30
		TIMES SOURCE:10 , 3.00
		TIMES SOURCE:13 , 8.00
	ENDIF
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者の口の汚れが移動
STAIN:0 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:0

;奴隷のＢ⇔調教者の指の汚れが移動
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
