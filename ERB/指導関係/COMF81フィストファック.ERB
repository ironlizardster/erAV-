@COM81
;フィストファック
PRINTL フィストファック
SAVESTR:0 = フィストファック
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1

LOSEBASE:体力 += 600
LOSEBASE:気力 += 300

SOURCE:6 = 1200
SOURCE:12 = 1200
SOURCE:13 = 1200
SOURCE:14 = 1200
SOURCE:16 = 1200


;ABL:V感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 2000
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 2500
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 3000
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 3300
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 3600
ELSE
	SOURCE:1 = 3800
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:6 , 4.00

IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

EXP:Ｖ経験 += 25
PRINTL Ｖ経験＋２５

IF EXP:Ｖ拡張経験 == 0 
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

EXP:Ｖ拡張経験 += 1
PRINTL Ｖ拡張経験＋1

RETURN 1
