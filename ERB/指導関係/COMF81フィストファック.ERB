@COM81
;フィストファック
PRINTL フィストファック
SAVESTR:0 = フィストファック
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1

LOSEBASE:体力 += 600
LOSEBASE:気力 += 300

SOURCE:痛覚 = 1200
SOURCE:露出 = 1200
SOURCE:屈従 = 1200
SOURCE:逸脱 = 1200
SOURCE:恭順 = 1200


;ABL:V感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 2000
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 2500
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 3000
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 3300
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 3600
ELSE
	SOURCE:快Ｖ = 3800
ENDIF

;大柄
SIF IS_大柄(TARGET)
	TIMES SOURCE:痛覚 , 0.80
;小柄体型
SIF IS_小柄(TARGET)
	TIMES SOURCE:痛覚 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:痛覚 , 4.00

IF HAVE_VAGINA(TARGET) && HAVE_VAGINA(PLAYER) 
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +1
	EXP:レズ経験 += 1
ELSEIF IS_MALE(TARGET) && IS_MALE(PLAYER) 
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +1
	EXP:ＢＬ経験 += 1
ENDIF

EXP:Ｖ経験 += 25
PRINTL Ｖ経験＋２５

IF EXP:Ｖ拡張経験 == 0 
	EXP:異常経験 += 1
	PRINTL 異常経験＋１
ENDIF

EXP:Ｖ拡張経験 += 1
PRINTL Ｖ拡張経験＋1

RETURN 1
