;=================================================
;逆レイプ
;セックス系コマンド：調教者の%CSTR:女性器呼び方%の中へ調教対象の%CSTR:男性器呼び方%を挿入させる
;=================================================
@COM25

PRINTL 逆アナルレイプ
SAVESTR:0 = 逆アナルレイプ

;-------------------------------------------------
;調教時メッセージ
;-------------------------------------------------
IF (FLAG:指導テキスト設定 & 1) == 0
	DRAWLINE 

	;前回と今回の調教者が同じで、前回の調教が逆レイプであり、抜かないフラグがある場合
	IF TFLAG:挿しっぱなし && PREVCOM == 25 && ((ASSIPLAY && TFLAG:前回の調教者が助手) || (ASSIPLAY == 0 && TFLAG:前回の調教者が助手 == 0))
		PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%の肉棒をきつく食い締めたまま、執拗に腰を振りたてた…
	ELSE
		PRINTFORM %CALLNAME:PLAYER%は
		SIF TEQUIP:縄
			PRINT 縛り上げられた
		PRINTFORM %CALLNAME:TARGET%の体にのしかかり、勃起した肉棒を強引に
		SIF EXP:PLAYER:絶頂経験 == 0
			PRINT 男を知らない
		PRINTFORML アヌスの奥へと咥えこんだ…
	ENDIF

	WAIT
ENDIF

;調教者の処女を与えるどうかの確認
$INPUT_LOOP_01
IF TALENT:PLAYER:アナル処女
	PRINTFORML %CALLNAME:PLAYER%のアナル処女を%CALLNAME:TARGET%に与えますか？
	PRINTL  [0] - はい
	PRINTL  [1] - いいえ
	INPUT
	IF RESULT == 1
		RETURN 0
	ELSEIF RESULT != 0
		GOTO INPUT_LOOP_01
	ENDIF
ENDIF

;-------------------------------------------------
;調教者の射精ゲージチェック（ふたなりの場合）
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:技巧 == 0
	B = 50
ELSEIF ABL:技巧 == 1
	B = 300
ELSEIF ABL:技巧 == 2
	B = 800
ELSEIF ABL:技巧 == 3
	B = 1500
ELSEIF ABL:技巧 == 4
	B = 2000
ELSE
	B = 3200
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES B , 0.80
ELSEIF ABL:信頼 == 1
	TIMES B , 0.90
ELSEIF ABL:信頼 == 2
	TIMES B , 1.00
ELSEIF ABL:信頼 == 3
	TIMES B , 1.10
ELSEIF ABL:信頼 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	TIMES B , 0.50
ELSEIF ABL:奉仕精神 == 1
	TIMES B , 0.80
ELSEIF ABL:奉仕精神 == 2
	TIMES B , 1.20
ELSEIF ABL:奉仕精神 == 3
	TIMES B , 1.50
ELSEIF ABL:奉仕精神 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;調教者のABL:V感覚をみる
IF ABL:PLAYER:Ｖ感覚 == 0
	TIMES B , 0.50
ELSEIF ABL:PLAYER:Ｖ感覚 == 1
	TIMES B , 0.80
ELSEIF ABL:PLAYER:Ｖ感覚 == 2
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｖ感覚 == 3
	TIMES B , 1.20
ELSEIF ABL:PLAYER:Ｖ感覚 == 4
	TIMES B , 1.50
ELSE
	TIMES B , 2.00
ENDIF

;調教者のEXP:Ｖ経験をみる
IF EXP:PLAYER:Ｖ経験 < EXPLV:1
	TIMES B , 0.20
ELSEIF EXP:PLAYER:Ｖ経験 < EXPLV:2
	TIMES B , 0.50
ELSEIF EXP:PLAYER:Ｖ経験 < EXPLV:3
	TIMES B , 0.80
ELSEIF EXP:PLAYER:Ｖ経験 < EXPLV:4
	TIMES B , 1.00
ELSEIF EXP:PLAYER:Ｖ経験 < EXPLV:5
	TIMES B , 1.20
ELSE
	TIMES B , 1.40
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:Ｃ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:Ｃ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:Ｃ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:Ｃ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
	BASE:PLAYER:射精ゲージ += B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 40
LOSEBASE:気力 += 75

SOURCE:3 = 550
SOURCE:13 = 1000
SOURCE:14 = 800

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 800
	TIMES SOURCE:14 , 3.00
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 1400
	TIMES SOURCE:14 , 2.00
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 2000
	TIMES SOURCE:14 , 1.50
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 2500
	TIMES SOURCE:14 , 1.00
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 2900
	TIMES SOURCE:14 , 0.80
ELSE
	SOURCE:0 = 3200
	TIMES SOURCE:14 , 0.60
ENDIF

;調教者のABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES SOURCE:0 , 0.50
	SOURCE:4 = 1600
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES SOURCE:0 , 0.80
	SOURCE:4 = 1900
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES SOURCE:0 , 1.00
	SOURCE:4 = 2300
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES SOURCE:0 , 1.50
	SOURCE:4 = 2700
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES SOURCE:0 , 2.50
	SOURCE:4 = 3100
ELSEIF ABL:PLAYER:技巧 == 5
	TIMES SOURCE:0 , 3.00
	SOURCE:4 = 3500
ELSE
	TIMES SOURCE:0 , 4.00
	SOURCE:4 = 4000
ENDIF

;調教者のABL:V感覚をみる
IF ABL:PLAYER:4 == 0
	TIMES SOURCE:0 , 0.50
ELSEIF ABL:PLAYER:4 == 1
	TIMES SOURCE:0 , 0.80
ELSEIF ABL:PLAYER:4 == 2
	TIMES SOURCE:0 , 1.00
ELSEIF ABL:PLAYER:4 == 3
	TIMES SOURCE:0 , 1.20
ELSEIF ABL:PLAYER:4 == 4
	TIMES SOURCE:0 , 1.50
ELSEIF ABL:PLAYER:4 == 5
	TIMES SOURCE:0 , 2.00
ELSE
	TIMES SOURCE:0 , 3.00
ENDIF

;調教対象がオトコではない
IF TALENT:オトコ == 0
	SOURCE:12 = 1200
	TIMES SOURCE:14 , 6.00
ENDIF


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＰ⇔調教者のＡの汚れが移動
STAIN:2 |= STAIN:PLAYER:4
STAIN:PLAYER:4 |= STAIN:2

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
;レズ・ホモ経験
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +4
	EXP:レズ経験 += 4
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTS EXPNAME:[[EXP:ＢＬ経験]]
	PRINTL +4
	EXP:ＢＬ経験 += 4
ENDIF

EXP:性交経験 += 1
PRINTL 性交経験＋１


S = 0
;Ｖ感覚が高いほどＶ経験が入る
IF ABL:PLAYER:Ａ感覚 <= 1
	S += 2
ELSEIF ABL:PLAYER:Ａ感覚 <= 4
	S += 3
ELSEIF ABL:PLAYER:Ａ感覚 <= 7
	S += 4
ELSE
	S += 5
ENDIF
EXP:PLAYER:Ａ経験 += S
PRINTFORML Ａ経験+{S}(%CALLNAME:PLAYER%)
S = 0

EXP:PLAYER:性交経験 += 1
PRINTFORML 性交経験＋１(%CALLNAME:PLAYER%)

CALL EVENT_SEITSU_REVARAPE

;異常経験
Z = 0
TFLAG:近親相姦 = 0
CALL INCEST

IF Z
	EXP:異常経験 += Z
	PRINTFORML %EXPNAME:[[EXP:異常経験]]%＋{Z}
ENDIF

Z = 0
;逆レイプで処女喪失の場合、調教者の異常経験に＋１
SIF TALENT:PLAYER:アナル処女
	Z += 1
IF Z
	EXP:PLAYER:異常経験 += Z
	PRINTFORML %EXPNAME:[[EXP:異常経験]]%＋{Z}(%CALLNAME:PLAYER%)
ENDIF

TFLAG:近親相姦 = 0

;愛情経験
;調教者が処女で調教対象が童貞
IF TALENT:PLAYER:アナル処女
	E = 100
ENDIF
IF CFLAG:好感度 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:[[EXP:愛情経験]]%+{E}
	EXP:愛情経験 += E
ENDIF
E = 0

;親族関係の判定
TFLAG:近親相姦 = 0
CALL INCEST

;調教者がアナル処女なら処女喪失
IF TALENT:PLAYER:アナル処女
	CSTR:PLAYER:アナル処女喪失相手 = %NAME:TARGET%
	TALENT:PLAYER:アナル処女 = 0
	SETCOLOR 0xF58F98
	PRINTFORML 【アナル処女喪失】(%CALLNAME:PLAYER%)
	RESETCOLOR
	CFLAG:PLAYER:アナル処女喪失年齢 = BASE:実年齢
	CFLAG:PLAYER:アナル処女喪失月 = DAY:日付_月
	CFLAG:PLAYER:アナル処女喪失日 = DAY:日付_日
	CFLAG:PLAYER:アナル処女喪失場所 = 14
	CFLAG:PLAYER:アナル処女喪失シチュ = 1
	CFLAG:PLAYER:アナル処女喪失相手 = 1
ENDIF

TFLAG:近親相姦 = 0

RETURN 1
;
;-------------------------------------------------
;=================================================
;精通
;=================================================
@EVENT_SEITSU_REVARAPE
A = NO:PLAYER
;【オトコ】か【ふたなり】であり【未熟】
SIF (TALENT:ふたなり == 0 && TALENT:オトコ == 0) || TALENT:未熟 == 0
	RETURN 0
;更に［Ｃ感度］５以上で触手調教や獣姦でなく
SIF ABL:Ｃ感覚 <= 4 || TEQUIP:触手召喚 || TEQUIP:獣姦プレイ
	RETURN 0
;「調教対象」から「調教者」への関係が150以上
SIF RELATION:TARGET:A < 150
	RETURN 0
PRINTFORML %NAME:PLAYER%のアヌスでペニスを刺激されて、%NAME:TARGET%は精通を迎えた…
TALENT:未熟 = 0

RETURN 1
