;愛撫実行判定
;-------------------------------------------------
@COM_ABLE0
RETURN 1
;-------------------------------------------------
;クンニ実行判定
;-------------------------------------------------
@COM_ABLE1
;対象がオトコだとダメ
SIF TALENT:オトコ
	RETURN 0
;触手調教中はダメ
SIF TEQUIP:触手召喚
	RETURN 0
						;****スライムパッチ追加ここから****
;スライム調教中はダメ
SIF TEQUIP:スライム生成
	RETURN 0
						;****スライムパッチ追加ここまで****
;三角木馬騎乗中はダメ
SIF TEQUIP:三角木馬騎乗
	RETURN 0
;パンツか上着下・ズボンタイプを着てるとダメ
SIF (CFLAG:着衣状態 & 17)
	RETURN 0
;黒ストッキングだとダメ
SIF CFLAG:靴下タイプ == 6 && CFLAG:靴下着用 == 0
	RETURN 0
;オムツをはいているとダメ
SIF CFLAG:特別コスタイプ == 69 && (CFLAG:着衣状態 & 64)
	RETURN 0
;ズーコの着ぐるみを着てるとダメ
SIF CFLAG:特別コスタイプ == 11 && (CFLAG:着衣状態 & 64)
	RETURN 0
;性器が精液、アナル、尿で汚れており、汚臭敏感で従順3以下のASSIの場合クンニはできない（調教者が汚れ無視ならＯＫ）
IF (STAIN:2 & 4 || STAIN:2 & 8 || STAIN:2 & 32) && ASSIPLAY
	SIF ABL:ASSI:Ｃ感覚 <= 3 && TALENT:ASSI:汚臭敏感 && TALENT:ASSI:汚れ無視 == 0
		RETURN 0
ENDIF
RETURN 1

;-------------------------------------------------
;アナル愛撫実行判定
;-------------------------------------------------
@COM_ABLE2
;獣姦プレイ中はダメ
SIF TEQUIP:獣姦プレイ
	RETURN 0
;三角木馬騎乗中はダメ
SIF TEQUIP:三角木馬騎乗
	RETURN 0
;パンツか上着下・ズボンタイプを着てるとダメ
SIF (CFLAG:着衣状態 & 17)
	RETURN 0
;黒ストッキングだとダメ
SIF CFLAG:靴下タイプ == 6 && CFLAG:靴下着用 == 0
	RETURN 0
;オムツをはいているとダメ
SIF CFLAG:特別コスタイプ == 69 && (CFLAG:着衣状態 & 64)
	RETURN 0
;ズーコの着ぐるみを着てるとダメ
SIF CFLAG:特別コスタイプ == 11 && (CFLAG:着衣状態 & 64)
	RETURN 0
;主人プレイなら自動成功
IF ASSIPLAY == 0
	RETURN 1
;助手プレイの場合
ELSE
	;潤滑不足の場合はダメ
	IF  PALAM:潤滑 < PALAMLV:2
		;助手がサドならOK
		IF TALENT:ASSI:サド
			RETURN 1
		;助手が従順4以上かつレズっ気4以上の場合はOK
		ELSEIF ABL:ASSI:信頼 <= 3 && ABL:ASSI:レズっ気 <= 3
			RETURN 1
		ENDIF
		RETURN 0
	ENDIF
ENDIF
RETURN 1

