;獣姦の場合はここで終了
IF TEQUIP:獣姦プレイ
	;犬の%CSTR:男性器呼び方%と初キス
	IF !CFLAG:初キス相手
		TFLAG:初キス口以外 = 1
		;屈服刻印３に相当
		TFLAG:屈服刻印2 = 3
		CALL 初キス喪失実行(TARGET,初相手_犬,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_スタジオ,初状況_男性器,"犬")
	ENDIF

	RETURN 1
ENDIF

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:技巧 == 0
	B = 1200
ELSEIF ABL:技巧 == 1
	B = 1700
ELSEIF ABL:技巧 == 2
	B = 2300
ELSEIF ABL:技巧 == 3
	B = 3000
ELSEIF ABL:技巧 == 4
	B = 3600
ELSE
	B = 4200
ENDIF

