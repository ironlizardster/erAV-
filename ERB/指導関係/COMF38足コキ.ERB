;-------------------------------------------------
;足コキ
;奉仕系コマンド：調教対象が調教者の%CSTR:男性器呼び方%を足で刺激
;　　　　　　　　嗜虐快楽経験が上昇する
;-------------------------------------------------
@COM38

PRINTL 足コキ

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 1
	PRINTS ABLNAME:[[ABL:欲望]]
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINTS ABLNAME:[[ABL:奉仕精神]]
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF
;ABL:サドっ気
IF ABL:サドっ気
	SIF S
		PRINT  + 
	A += ABL:サドっ気 * 5
	PRINTS ABLNAME:[[ABL:サドっ気]]
	PRINTV 'LV,ABL:サドっ気,'(,ABL:サドっ気 * 5,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 1
	PRINTS ABLNAME:[[ABL:精液中毒]]
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	S = 1
ENDIF

;快楽刻印
IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 1
	PRINTS MARKNAME:[[MARK:快楽刻印]]
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 1,')
	S = 1
ENDIF


;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:[[PALAM:欲情]]
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;恥じらい
IF TALENT:恥じらい
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:[[TALENT:恥じらい]]
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭鈍感
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:[[TALENT:汚臭鈍感]]
	PRINTV '(,1,')
	S = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭敏感
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:[[TALENT:汚臭敏感]]
	PRINTV '(,1,')
	S = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:[[TALENT:献身的]]
	PRINTV '(,6,')
	S = 1
ENDIF
;快感の否定
IF TALENT:快感の否定
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:[[TALENT:快感の否定]]
	PRINTV '(,1,')
	S = 1
ENDIF
;男嫌い
IF TALENT:男性不信 && TALENT:PLAYER:オトコ
	PRINT  - 
	A -= 8
	PRINTS TALENTNAME:[[TALENT:男性不信]]
	PRINTV '(,5,')
	S = 5
ENDIF
;倒錯的
IF TALENT:倒錯的
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:[[TALENT:倒錯的]]
	PRINTV '(,4,')
	S = 4
ENDIF
;愛
IF TALENT:恋心 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:[[TALENT:恋心]]
	PRINTV '(,5,')
	S = 5
ENDIF
;サド
IF TALENT:サド
	SIF S
		PRINT  + 
	A += 10
	PRINTS TALENTNAME:[[TALENT:サド]]
	PRINTV '(,10,')
	S = 4
ENDIF
;小悪魔
IF TALENT:小悪魔
	SIF S
		PRINT  + 
	A += 4
	PRINTS TALENTNAME:[[TALENT:小悪魔]]
	PRINTV '(,4,')
	S = 4
ENDIF
;調教者がふたなり
IF TALENT:PLAYER:ふたなり
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:[[TALENT:ふたなり]]
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

SIF TALENT:汚臭鈍感
	Y /= 3
SIF TALENT:汚臭敏感
	Y *= 2

;足コキでは汚れがそれほど影響しない
Y /= 3

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF TALENT:汚臭鈍感
		PRINT 汚れあり、
		PRINTS TALENTNAME:[[TALENT:汚臭鈍感]]
	ELSEIF TALENT:汚臭敏感
		PRINT 汚れあり、
		PRINTS TALENTNAME:[[TALENT:汚臭敏感]]
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF


;合計を表示(22以上で実行)
PRINT  = 
PRINTV A

V = 22
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

SAVESTR:0 = 足コキ
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 10
LOSEBASE:気力 += 150

SOURCE:屈従 = 550
SOURCE:逸脱 = 400

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*10 + 50

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 300
	SOURCE:達成 = 50
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 350
	SOURCE:達成 = 100
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 400
	SOURCE:達成 = 150
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 450
	SOURCE:達成 = 200
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 500
	SOURCE:達成 = 250
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 580
	SOURCE:達成 = 300
	TIMES SOURCE:不潔 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:性行動 , 0.50
	TIMES SOURCE:達成 , 0.50
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:性行動 , 0.80
	TIMES SOURCE:達成 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:性行動 , 1.50
	TIMES SOURCE:達成 , 1.50
ELSE
	TIMES SOURCE:性行動 , 2.00
	TIMES SOURCE:達成 , 2.00
ENDIF

;ABL:サドっ気をみる
IF ABL:サドっ気 == 0
	TIMES SOURCE:性行動 , 1.00
	TIMES SOURCE:達成 , 1.00
ELSEIF ABL:サドっ気 == 1
	TIMES SOURCE:性行動 , 1.20
	TIMES SOURCE:達成 , 1.20
ELSEIF ABL:サドっ気 == 2
	TIMES SOURCE:性行動 , 1.40
	TIMES SOURCE:達成 , 1.40
ELSEIF ABL:サドっ気 == 3
	TIMES SOURCE:性行動 , 1.60
	TIMES SOURCE:達成 , 1.60
ELSEIF ABL:サドっ気 == 4
	TIMES SOURCE:性行動 , 1.80
	TIMES SOURCE:達成 , 1.80
ELSE
	TIMES SOURCE:性行動 , 2.00
	TIMES SOURCE:達成 , 2.00
ENDIF

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:技巧 == 0
	B = 350
ELSEIF ABL:技巧 == 1
	B = 700
ELSEIF ABL:技巧 == 2
	B = 1300
ELSEIF ABL:技巧 == 3
	B = 1800
ELSEIF ABL:技巧 == 4
	B = 2600
ELSE
	B = 3500
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES B , 0.80
ELSEIF ABL:信頼 == 1
	TIMES B , 0.90
ELSEIF ABL:信頼 == 2
	TIMES B , 1.00
ELSEIF ABL:信頼 == 3
	TIMES B , 1.10
ELSEIF ABL:信頼 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕技術をみる
IF ABL:奉仕技術 == 0
	TIMES B , 0.50
ELSEIF ABL:奉仕技術 == 1
	TIMES B , 0.80
ELSEIF ABL:奉仕技術 == 2
	TIMES B , 1.20
ELSEIF ABL:奉仕技術 == 3
	TIMES B , 1.50
ELSEIF ABL:奉仕技術 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;プレイヤーのABL:マゾっ気をみる
IF ABL:PLAYER:マゾっ気 == 0
	TIMES B , 0.90
ELSEIF ABL:PLAYER:マゾっ気 == 1
	TIMES B , 1.00
ELSEIF ABL:PLAYER:マゾっ気 == 2
	TIMES B , 1.10
ELSEIF ABL:PLAYER:マゾっ気 == 3
	TIMES B , 1.20
ELSEIF ABL:PLAYER:マゾっ気 == 4
	TIMES B , 1.30
ELSE
	TIMES B , 1.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:Ｃ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:オトコ  || TALENT:PLAYER:ふたなり
	BASE:PLAYER:射精ゲージ += B

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
S = BASE:PLAYER:射精ゲージ
EJAC = MAXBASE:PLAYER:射精ゲージ

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;射精している
	TIMES SOURCE:性行動 , 3.00

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:中毒充足 = 0
		TIMES SOURCE:達成 , 2.00
		TIMES SOURCE:屈従, 2.00
	ELSEIF ABL:精液中毒 == 1
		SOURCE:中毒充足 = 200
		TIMES SOURCE:達成 , 2.50
		TIMES SOURCE:屈従, 1.60
	ELSEIF ABL:精液中毒 == 2
		SOURCE:中毒充足 = 600
		TIMES SOURCE:達成 , 3.00
		TIMES SOURCE:屈従, 1.00
	ELSEIF ABL:精液中毒 == 3
		SOURCE:中毒充足 = 1500
		TIMES SOURCE:達成 , 4.00
		TIMES SOURCE:屈従, 0.70
	ELSEIF ABL:精液中毒 == 4
		SOURCE:中毒充足 = 3000
		TIMES SOURCE:達成 , 5.00
		TIMES SOURCE:屈従, 0.40
	ELSE
		SOURCE:中毒充足 = 6000
		TIMES SOURCE:達成 , 6.00
		TIMES SOURCE:屈従 , 0.10
	ENDIF
ENDIF

;大量射精
IF E == 2
	TIMES SOURCE:中毒充足 , 2.00
	TIMES SOURCE:達成 , 1.50

	EXP:PLAYER:射精経験 += 2
	EXP:精液経験 += 2
	PRINTL 大量射精
	PRINTL 精液経験+2
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:射精ゲージ -= EJAC*2
	SIF BASE:PLAYER:射精ゲージ >= EJAC
		BASE:PLAYER:射精ゲージ = EJAC-1

	;足で射精させたフラグ
	TFLAG:足で射精 = 2

;通常の射精
ELSEIF E == 1
	EXP:PLAYER:射精経験 += 1
	EXP:精液経験 += 1
	PRINTL 射精
	PRINTL 精液経験+1

	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:射精ゲージ -= EJAC
	SIF BASE:PLAYER:射精ゲージ >= EJAC
		BASE:PLAYER:射精ゲージ = EJAC-1

	;足で射精させたフラグ
	TFLAG:足で射精 = 1
ENDIF


;-------------------------------------------------
;汚れの処理
;-------------------------------------------------


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;嗜虐快楽経験
Z = 0
IF TALENT:サド == 1 || (ABL:欲望 >= 3 && ABL:サドっ気 >= 3)
	PRINTFORML %EXPNAME:[[EXP:嗜虐快楽経験]]%+3
	EXP:嗜虐快楽経験 += 3
ELSEIF ABL:欲望 >= 3 && ABL:サドっ気 >= 1
	PRINTFORML %EXPNAME:[[EXP:嗜虐快楽経験]]%+2
	EXP:嗜虐快楽経験 += 2
ELSEIF ABL:欲望 >= 3 || ABL:サドっ気 >= 1
	PRINTFORML %EXPNAME:[[EXP:嗜虐快楽経験]]%+1
	EXP:嗜虐快楽経験 += 1
ENDIF
Z = 0

;レズ経験・ホモ経験
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:[[EXP:レズ経験]]%+3
	EXP:レズ経験 += 3
ELSEIF TALENT:オトコ == 1 && TALENT:PLAYER:オトコ == 1
	PRINTFORML %EXPNAME:[[EXP:ＢＬ経験]]%+3
	EXP:ＢＬ経験 += 3
ENDIF

;愛情経験
E = 1
IF CFLAG:好感度 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:[[EXP:愛情経験]]%+{E}
	EXP:愛情経験 += E
ENDIF
E = 0

TFLAG:快楽経験 = 1

;屈服刻印１に相当
TFLAG:屈服刻印2 = 1

;調教者がふたなりなら屈従半減
IF TALENT:PLAYER:ふたなり
	SOURCE:屈従 /= 2
ENDIF

RETURN 1
