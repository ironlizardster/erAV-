;-------------------------------------------------
;バイブ
;道具使用コマンド：調教対象のヴァギナをバイブで刺激
;-------------------------------------------------
@COM11
;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

IF TEQUIP:触手召喚
	PRINTL 触手挿入
	SAVESTR:0 = 触手挿入
ELSE
	PRINTL バイブ
	SAVESTR:0 = バイブ
ENDIF
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:体力 += 30
LOSEBASE:気力 += 100

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 80
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 250
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 600
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 1000
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 1300
ELSE
	SOURCE:1 = 1700
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5500
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 300
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 50
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 400
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:1 , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:1 , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:1 , 1.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:1 , 0.80
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:1 , 0.90
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:1 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:1 , 1.10
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:1 , 1.20
ELSE
	TIMES SOURCE:1 , 1.30
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 0.80
;小柄
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:6 , 4.00

						;****スライムパッチ追加ここから****
IF ITEM:魔術式バイブ >= 1 && TEQUIP:触手召喚 == 0
	TIMES LOSEBASE:体力 , 0.80
	TIMES LOSEBASE:気力 , 0.80
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:6 , 0.80
ENDIF
						;****スライムパッチ追加ここまで****
;-------------------------------------------------
;経験上昇
;-------------------------------------------------
S = 0
;Ｖ感覚が高いほどＶ経験が入る
IF ABL:Ｖ感覚 <= 1
	S = 1
ELSEIF ABL:Ｖ感覚 <= 4
	S = 2
ELSEIF ABL:Ｖ感覚 <= 7
	S = 3
ELSE
	S = 4
ENDIF
EXP:Ｖ経験 += S
PRINTFORML Ｖ経験+{S}
S = 0

;レズ経験
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +3
	EXP:レズ経験 += 3
ENDIF

;-------------------------------------------------
;バイブの着脱
;-------------------------------------------------
TEQUIP:バイブ = 1 - TEQUIP:バイブ
SIF TEQUIP:触手召喚
	T = 0

;-------------------------------------------------
;汚れ（触手）
;-------------------------------------------------
IF TEQUIP:触手召喚 && TEQUIP:バイブ
	STAIN:3 |= 2
	STAIN:3 |= 4
ENDIF

RETURN 1

;-------------------------------------------------
;-------------------------------------------------
;バイブ挿入中
;-------------------------------------------------
@EQUIP_COM11

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1

IF TEQUIP:触手召喚
	PRINTL ＜触手挿入中＞
ELSEIF TALENT:淫乱 == 1
	PRINTL ＜極太バイブ挿入中＞
ELSEIF TALENT:淫乱 == 0
	PRINTL ＜バイブ挿入中＞
ELSE
	PRINTL ＜バイブ挿入中＞
ENDIF

LOSEBASE:体力 += 10
LOSEBASE:気力 += 50

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	A = 40
ELSEIF ABL:Ｖ感覚 == 1
	A = 120
ELSEIF ABL:Ｖ感覚 == 2
	A = 300
ELSEIF ABL:Ｖ感覚 == 3
	A = 500
ELSEIF ABL:Ｖ感覚 == 4
	A = 650
ELSE
	A = 850
ENDIF

;EXP:Ｖ経験をみる
;処女はありえない
IF EXP:Ｖ経験 < EXPLV:2
	TIMES A , 0.60
	B = 150
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES A , 1.00
	B = 20
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES A , 1.20
	B = 0
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES A , 1.40
	B = 0
ELSE
	TIMES A , 1.60
	B = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES A , 0.10
	B += 400
	TIMES B , 3.00
	C += 1000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES A , 0.40
	B += 150
	TIMES B , 1.00
	C += 200
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES A , 1.00
	TIMES B , 0.50
	C += 80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES A , 1.40
	TIMES B , 0.20
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES A , 1.80
	TIMES B , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES A , 0.80
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES A , 0.90
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES A , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES A , 1.10
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES A , 1.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES A , 0.80
ELSEIF ABL:信頼 == 1
	TIMES A , 0.90
ELSEIF ABL:信頼 == 2
	TIMES A , 1.00
ELSEIF ABL:信頼 == 3
	TIMES A , 1.10
ELSEIF ABL:信頼 == 4
	TIMES A , 1.20
ELSE
	TIMES A , 1.30
ENDIF


;大柄
SIF TALENT:大柄
	TIMES B , 0.80
;小柄
SIF TALENT:小柄
	TIMES B , 2.00
;未熟
SIF TALENT:未熟
	TIMES B , 4.00

;貞操観念
IF TALENT:貞操観念
	;処女はありえない
	TIMES B, 3.00
ENDIF

SOURCE:1 += A
SOURCE:6 += B

						;****スライムパッチ追加ここから****
IF ITEM:魔術式バイブ >= 1 && TEQUIP:触手召喚 == 0
	LOSEBASE:体力 -= 2
	LOSEBASE:気力 -= 10
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:6 , 0.80
ENDIF
						;****スライムパッチ追加ここまで****
;-------------------------------------------------
;経験上昇
;-------------------------------------------------
S = 0
;自慰実行時は経験＋１
SIF SELECTCOM == 3
	S = 1
;Ｖ感覚が高いほどＶ経験が入る
IF ABL:Ｖ感覚 <= 1
	S += 1
ELSEIF ABL:Ｖ感覚 <= 4
	S += 2
ELSEIF ABL:Ｖ感覚 <= 7
	S += 3
ELSE
	S += 4
ENDIF
EXP:Ｖ経験 += S
PRINTFORML Ｖ経験+{S}
S = 0

;-------------------------------------------------
;その他の処理
;-------------------------------------------------
SIF TEQUIP:触手召喚
	T += 1

RETURN 1
