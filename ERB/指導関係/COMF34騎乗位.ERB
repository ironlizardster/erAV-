;-------------------------------------------------
;騎乗位
;奉仕系コマンド：調教対象が調教者の%CSTR:男性器呼び方%を%CSTR:女性器呼び方%で刺激
;-------------------------------------------------
@COM34
;前のプレイが騎乗位かつ調教者の技巧3以上の場合、騎乗Ｇスポ責めまたは騎乗子宮口責めへ派生
;Ｖ感覚が高く欲情しているほど子宮口へ行きやすい
IF FLAG:追加コマンドなし == 0
	IF PREVCOM == [[TRAIN:騎乗位]] && ABL:PLAYER:技巧 > 2 && ((ASSIPLAY && TFLAG:前回の調教者が助手) || (ASSIPLAY == 0 && TFLAG:前回の調教者が助手 == 0))
		;欲情
		IF PALAM:欲情 < PALAMLV:1
			A = 1
		ELSEIF PALAM:欲情 < PALAMLV:2
			A = 2
		ELSEIF PALAM:欲情 < PALAMLV:3
			A = 3
		ELSEIF PALAM:欲情 < PALAMLV:4
			A = 4
		ELSE
			A = 5
		ENDIF
		B = RAND:11
		A *= B
		B = RAND:11
		A += B*ABL:Ｖ感覚
		;Aの値は0～100
		;騎乗位だと50以上で子宮口
		IF A >= 50
			CALL COM_ABLE121
			SIF RESULT == 1
			JUMP COM121
		ELSE
			CALL COM_ABLE120
			SIF RESULT == 1
			JUMP COM120
		ENDIF
	ENDIF
ENDIF

PRINTL 騎乗位

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF S
		PRINT  + 
	A += ABL:欲望 * 3
	PRINTS ABLNAME:[[ABL:欲望]]
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
	S = 1
ENDIF
;ABL:Ｖ感覚
IF ABL:Ｖ感覚
	SIF S
		PRINT  + 
	A += ABL:Ｖ感覚 * 2
	PRINTS ABLNAME:[[ABL:Ｖ感覚]]
	PRINTV 'LV,ABL:Ｖ感覚,'(,ABL:Ｖ感覚 * 2,')
	S = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF S
		PRINT  + 
	A += ABL:奉仕精神 * 4
	PRINTS ABLNAME:[[ABL:奉仕精神]]
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	S = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF S
		PRINT  + 
	A += ABL:精液中毒 * 1
	PRINTS ABLNAME:[[ABL:精液中毒]]
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	S = 1
ENDIF

IF MARK:快楽刻印
	SIF S
		PRINT  + 
	A += MARK:快楽刻印 * 3
	PRINTS MARKNAME:[[MARK:快楽刻印]]
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 3,')
	S = 1
ENDIF

;PALAM:潤滑（の不足）
IF PALAM:潤滑 < PALAMLV:3
	PRINT  - 
	A -= 5
	PRINT 潤滑不足
	PRINTV '(,5,')
	S = 1
ENDIF
;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	L = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	L = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	L = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	L = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:[[PALAM:欲情]]
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;恥じらい
IF IS_恥じらい(TARGET)
	PRINT  - 
	A -= 2
	PRINTS GET_STR_恥じらい(TARGET)
	PRINTV '(,2,')
	S = 1
ENDIF
;快感の否定
IF IS_快感の否定(TARGET)
	PRINT  - 
	A -= 5
	PRINTS GET_STR_快感に素直(TARGET)
	PRINTV '(,5,')
	S = 1
ENDIF
;男嫌い
IF TALENT:男性不信 && IS_MALE(PLAYER) 
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:[[TALENT:男性不信]]
	PRINTV '(,12,')
	S = 1
ENDIF
;愛
IF TALENT:恋心 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:[[TALENT:恋心]]
	PRINTV '(,5,')
	S = 5
ENDIF

;セックス狂
IF TALENT:セックス狂
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:[[TALENT:セックス狂]]
	PRINTV '(,5,')
	S = 5
ENDIF

;処女
IF IS処女(TARGET)
	PRINT  - 
	A -= 20
	PRINTS TALENTNAME:[[IS処女(TARGET)]]
	PRINTV '(,20,')
	S = 1
ELSEIF EXP:Ｖ経験 < EXPLV:2
	PRINT  - 
	A -= 5
	PRINTS EXPNAME:[[EXP:Ｖ経験]]
	PRINT 不足
	PRINTV '(,5,')
	S = 1
ENDIF

;調教者がふたなり
IF IS_FUTA(PLAYER)
	SIF S
		PRINT  + 
	A += 4
	PRINT ふたなり
	PRINTV '(,4,')
	S = 1
ENDIF

;しあわせ草
IF TEQUIP:媚薬効果
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:[[ITEM:プリンセスポイズン]]
	PRINTV '(,6,')
	S = 1
ENDIF

;獣姦
IF TEQUIP:獣姦プレイ && TALENT:牝犬 == 0
	SIF S
		PRINT  - 
	A -= 20
	PRINTS ITEMNAME:[[ITEM:犬]]
	PRINTV '(,20,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:ペニス & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:ペニス & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:ペニス & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:ペニス & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:アナル & 32
	Y += 3

;獣姦の場合は汚れ7で固定
SIF TEQUIP:獣姦プレイ
	Y = 7

SIF IS_汚臭鈍感(TARGET)
	Y /= 3
SIF IS_汚臭敏感(TARGET)
	Y *= 2

;騎乗位では汚れの影響が少なめ
Y /= 3

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF IS_汚臭鈍感(TARGET)
		PRINT 汚れあり、
		PRINTS GET_STR_汚臭耐性(TARGET)
	ELSEIF IS_汚臭敏感(TARGET)
		PRINT 汚れあり、
		PRINTS GET_STR_汚臭耐性(TARGET)
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0


SAVESTR:0 = 騎乗位
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0
	TFLAG:主人による処女喪失 = 1

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOCAL =60
LOCAL:1 = 150

LOCAL:2 = ABL:Ｖ感覚 * 3
SIF LOCAL:2 <= 0
	LOCAL:2 = 1
LOCAL:3 = ABL:Ｖ感覚 * 2
SIF LOCAL:3 <= 0
	LOCAL:3 = 1

LOCAL -= LOCAL:2
SIF LOCAL <= 10
	LOCAL = 10
LOCAL:1 -= LOCAL:3
SIF LOCAL:1 <= 10
	LOCAL:1 = 10

LOSEBASE:体力 += LOCAL
LOSEBASE:気力 += LOCAL:1

SOURCE:露出 = 900

;上のほうで計算した汚れデータ
SOURCE:不潔 = Y*10 + 60

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 200
	SOURCE:達成 = 50
	SOURCE:逸脱 = 300
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 250
	SOURCE:達成 = 200
	SOURCE:逸脱 = 100
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 350
	SOURCE:達成 = 550
	SOURCE:逸脱 = 30
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 450
	SOURCE:達成 = 900
	SOURCE:逸脱 = 0
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 600
	SOURCE:達成 = 1500
	SOURCE:逸脱 = 0
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 750
	SOURCE:達成 = 2200
	SOURCE:逸脱 = 0
	TIMES SOURCE:不潔 , 0.10
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 20
	SOURCE:情愛 = 25
	TIMES SOURCE:性行動 , 0.50
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 75
	SOURCE:情愛 = 75
	TIMES SOURCE:性行動 , 0.80
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 200
	SOURCE:情愛 = 125
	TIMES SOURCE:性行動 , 1.00
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 500
	SOURCE:情愛 = 175
	TIMES SOURCE:性行動 , 1.20
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 850
	SOURCE:情愛 = 300
	TIMES SOURCE:性行動 , 1.50
ELSE
	SOURCE:快Ｖ = 1100
	SOURCE:情愛 = 425
	TIMES SOURCE:性行動 , 2.00
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛覚 = 5000
	SOURCE:露出 = 12000
	SOURCE:逸脱 = 120000
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.60
	SOURCE:痛覚 = 220
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
	SOURCE:痛覚 = 30
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
	SOURCE:痛覚 = 5
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.40
ELSE
	TIMES SOURCE:快Ｖ , 1.50
ENDIF


;PALAM:潤滑
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛覚 += 900
	TIMES SOURCE:痛覚 , 3.00
	SOURCE:露出 += 2000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.60
	SOURCE:痛覚 += 250
	TIMES SOURCE:痛覚 , 1.00
	SOURCE:露出 += 400
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:痛覚 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:痛覚 , 0.30
ELSEIF PALAM:潤滑 < PALAMLV:5
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:痛覚 , 0.20
ELSE
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:痛覚 , 0.10
ENDIF

;大柄
SIF IS_大柄(TARGET)
	TIMES SOURCE:痛覚 , 0.80
;小柄体型
SIF IS_小柄(TARGET)
	TIMES SOURCE:痛覚 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:痛覚 , 4.00

;貞操観念
IF IS_貞操観念(TARGET)
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:情愛 , 0.60
		TIMES SOURCE:逸脱, 15.00
		SOURCE:反感 = 10000
	ELSE
		TIMES SOURCE:情愛 , 0.60
		TIMES SOURCE:逸脱, 15.00
		SOURCE:反感 = 1000
	ENDIF
;貞操無頓着
ELSEIF IS_貞操無頓着(TARGET)
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:逸脱, 0.50
		SOURCE:反感 = 300
	ELSE
		TIMES SOURCE:逸脱, 0.50
	ENDIF
ELSE
	IF EXP:Ｖ経験 == 0
		SOURCE:反感 = 3000
	ENDIF
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:情愛 , 0.30
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:情愛 , 1.50
ELSEIF PALAM:欲情 < PALAMLV:5
	TIMES SOURCE:快Ｖ , 1.40
	TIMES SOURCE:情愛 , 1.80
ELSE
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:情愛 , 2.20
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:情愛 , 0.60
	TIMES SOURCE:反感 , 2.00
	SOURCE:屈従 += 700
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.80
	TIMES SOURCE:反感 , 1.50
	SOURCE:屈従 += 500
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:反感 , 1.00
	SOURCE:屈従 += 300
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:情愛 , 1.20
	TIMES SOURCE:反感 , 0.80
	SOURCE:屈従 += 100
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:情愛 , 1.40
	TIMES SOURCE:反感 , 0.60
	SOURCE:屈従 += 30
ELSE
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:情愛 , 1.60
	TIMES SOURCE:反感 , 0.30
	SOURCE:屈従 += 0
ENDIF

;ABL:技巧をみる
IF ABL:技巧 == 0
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:情愛 , 0.60
ELSEIF ABL:技巧 == 1
	TIMES SOURCE:快Ｖ , 0.80
	TIMES SOURCE:情愛 , 0.80
ELSEIF ABL:技巧 == 2
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:情愛 , 1.00
ELSEIF ABL:技巧 == 3
	TIMES SOURCE:快Ｖ , 1.30
	TIMES SOURCE:情愛 , 1.20
ELSEIF ABL:技巧 == 4
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:情愛 , 1.40
ELSE
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:情愛 , 1.60
ENDIF

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;射精している
IF E
	TIMES SOURCE:性行動 , 3.00
	TIMES SOURCE:達成 , 5.00
	TIMES SOURCE:屈従 , 1.50

	;ABL:精液中毒をみる
	IF ABL:精液中毒 == 0
		SOURCE:充足 = 0
	ELSEIF ABL:精液中毒 == 1
		SOURCE:充足 = 300
	ELSEIF ABL:精液中毒 == 2
		SOURCE:充足 = 1000
	ELSEIF ABL:精液中毒 == 3
		SOURCE:充足 = 1500
	ELSEIF ABL:精液中毒 == 4
		SOURCE:充足 = 2200
	ELSE
		SOURCE:充足 = 3000
	ENDIF
ENDIF

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

TFLAG:快楽経験 = 1

IF EXP:Ｖ経験 <= 1
	;屈服刻印３に相当
	TFLAG:屈服刻印2 = 3
ELSE
	;屈服刻印２に相当
	TFLAG:屈服刻印2 = 2
ENDIF

;調教者がふたなり
IF IS_FUTA(PLAYER)
	SOURCE:屈従 /= 2
ENDIF

RETURN 1
