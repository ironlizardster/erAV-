;-------------------------------------------------
;対面座位アナル
;セックス系コマンド：調教者の%CSTR:男性器呼び方%を調教対象のアナルに挿入
;-------------------------------------------------
@COM28
;Ａ処女を奪うかどうかの確認
CALL CONFIRM_LOST_ANALVIRGIN
SIF RESULT == 0
	RETURN 0


PRINTL 対面座位アナル
SAVESTR:0 = 対面座位アナル
CALL TRAIN_MESSAGE_B

;A経験を伴うコマンドのフラグ
TFLAG:Ａ挿入判定 = 1
;調教者の射精チェック
CALL COM_EJAC_PLAYER_ANALSEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOCAL = 60
LOCAL:1 = 120

LOCAL:2 = ABL:Ａ感覚 * 3
SIF LOCAL:2 <= 0
	LOCAL:2 = 1
LOCAL:3 = ABL:Ａ感覚 * 2
SIF LOCAL:3 <= 0
	LOCAL:3 = 1

LOCAL -= LOCAL:2
SIF LOCAL <= 10
	LOCAL = 10
LOCAL:1 -= LOCAL:3
SIF LOCAL:1 <= 10
	LOCAL:1 = 10

LOSEBASE:体力 += LOCAL
LOSEBASE:気力 += LOCAL:1

SOURCE:情愛 = 100
SOURCE:充足 = 100
SOURCE:露出 = 100

;ABL:Ａ感覚をみる
IF ABL:Ａ感覚 == 0
	SOURCE:快Ａ += 10
	SOURCE:情愛 += 10
	SOURCE:屈従 += 80
ELSEIF ABL:Ａ感覚 == 1
	SOURCE:快Ａ += 30
	SOURCE:情愛 += 130
	SOURCE:屈従 += 300
ELSEIF ABL:Ａ感覚 == 2
	SOURCE:快Ａ += 200
	SOURCE:情愛 += 500
	SOURCE:屈従 += 700
ELSEIF ABL:Ａ感覚 == 3
	SOURCE:快Ａ += 500
	SOURCE:情愛 += 1000
	SOURCE:屈従 += 1500
ELSEIF ABL:Ａ感覚 == 4
	SOURCE:快Ａ += 900
	SOURCE:情愛 += 1500
	SOURCE:屈従 += 2600
ELSE
	SOURCE:快Ａ += 1400
	SOURCE:情愛 += 2000
	SOURCE:屈従 += 4000
ENDIF

;EXP:Ａ経験をみる
IF EXP:Ａ経験 < EXPLV:1
	TIMES SOURCE:快Ａ , 0.10
	SOURCE:痛覚 = 18000
	LOSEBASE:体力 += 40
	LOSEBASE:気力 += 60
ELSEIF EXP:Ａ経験 < EXPLV:2
	TIMES SOURCE:快Ａ , 0.30
	SOURCE:痛覚 = 10000
	LOSEBASE:体力 += 30
	LOSEBASE:気力 += 50
ELSEIF EXP:Ａ経験 < EXPLV:3
	TIMES SOURCE:快Ａ , 0.50
	SOURCE:痛覚 = 4500
	LOSEBASE:体力 += 20
	LOSEBASE:気力 += 40
ELSEIF EXP:Ａ経験 < EXPLV:4
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛覚 = 1500
	LOSEBASE:体力 += 10
	LOSEBASE:気力 += 30
ELSEIF EXP:Ａ経験 < EXPLV:5
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛覚 = 700
	LOSEBASE:体力 += 0
	LOSEBASE:気力 += 20
ELSE
	TIMES SOURCE:快Ａ , 1.60
	SOURCE:痛覚 = 300
	LOSEBASE:体力 += 0
	LOSEBASE:気力 += 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.40
	SOURCE:痛覚 += 10000
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	SOURCE:痛覚 += 3600
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	SOURCE:痛覚 += 1200
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	SOURCE:痛覚 += 200
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.80
	SOURCE:痛覚 += 100
ENDIF

;ABL:奉仕精神をみる
IF ABL:奉仕精神 == 0
	SOURCE:性行動 = 50
	SOURCE:達成 = 10
	SOURCE:恭順 = 100
	TIMES SOURCE:不潔 , 4.00
ELSEIF ABL:奉仕精神 == 1
	SOURCE:性行動 = 150
	SOURCE:達成 = 50
	SOURCE:恭順 = 300
	TIMES SOURCE:不潔 , 2.50
ELSEIF ABL:奉仕精神 == 2
	SOURCE:性行動 = 300
	SOURCE:達成 = 100
	SOURCE:恭順 = 700
	TIMES SOURCE:不潔 , 1.50
ELSEIF ABL:奉仕精神 == 3
	SOURCE:性行動 = 400
	SOURCE:達成 = 200
	SOURCE:恭順 = 1200
	TIMES SOURCE:不潔 , 1.00
ELSEIF ABL:奉仕精神 == 4
	SOURCE:性行動 = 500
	SOURCE:達成 = 300
	SOURCE:恭順 = 1800
	TIMES SOURCE:不潔 , 0.50
ELSE
	SOURCE:性行動 = 800
	SOURCE:達成 = 500
	SOURCE:恭順 = 2500
	TIMES SOURCE:不潔 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF IS_FUTA(ASSI)
		TIMES SOURCE:快Ｖ , 2.50
ENDIF

;大柄
SIF IS_大柄(TARGET)
	TIMES SOURCE:痛覚 , 0.80
;小柄体型
SIF IS_小柄(TARGET)
	TIMES SOURCE:痛覚 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:痛覚 , 2.00

;貞操観念
IF IS_貞操観念(TARGET)
	IF EXP:Ａ経験 == 0
		TIMES SOURCE:情愛 , 1.20
		TIMES SOURCE:恭順 , 1.10
		SOURCE:反感 = 500
	ELSE
		TIMES SOURCE:情愛 , 1.20
		TIMES SOURCE:恭順 , 1.10
		SOURCE:反感 = 0
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ａ , 0.60
	TIMES SOURCE:屈従 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ａ , 0.80
	TIMES SOURCE:屈従 , 0.80
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ａ , 1.00
	TIMES SOURCE:屈従 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ａ , 1.20
	TIMES SOURCE:屈従 , 1.20
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ａ , 1.40
	TIMES SOURCE:屈従 , 1.40
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:情愛 , 0.60
	TIMES SOURCE:反感 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:情愛 , 0.80
	TIMES SOURCE:反感 , 1.50
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:情愛 , 1.00
	TIMES SOURCE:反感 , 1.00
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:情愛 , 1.20
	TIMES SOURCE:反感 , 0.80
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:情愛 , 1.40
	TIMES SOURCE:反感 , 0.60
ELSE
	TIMES SOURCE:情愛 , 1.60
	TIMES SOURCE:反感 , 0.30
ENDIF

;プレイヤーのABL:技巧をみる
IF ABL:PLAYER:技巧 == 0
	SOURCE:情愛 += 100
	SOURCE:愛液分泌 += 0
	A = 0
ELSEIF ABL:PLAYER:技巧 == 1
	SOURCE:情愛 += 150
	SOURCE:愛液分泌 += 50
	A = 0
ELSEIF ABL:PLAYER:技巧 == 2
	SOURCE:情愛 += 200
	SOURCE:愛液分泌 += 100
	A = 0
ELSEIF ABL:PLAYER:技巧 == 3
	SOURCE:情愛 += 300
	SOURCE:愛液分泌 += 150
	A = 50
ELSEIF ABL:PLAYER:技巧 == 4
	SOURCE:情愛 += 500
	SOURCE:愛液分泌 += 250
	A = 100
ELSE
	SOURCE:情愛 += 800
	SOURCE:愛液分泌 += 400
	A = 300
ENDIF

SOURCE:快Ｃ += A

;TALENT:愛をみる
IF TALENT:恋心
	TIMES SOURCE:情愛 , 3.00
	TIMES SOURCE:充足 , 2.00
	TIMES SOURCE:恭順 , 2.00
ENDIF

;アナルセックス後の処理
CALL COM_AFTER_ANAL_SEX

RETURN 1
