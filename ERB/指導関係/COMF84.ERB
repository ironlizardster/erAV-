@COM84
;Gスポット刺激

PRINTL Ｇスポット刺激
SAVESTR:0 = Ｇスポット刺激
CALL TRAIN_MESSAGE_B

LOSEBASE:体力 += 50
LOSEBASE:気力 += 180

SOURCE:3 = 300
SOURCE:7 = 200
SOURCE:10 = 500
SOURCE:12 = 300
SOURCE:14 = 400
SOURCE:15 = 50

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 10
	SOURCE:13 = 20
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 150
	SOURCE:13 = 120
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 600
	SOURCE:13 = 500
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 1800
	SOURCE:13 = 1200
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 2400
	SOURCE:13 = 1800
ELSE
	SOURCE:1 = 3200
	SOURCE:13 = 2400
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	TIMES SOURCE:13, 0.20
	SOURCE:6 = 150
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:13, 0.50
	SOURCE:6 = 80
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:13, 0.80
	SOURCE:6 = 80
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:13, 1.00
	SOURCE:6 = 0
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:13, 1.20
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:13, 1.50
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 2000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:1 , 0.20
	SOURCE:6 += 800
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:6 , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:6 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:7 , 0.90
	TIMES SOURCE:10 , 0.90
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:7 , 1.00
	TIMES SOURCE:10 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:1 , 2.10
	TIMES SOURCE:7 , 1.10
	TIMES SOURCE:10 , 1.10
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:1 , 3.20
	TIMES SOURCE:7 , 1.20
	TIMES SOURCE:10 , 1.20
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:1 , 4.30
	TIMES SOURCE:7 , 1.30
	TIMES SOURCE:10 , 1.30
ENDIF


;Ｖ敏感、鈍感をみる
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:Ｖ鈍感
	TIMES SOURCE:6 , 1.50
	TIMES SOURCE:13 , 1.50
	TIMES SOURCE:14 , 1.50
ELSEIF TALENT:Ｖ敏感
	TIMES SOURCE:6 , 0.60
	TIMES SOURCE:13 , 0.60
	TIMES SOURCE:14 , 0.60
ENDIF

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	TIMES SOURCE:13 , 2.00
ENDIF

EXP:Ｖ経験 += 1
PRINTL Ｖ経験＋１


IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +3
	EXP:レズ経験 += 3
ENDIF


RETURN 1


