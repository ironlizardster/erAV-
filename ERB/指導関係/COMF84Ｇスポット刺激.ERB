@COM84
;Gスポット刺激

PRINTL Ｇスポット刺激
SAVESTR:0 = Ｇスポット刺激
CALL TRAIN_MESSAGE_B

LOSEBASE:体力 += 50
LOSEBASE:気力 += 180

SOURCE:情愛 = 300
SOURCE:中毒充足 = 200
SOURCE:愛液分泌 = 500
SOURCE:露出 = 300
SOURCE:逸脱 = 400
SOURCE:反感 = 50

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:快Ｖ = 10
	SOURCE:屈従 = 20
ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:快Ｖ = 150
	SOURCE:屈従 = 120
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:快Ｖ = 600
	SOURCE:屈従 = 500
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:快Ｖ = 1800
	SOURCE:屈従 = 1200
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:快Ｖ = 2400
	SOURCE:屈従 = 1800
ELSE
	SOURCE:快Ｖ = 3200
	SOURCE:屈従 = 2400
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:快Ｖ , 0.20
	TIMES SOURCE:屈従, 0.20
	SOURCE:痛覚 = 150
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:快Ｖ , 0.50
	TIMES SOURCE:屈従, 0.50
	SOURCE:痛覚 = 80
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:屈従, 0.80
	SOURCE:痛覚 = 80
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:快Ｖ , 1.20
	TIMES SOURCE:屈従, 1.00
	SOURCE:痛覚 = 0
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:快Ｖ , 1.60
	TIMES SOURCE:屈従, 1.20
	SOURCE:痛覚 = 0
ELSE
	TIMES SOURCE:快Ｖ , 1.80
	TIMES SOURCE:屈従, 1.50
	SOURCE:痛覚 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 0.10
	SOURCE:痛覚 += 2000
	TIMES SOURCE:痛覚 , 3.00
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 0.20
	SOURCE:痛覚 += 800
	TIMES SOURCE:痛覚 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 0.60
	TIMES SOURCE:痛覚 , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 1.00
	TIMES SOURCE:痛覚 , 0.50
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 2.00
	TIMES SOURCE:痛覚 , 0.10
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:快Ｖ , 1.50
	TIMES SOURCE:中毒充足 , 0.90
	TIMES SOURCE:愛液分泌 , 0.90
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:快Ｖ , 1.80
	TIMES SOURCE:中毒充足 , 1.00
	TIMES SOURCE:愛液分泌 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:快Ｖ , 2.10
	TIMES SOURCE:中毒充足 , 1.10
	TIMES SOURCE:愛液分泌 , 1.10
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:快Ｖ , 3.20
	TIMES SOURCE:中毒充足 , 1.20
	TIMES SOURCE:愛液分泌 , 1.20
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:快Ｖ , 4.30
	TIMES SOURCE:中毒充足 , 1.30
	TIMES SOURCE:愛液分泌 , 1.30
ENDIF


;Ｖ敏感、鈍感をみる
;快Ｖ自体のチェックは後でまとめてやる
IF TALENT:Ｖ鈍感
	TIMES SOURCE:痛覚 , 1.50
	TIMES SOURCE:屈従 , 1.50
	TIMES SOURCE:逸脱 , 1.50
ELSEIF TALENT:Ｖ敏感
	TIMES SOURCE:痛覚 , 0.60
	TIMES SOURCE:屈従 , 0.60
	TIMES SOURCE:逸脱 , 0.60
ENDIF

;処女で貞操観念
IF EXP:Ｖ経験 == 0 && TALENT:貞操観念
	TIMES SOURCE:屈従 , 2.00
ENDIF

EXP:Ｖ経験 += 1
PRINTL Ｖ経験＋１


IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTS EXPNAME:[[EXP:レズ経験]]
	PRINTL +3
	EXP:レズ経験 += 3
ENDIF


RETURN 1


