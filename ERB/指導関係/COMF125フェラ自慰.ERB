	SIF S
		PRINT  - 
	A -= 15
	PRINTS ITEMNAME:[[ITEM:犬]]
	PRINTV '(,15,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:ペニス & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:ペニス & 4
	Y += 3
;アナルの汚れ
SIF STAIN:PLAYER:ペニス & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:ペニス & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:アナル & 32
	Y += 3

;獣姦の場合は汚れ7で固定
SIF TEQUIP:獣姦プレイ
	Y = 7

SIF IS_汚臭鈍感(TARGET)
	Y /= 3
SIF IS_汚臭敏感(TARGET)
	Y *= 2

;汚れあり
IF Y
	PRINT  - 
	A -= Y
	;汚臭鈍感
	IF IS_汚臭鈍感(TARGET)
		PRINT 汚れあり、
		PRINTS GET_STR_汚臭耐性(TARGET)
	ELSEIF IS_汚臭敏感(TARGET)
		PRINT 汚れあり、
		PRINTS GET_STR_汚臭耐性(TARGET)
	ELSE
		PRINT 汚れあり
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(50以上で実行)
PRINT  = 
PRINTV A

;難易度上昇
;公開で+10、バイブで+5、アナルバイブで+5、シャワーで＋3
V = 50
SIF TEQUIP:撮影中
	V += 10
SIF TEQUIP:シャワー
	V += 3
SIF TEQUIP:バイブ
	V += 5
SIF TEQUIP:アナルバイブ
	V += 5

SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 実行値
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

SAVESTR:0 = フェラ自慰
CALL TRAIN_MESSAGE_B

