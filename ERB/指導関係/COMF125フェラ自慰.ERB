	LOSEBASE:体力 += 20
	LOSEBASE:気力 += 70
ELSE
	LOSEBASE:体力 += 30
	LOSEBASE:気力 += 150
ENDIF

SOURCE:屈従 = 1500
SOURCE:逸脱 = 500

