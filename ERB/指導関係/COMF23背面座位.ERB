;-------------------------------------------------
;背面座位
;セックス系コマンド：調教者の%CSTR:男性器呼び方%を調教対象の%CSTR:女性器呼び方%に挿入
;正常位に比べて露出と情愛が低いが、調教対象の体力消耗が少ない
;-------------------------------------------------
@COM23
;設定が有効で、前のプレイが背面座位かつ調教者の技巧3以上の場合、挿入Ｇスポ責めまたは挿入子宮口責めへ派生
;Ｖ感覚が高く欲情しているほど子宮口へ行きやすい
IF FLAG:追加コマンドなし == 0
	IF PREVCOM == [[TRAIN:背面座位]] && ABL:PLAYER:技巧 > 2 && ((ASSIPLAY && TFLAG:前回の調教者が助手) || (ASSIPLAY == 0 && TFLAG:前回の調教者が助手 == 0))
		;欲情
		IF PALAM:欲情 < PALAMLV:1
			A = 1
		ELSEIF PALAM:欲情 < PALAMLV:2
			A = 2
		ELSEIF PALAM:欲情 < PALAMLV:3
			A = 3
		ELSEIF PALAM:欲情 < PALAMLV:4
			A = 4
		ELSE
			A = 5
		ENDIF
		B = RAND:11
		A *= B
		B = RAND:11
		A += B*ABL:Ｖ感覚
		;Aの値は0～100
		;背面座位だと60以上で子宮口
		IF A >= 60
			CALL COM_ABLE121
			SIF RESULT == 1
			JUMP COM121
		ELSE
			CALL COM_ABLE120
			SIF RESULT == 1
			JUMP COM120
		ENDIF
	ENDIF
ENDIF

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0


PRINTL 背面座位
SAVESTR:0 = 背面座位
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:V経験を伴うコマンド = 1
SIF TALENT:恋心 && ASSIPLAY == 0 && EXP:Ｖ経験 == 0
	TFLAG:主人による処女喪失 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOCAL = 20
LOCAL:1 = 150

LOCAL:2 = ABL:Ｖ感覚 * 3
SIF LOCAL:2 <= 0
	LOCAL:2 = 1
LOCAL:3 = ABL:Ｖ感覚 * 2
SIF LOCAL:3 <= 0
	LOCAL:3 = 1

LOCAL -= LOCAL:2
SIF LOCAL <= 10
	LOCAL = 10
LOCAL:1 -= LOCAL:3
SIF LOCAL:1 <= 10
	LOCAL:1 = 10

LOSEBASE:体力 += LOCAL
LOSEBASE:気力 += LOCAL:1

SOURCE:12 = 200

;ABL:Ｖ感覚をみる
IF ABL:Ｖ感覚 == 0
	SOURCE:1 = 50
	SOURCE:3 = 50

ELSEIF ABL:Ｖ感覚 == 1
	SOURCE:1 = 150
	SOURCE:3 = 100
ELSEIF ABL:Ｖ感覚 == 2
	SOURCE:1 = 300
	SOURCE:3 = 200
ELSEIF ABL:Ｖ感覚 == 3
	SOURCE:1 = 600
	SOURCE:3 = 300
ELSEIF ABL:Ｖ感覚 == 4
	SOURCE:1 = 1000
	SOURCE:3 = 500
ELSE
	SOURCE:1 = 1500
	SOURCE:3 = 700
ENDIF

;EXP:Ｖ経験をみる
IF EXP:Ｖ経験 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 3000

	IF ASSIPLAY && TALENT:PLAYER:オトコ == 0
		EXP:異常経験 += 1
		PRINTFORML %EXPNAME:[[EXP:異常経験]]%＋１
	ENDIF
ELSEIF EXP:Ｖ経験 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 240
ELSEIF EXP:Ｖ経験 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:Ｖ経験 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:Ｖ経験 < EXPLV:5
	TIMES SOURCE:1 , 1.40
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.60
	SOURCE:6 = 0
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:Ｂ感覚 == 0
	SOURCE:17 = 50
	SOURCE:3 += 50
ELSEIF ABL:Ｂ感覚 == 1
	SOURCE:17 = 200
	SOURCE:3 += 200
ELSEIF ABL:Ｂ感覚 == 2
	SOURCE:17 = 500
	SOURCE:3 += 400
ELSEIF ABL:Ｂ感覚 == 3
	SOURCE:17 = 800
	SOURCE:3 += 600
ELSEIF ABL:Ｂ感覚 == 4
	SOURCE:17 = 1300
	SOURCE:3 += 1000
ELSE
	SOURCE:17 = 1800
	SOURCE:3 += 1400
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:Ｃ感覚 == 0
	SOURCE:0 = 40
ELSEIF ABL:Ｃ感覚 == 1
	SOURCE:0 = 160
ELSEIF ABL:Ｃ感覚 == 2
	SOURCE:0 = 500
ELSEIF ABL:Ｃ感覚 == 3
	SOURCE:0 = 900
ELSEIF ABL:Ｃ感覚 == 4
	SOURCE:0 = 1400
ELSE
	SOURCE:0 = 2100
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 600
	TIMES SOURCE:6 , 2.60
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES SOURCE:1 , 0.70
	SOURCE:6 += 180
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:ふたなり
		TIMES SOURCE:1 , 2.50
ENDIF

;大柄
SIF TALENT:大柄
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:小柄
	TIMES SOURCE:6 , 2.00
;未熟
SIF TALENT:未熟
	TIMES SOURCE:6 , 4.00

;貞操観念
IF TALENT:貞操観念
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;貞操無頓着
ELSEIF TALENT:貞操無頓着
	IF EXP:Ｖ経験 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:Ｖ経験 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:欲情 < PALAMLV:1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:欲情 < PALAMLV:2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:欲情 < PALAMLV:3
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:0 , 1.10
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:欲情 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:3 , 2.00
ELSEIF PALAM:欲情 >= PALAMLV:4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:0 , 1.30
	TIMES SOURCE:3 , 2.60
ENDIF

;ABL:従順をみる
IF ABL:信頼 == 0
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:信頼 == 1
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.30
	TIMES SOURCE:15 , 1.80
ELSEIF ABL:信頼 == 2
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.50
	TIMES SOURCE:15 , 1.60
ELSEIF ABL:信頼 == 3
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:3 , 1.90
	TIMES SOURCE:15 , 1.40
ELSEIF ABL:信頼 == 4
	TIMES SOURCE:1 , 2.10
	TIMES SOURCE:3 , 2.20
	TIMES SOURCE:15 , 1.20
ELSE
	TIMES SOURCE:1 , 2.50
	TIMES SOURCE:3 , 2.60
	TIMES SOURCE:15 , 1.00
ENDIF

;プレイヤーのABL:技巧をみる
IF ABL:PLAYER:Ｖ感覚 == 0
	SOURCE:3 += 100
	SOURCE:10 = 0
	A = 0
ELSEIF ABL:PLAYER:Ｖ感覚 == 1
	SOURCE:3 += 150
	SOURCE:10 += 50
	A = 0
ELSEIF ABL:PLAYER:Ｖ感覚 == 2
	SOURCE:3 += 200
	SOURCE:10 += 100
	A = 0
ELSEIF ABL:PLAYER:Ｖ感覚 == 3
	SOURCE:3 += 300
	SOURCE:10 += 150
	A = 50
ELSEIF ABL:PLAYER:Ｖ感覚 == 4
	SOURCE:3 += 500
	SOURCE:10 += 250
	A = 100
ELSE
	SOURCE:3 += 800
	SOURCE:10 += 400
	A = 300
ENDIF

SOURCE:0 += A

SIF EXPLV:10 >= 3
	SOURCE:1 += A

;TALENT:愛をみる
SIF TALENT:恋心
	TIMES SOURCE:3 , 2.00

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
;
