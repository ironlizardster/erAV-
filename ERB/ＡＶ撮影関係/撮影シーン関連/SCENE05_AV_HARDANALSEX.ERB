;=================================================
; シーン経験増加処理
;--------------------------------------------
; シーンの経験増加処理を行います
;=================================================
@SCENE_EXP_5

VARSET LOCAL, 0

LOCAL = (1 + RAND:5)
LOCAL += ABL:性交中毒
SIF TALENT:尻穴狂い
	TIMES LOCAL , 1.25

TCVAR:Ａ経験 += LOCAL
TCVAR:性交経験 += LOCAL
TCVAR:フェラ経験 += LOCAL


IF P:4 != 0 && ABL:Ａ感覚 != 0
	LOCAL:1 += ABL:Ａ感覚
	SIF TALENT:尻穴狂い
		TIMES LOCAL , 1.50
	SIF ABL:Ｃ感覚 >= LOCAL
		LOCAL:1 = LOCAL
	TCVAR:絶頂経験 += LOCAL:1
ENDIF
