;HAVE_PENIS
;P持ちかどうかを見る
;-----------------------------------------------
;つまりありがちなあれ
;===============================================
@HAVE_PENIS(ARG)
#FUNCTION
RETURNF GETBIT(TALENT:ARG:性別, 1)

@HAVE_VAGINA(ARG)
#FUNCTION
RETURNF GETBIT(TALENT:ARG:性別, 0)

@IS_FUTA(ARG)
#FUNCTION
RETURNF TALENT:ARG:性別 == 性別_ふたなり

@IS_MALE(ARG)
#FUNCTION
RETURNF TALENT:ARG:性別 == 性別_オトコ

@IS_FEMALE(ARG)
#FUNCTION
RETURNF TALENT:ARG:性別 == 性別_女

@TOGGLE_V(ARG)
INVERTBIT TALENT:ARG:性別, 0

@TOGGLE_P(ARG)
INVERTBIT TALENT:ARG:性別, 1

@SET_SEXUALITY(ARG,ARG:1)
TALENT:ARG:性別 = ARG:1

@IS_Ｃ鈍感(ARG)
#FUNCTION
RETURNF TALENT:ARG:Ｃ感度 < 0

@IS_Ｃ敏感(ARG)
#FUNCTION
RETURNF TALENT:ARG:Ｃ感度 > 0

;===============================================
;GET_STR_
;数値で違う素質に変化するTALENTの正しい状態をCALLFORMFでRESULTSから取り出すためにRETURNFの前にセットする
;===============================================
@GET_STR_Ｃ感度(ARG)
#FUNCTIONS
RESULTS '= ""
SELECTCASE TALENT:ARG:Ｃ感度
	CASE IS < 0
		RESULTS '= "Ｃ鈍感"
	CASE IS > 0
		RESULTS '= "Ｃ敏感"
ENDSELECT
RETURNF RESULTS

