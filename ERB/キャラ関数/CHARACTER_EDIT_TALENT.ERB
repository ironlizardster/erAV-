#DIM 入力値
;ガード節
SIF CHARACTER_EDIT_IGNORE_TALENT(対象, 入力値) || TALENTNAME:入力値 == ""
	RETURN 0

TALENT:対象:入力値 = !TALENT:対象:入力値
SELECTCASE 入力値
	CASE [[TALENT:オトコ]]
		IF TALENT:対象:オトコ
			TALENT:対象:処女 = 0
			TALENT:対象:ギャル系 = 0
		ELSE
			TALENT:対象:オトコの娘 = 0
