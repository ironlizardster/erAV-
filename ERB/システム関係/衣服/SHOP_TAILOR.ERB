	PRINTFORML %NAME:TARGET%に新しい下着を買ってやりました。
	IF CFLAG:パンツ状態 >= 0 && CFLAG:パンツ継続日数 >= 6
		WAIT
		CALL SELLING_PANTS
	ENDIF

	;全裸だった場合は通常コスに普段着を代入しておく
	IF CFLAG:上着タイプ == 0

		;/////2012/09/22改造部分/////
		;挙動がバグにしか見えないのでコメントアウトしました。
		;CFLAG:上着タイプ = 1

		CFLAG:上着上状態 = -3
		CFLAG:上着下状態 = -3
	ENDIF

	CFLAG:パンツ状態 = 0
	CFLAG:ブラジャー状態 = 0

	;下着着衣フラグの初期化
	CALL WEARING_UNDERWARE_ALL

