	ELSE
		PRINTFORM %NAME:TARGET%に
	ENDIF

	CFLAG:特別コスタイプ = R
	CFLAG:特別コス状態 = 0

	CALL PRINT_CLOTHTYPE_SPECIAL
	PRINTFORML を装着させました。

	CFLAG:着衣状態 |= 64

ENDIF

MONEY -= C

WAIT

TARGET = -1

RESTART

