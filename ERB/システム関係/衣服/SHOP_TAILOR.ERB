ENDIF

$INPUT_LOOP_52

PRINTL □髪色一覧
PRINTFORML 所持金：{MONEY}ポイント
DRAWLINE

PRINTL  [ 0] - 黒色
PRINTL  [ 1] - 茶色
PRINTL  [ 2] - 金色
PRINTL  [ 3] - 銀色
PRINTL  [ 4] - 赤色
PRINTL  [ 5] - 青色
PRINTL  [ 6] - 緑色
PRINTL  [ 7] - ピンク
PRINTL  [ 8] - 紫色
PRINTL  [ 9] - 脱色する
PRINTL  [10] - 栗色
PRINTL  [99] - 地毛色に戻す
DRAWLINE
PRINTL  [999] - 戻る

INPUT

;今の髪色と同じ場合は染めない（地毛色→染色同色も不可（現実的に意味ないことはしない））
IF RESULT == CFLAG:髪色
	PRINTFORMW %CALLNAME:TARGET%の現在の髪色です
	GOTO INPUT_LOOP_52

;999は前の選択肢に戻る
ELSEIF RESULT == 999
	RETURN 1

;不正な値は弾く
ELSEIF RESULT != 99 && RESULT >= 11
	GOTO INPUT_LOOP_52

;地毛色に戻す以外の選択肢を選んだ場合の処理（肢０－１０）
ELSEIF RESULT != 99 && RESULT != CFLAG:髪色 
	PRINTFORM %CALLNAME:TARGET%は髪を
	IF RESULT == 0
		PRINTFORM 黒色に染め
	ELSEIF RESULT == 1
		PRINTFORM 茶色に染め
	ELSEIF RESULT == 2
		PRINTFORM 金色に染め
	ELSEIF RESULT == 3
		PRINTFORM 銀色に染め
	ELSEIF RESULT == 4
		PRINTFORM 赤色に染め
	ELSEIF RESULT == 5
		PRINTFORM 青色に染め
	ELSEIF RESULT == 6
		PRINTFORM 緑色に染め
	ELSEIF RESULT == 7
		PRINTFORM ピンクに染め
	ELSEIF RESULT == 8
		PRINTFORM 紫色に染め
	ELSEIF RESULT == 9
		PRINTFORM 脱色し
	ELSEIF RESULT == 10
		PRINTFORM 栗色に染め
	ENDIF
	PRINTFORMW てもらった
	CFLAG:TARGET:髪色 = RESULT
	CFLAG:TARGET:染髪済 = 1
	MONEY -= C	

ELSEIF RESULT == 99 && CFLAG:TARGET:染髪済 == 1

	IF MONEY < 1500
		PRINTW お金が足りません
		GOTO INPUT_LOOP_52
	ENDIF

	;地毛色の値がある→0でないとき
	;未入力の時はCSVCFLAGを取ってこれる。未指定なら0のはず
	IF CFLAG:TARGET:地毛色 
		PRINTFORMW %CALLNAME:TARGET%は髪の色を地毛に戻した。
		CFLAG:TARGET:髪色 = CFLAG:TARGET:地毛色
	;初期設定で髪を染めているキャラは、地毛を黒髪とみなします。
	;CFLAG:603をCSVで指定することでみなしを免れます
	ELSEIF CSVCFLAG(NO:TARGET,601) == 1
		PRINTFORMW %CALLNAME:TARGET%は髪の色を地毛に戻した。
		CFLAG:TARGET:髪色 = 0
	ELSE
		PRINTFORMW %CALLNAME:TARGET%は髪の色を地毛に戻した。
		CFLAG:TARGET:髪色 = CSVCFLAG(NO:TARGET,600)
	ENDIF
	;染髪なしにする。
	CFLAG:TARGET:染髪済 = 0
	MONEY -= 1500

ELSEIF RESULT == 99 && CFLAG:TARGET:染髪済 == 0
	PRINTFORMW %CALLNAME:TARGET%は髪の毛を染めていません。
	GOTO INPUT_LOOP_52
ELSE	
	GOTO INPUT_LOOP_52
ENDIF
RETURN 1
