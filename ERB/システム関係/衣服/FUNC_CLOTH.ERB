;=================================================
;衣装関連の関数群
;=================================================
;-------------------------------------------------
;使用一時フラグ
;TFLAG:下着着衣下が穿けない状態になっている = パンツ状態(&1:尿まみれ &2:精液まみれ &64:調教後に処分)
;TFLAG:46 = ブラジャー状態(&1:尿まみれ &2:精液まみれ &64:調教後に処分)
;TFLAG:上着上汚れ = 上着上状態(&1:尿まみれ &2:精液まみれ &64:調教後に処分)
;TFLAG:上着下汚れ = 上着下状態(&1:尿まみれ &2:精液まみれ &64:調教後に処分)
;TFLAG:特別コスチューム汚れ = 特別コスの状態(&1:尿まみれ &2:精液まみれ &64:調教後に処分)
;-------------------------------------------------
;使用キャラフラグ
;CFLAG;40 = 着衣状態（&1:パンツ &2:ブラジャー &4:上着上 &8:上着下・スカートタイプ &:16上着下・ズボンタイプ &64:特別コス）
;CFLAG:上着タイプ = 上着タイプ
;CFLAG:特別コスタイプ = 特別コスタイプ
;CFLAG:パンツ状態 = パンツ状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:ブラジャー状態 = ブラジャー状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:上着上状態 = 上着上状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:上着下状態 = 上着下状態（-2:廃棄 -1:没収 0:通常 1以上:洗濯中）
;CFLAG:特別コス状態 = 特別コス状態（-1:没収 0:通常 1以上:洗濯中）
;-------------------------------------------------
;よく参照するシチュ早見表
;CFLAG:着衣状態 & 3  == False;:何も下着を着けていない True:何か下着を付けている
;CFLAG:着衣状態 & 6  == False:上半身裸 True:上半身に何か着ている
;CFLAG:着衣状態 & 17 == False:性器を直に触れる状態 True:性器を触れない状態
;CFLAG:着衣状態 & 24 == False:上着の下部分を付けていない True:上着の下部分を付けている
;CFLAG:着衣状態 & 25 == False:下半身裸 True:下半身に何か着ている
;CFLAG:着衣状態 & 28 == False:上着を上も下も一切着ていない True:何か上着を着ている
;=================================================
;-------------------------------------------------
;使用中の着衣表示
;-------------------------------------------------
@PRINT_CLOTHTYPE
;着衣設定を使ってない場合
IF (CFLAG:上着タイプ == 0 && CFLAG:特別コスタイプ == 0) || FLAG:着衣使用 == 0
	PRINT 全裸
	RETURN 1
ENDIF

;特別コス：ズーコの着ぐるみの場合の処理
IF CFLAG:特別コスタイプ == 11 && (CFLAG:着衣状態 & 64)
	PRINT ズーコの着ぐるみ
	RETURN 1
ENDIF

;基本コスチューム処理
CALL PRINT_CLOTHTYPE_MAIN

;特別コスチューム処理
IF CFLAG:特別コスタイプ && (CFLAG:着衣状態 & 64)
	PRINT と
	CALL PRINT_CLOTHTYPE_SPECIAL
	PRINT 
ENDIF

RETURN 1

;基本コスチューム
@PRINT_CLOTHTYPE_MAIN
;ふんどしの場合
IF CFLAG:上着タイプ == 192 && (CFLAG:着衣状態 & 16)
	PRINT ふんどし一丁
	RETURN 1
;体操着とブルマで半脱ぎの場合の処理
ELSEIF CFLAG:上着タイプ == 109
	IF (CFLAG:着衣状態 & 4) && (CFLAG:着衣状態 & 24) == 0
		PRINT 体操着の上だけで
		IF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 0
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 1
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 0
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1 
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:オトコ == 1 
			PRINTFORM パンツ丸見え
		ELSE
			PRINT 下半身裸
		ENDIF
		RETURN 1
	ELSEIF (CFLAG:着衣状態 & 4) == 0 && (CFLAG:着衣状態 & 24)
		IF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 0 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT ブルマとブラジャー
		ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT ブルマとブラジャー
		ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT ブルマとサラシ
		ELSE
			PRINT ブルマ一枚
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;全身タイプで半脱ぎの場合の処理
IF CFLAG:上着タイプ >= 201 && CFLAG:上着タイプ <= 300
	IF (CFLAG:着衣状態 & 4) && (CFLAG:着衣状態 & 24) == 0
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT の下が破れて、
		IF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 0
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 1
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 0
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1
			PRINTFORM パンツ丸見え
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1
			PRINTFORM パンツ丸見え
		ELSE
			PRINT 下半身裸
		ENDIF
		RETURN 1
	ELSEIF (CFLAG:着衣状態 & 4) == 0 && (CFLAG:着衣状態 & 24)
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT の胸元が裂けて、
		IF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 0 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT 上はブラジャーだけ
		ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT 上はブラジャーだけ
		ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT 上はサラシだけ
		ELSE
			IF TALENT:オトコ == 0 && TALENT:絶壁 == 0 && (TALENT:貧乳 == 0 || TALENT:幼稚 == 0)
				PRINT 乳房むきだし
			ELSE
				PRINT 上半身裸
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
;通常の衣服
IF (CFLAG:着衣状態 & 28)
	IF (CFLAG:着衣状態 & 4) && (CFLAG:着衣状態 & 24) == 0
		CALL PRINT_CLOTHTYPE_MAIN2
		PRINT 上だけ
		IF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 0
			PRINTFORM パンツ一枚
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 1
			PRINTFORM パンツ一枚
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 0
			PRINTFORM パンツ一枚
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1
			PRINTFORM パンツ一枚
		ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1
			PRINTFORM パンツ一枚
		ELSE
			PRINT 下半身裸
		ENDIF
	ELSEIF (CFLAG:着衣状態 & 4) == 0 && (CFLAG:着衣状態 & 24)
		IF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 0 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT ブラジャーに
		ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && NO:TARGET != [[キャラ:桐生歌音]]
			PRINT ブラジャーに
		ELSEIF (CFLAG:着衣状態 & 2) && NO:TARGET == [[キャラ:桐生歌音]]
			PRINT ブラジャーに
		ELSE
			IF TALENT:オトコ == 0 && TALENT:絶壁 == 0 && (TALENT:貧乳 == 0 || TALENT:幼稚 == 0)
				PRINT 乳房むきだしに
			ELSE
				PRINT 上半身裸に
			ENDIF
		ENDIF
		CALL PRINT_CLOTHTYPE_MAIN2
		IF CFLAG:上着タイプ >= 1 && CFLAG:上着タイプ <= 100
			PRINT のスカート
		ELSE
			PRINT 下だけ
		ENDIF
	ELSE
		CALL PRINT_CLOTHTYPE_MAIN2
		SIF (CFLAG:着衣状態 & 64) == 0
			PRINT 
	ENDIF
ELSEIF (CFLAG:着衣状態 & 1) && (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 0
	PRINT 下着
	SIF (CFLAG:着衣状態 & 64) == 0
		PRINT 
ELSEIF (CFLAG:着衣状態 & 1) && (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && TALENT:オトコ == 0
	PRINT 下着
	SIF (CFLAG:着衣状態 & 64) == 0
		PRINT 
ELSEIF (CFLAG:着衣状態 & 1) && (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && TALENT:オトコ == 1
	PRINT 黒いブーメランパンツ
	SIF (CFLAG:着衣状態 & 64) == 0
		PRINT 
ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 0
	PRINTFORM パンツ一枚
ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 0 && TALENT:ふたなり == 1
	PRINTFORM パンツ一枚
ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 0
	PRINTFORM パンツ一枚
ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1
	PRINTFORM パンツ一枚
ELSEIF (CFLAG:着衣状態 & 1) && TALENT:淫乱 == 1 && TALENT:ふたなり == 1
	PRINTFORM パンツ一枚
ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 0 && NO:TARGET != [[キャラ:桐生歌音]]
	PRINT ブラ一枚で下半身丸出し
ELSEIF (CFLAG:着衣状態 & 2) && TALENT:淫乱 == 1 && NO:TARGET != [[キャラ:桐生歌音]]
	PRINT ブラ一枚で下半身丸出し
ELSEIF (CFLAG:着衣状態 & 2) &&  NO:TARGET == [[キャラ:桐生歌音]]
	PRINT サラシのみで下半身丸出し
ELSE
	PRINT 全裸
ENDIF

RETURN 1

;-------------------------------------------------
;着用可能な衣装の全装着
;-------------------------------------------------
@WEARING_CLOTH_ABLE

CALL WEARING_CLOTH_ALL

SIF CFLAG:パンツ状態 != 0
	CFLAG:着衣状態 -= (CFLAG:着衣状態 & 1)
SIF CFLAG:ブラジャー状態 != 0
	CFLAG:着衣状態 -= (CFLAG:着衣状態 & 2)
SIF CFLAG:上着上状態 != 0
	CFLAG:着衣状態 -= (CFLAG:着衣状態 & 4)
SIF CFLAG:上着下状態 != 0
	CFLAG:着衣状態 -= (CFLAG:着衣状態 & 8)
SIF CFLAG:上着下状態 != 0
	CFLAG:着衣状態 -= (CFLAG:着衣状態 & 16)
SIF CFLAG:特別コス状態 != 0
	CFLAG:着衣状態 -= (CFLAG:着衣状態 & 64)

;靴下を履く
SIF CFLAG:靴下タイプ != 0
	CFLAG:靴下着用 = 0

;-------------------------------------------------
;着衣フラグの初期化
;-------------------------------------------------
@WEARING_CLOTH_ALL
;標準衣装が設定されてない場合は戻る
SIF CFLAG:上着タイプ == 0 && CFLAG:特別コスタイプ == 0
	RETURN 0

;一旦全裸に
CFLAG:着衣状態 = 0

;全裸以外の場合の標準コス処理
IF CFLAG:上着タイプ != 0

	;下着着衣フラグの初期化
	CALL WEARING_UNDERWARE_ALL

	;上着着衣フラグの初期化
	CALL WEARING_OUTERWARE_ALL

ENDIF

;特別コスの装着
SIF CFLAG:特別コスタイプ
	CFLAG:着衣状態 |= 64

RETURN 1

;-------------------------------------------------
;下着着衣フラグの初期化
;-------------------------------------------------
@WEARING_UNDERWARE_ALL

;パンツ装着
CFLAG:着衣状態 |= 1

;絶壁、未熟、幼稚＋貧乳の場合を除きブラ装着
SIF TALENT:絶壁 == 0 && TALENT:未熟 == 0 && (TALENT:幼稚 == 0 || TALENT:貧乳 == 0)
	CFLAG:着衣状態 |= 2
;和服・バニースーツ用ノーブラ化処理
SIF CFLAG:上着タイプ == 202 || CFLAG:上着タイプ == 254
	CFLAG:着衣状態 -= CFLAG:着衣状態 & 2
;全裸の上にまとうタイプ(191～200or241～250or291～300)ならノーブラ・ノーパン化
SIF CFLAG:上着タイプ >= 191 && CFLAG:上着タイプ <= 200
	CFLAG:着衣状態 -= CFLAG:着衣状態 & 3
SIF CFLAG:上着タイプ >= 241 && CFLAG:上着タイプ <= 250
	CFLAG:着衣状態 -= CFLAG:着衣状態 & 3
SIF CFLAG:上着タイプ >= 291 && CFLAG:上着タイプ <= 300
	CFLAG:着衣状態 -= CFLAG:着衣状態 & 3
;島の娘の服用ノーブラ・ノーパン処理
SIF CFLAG:上着タイプ == 29
	CFLAG:着衣状態 -= CFLAG:着衣状態 & 3
;オムツ着用時のノーパン処理
SIF CFLAG:特別コスタイプ == 69
	CFLAG:着衣状態 -= CFLAG:着衣状態 & 1

;-------------------------------------------------
;上着着衣フラグの初期化
;-------------------------------------------------
@WEARING_OUTERWARE_ALL

;下がスカートタイプのツーピース
IF CFLAG:上着タイプ >= 1 && CFLAG:上着タイプ <= 100
	CFLAG:着衣状態 |= 4
	CFLAG:着衣状態 |= 8
;下がズボンタイプのツーピース
ELSEIF CFLAG:上着タイプ >= 101 && CFLAG:上着タイプ <= 200
	CFLAG:着衣状態 |= 4
	CFLAG:着衣状態 |= 16
;下がスカートタイプの全身衣装
ELSEIF CFLAG:上着タイプ >= 201 && CFLAG:上着タイプ <= 250
	CFLAG:着衣状態 |= 4
	CFLAG:着衣状態 |= 8
;下がズボンタイプの全身衣装
ELSEIF CFLAG:上着タイプ >= 251 && CFLAG:上着タイプ <= 300
	CFLAG:着衣状態 |= 4
	CFLAG:着衣状態 |= 16
ENDIF

;ふんどし
SIF CFLAG:上着タイプ == 192
	CFLAG:着衣状態 = 16

;-------------------------------------------------
;調教後の衣類の処理
;-------------------------------------------------
@AFTERTRAIN_CLOTH
;汚れた衣類の始末
S = 0
;特別コス
IF CFLAG:特別コスタイプ && (TFLAG:特別コスチューム汚れ & 64)
	PRINTFORM （%NAME:TARGET%の
	CALL PRINT_CLOTHTYPE_SPECIAL
	PRINTL は廃棄処分にされました）
	CFLAG:特別コスタイプ = 0
	TFLAG:特別コスチューム汚れ = 0
	S = 1
	SIF CFLAG:着衣状態 & 64
		CFLAG:着衣状態 -= 64
ELSEIF CFLAG:特別コスタイプ == 69 && (TFLAG:特別コスチューム汚れ & 3) && CFLAG:特別コス状態 == 0 && MONEY >= 50
	;オムツの場合の特殊処理
	$INPUT_LOOP_01
	PRINTFORML 50ドラクマで%CALLNAME:TARGET%のオムツを交換してやりますか？
	PRINTL  [0] - はい
	PRINTL  [1] - いいえ
	INPUT
	IF RESULT == 0
		PRINTFORM （%CALLNAME:TARGET%に新しいオムツをはかせてやりました）
		MONEY -= 50
		CFLAG:特別コス状態 = 0
		TFLAG:特別コスチューム汚れ = 0
		IF TALENT:未熟 == 0
			PRINTL 
			PRINTFORM %PALAMNAME:[[PALAM:恥情]]%の珠＋500
			JUEL:恥情 += 500
		ENDIF
		WAIT
		S = 1
	ELSEIF RESULT == 1
		PRINTFORMW （%NAME:TARGET%のオムツを洗濯に出しました）
		CFLAG:特別コス状態 = 2
		TFLAG:特別コスチューム汚れ = 0
		S = 1
		SIF CFLAG:着衣状態 & 64
			CFLAG:着衣状態 -= 64
	ELSE
		GOTO INPUT_LOOP_01
	ENDIF
ELSEIF CFLAG:特別コスタイプ && (TFLAG:特別コスチューム汚れ & 3) && CFLAG:特別コス状態 == 0
	PRINTFORM （%NAME:TARGET%の
	CALL PRINT_CLOTHTYPE_SPECIAL
	PRINTW を洗濯に出しました）
	CFLAG:特別コス状態 = 5
	;オムツの場合は洗濯速度は下着並
	SIF CFLAG:特別コスタイプ == 69
		CFLAG:特別コス状態 = 2
	TFLAG:特別コスチューム汚れ = 0
	S = 1
	SIF CFLAG:着衣状態 & 64
		CFLAG:着衣状態 -= 64
ENDIF
;上着下
IF CFLAG:上着タイプ && (TFLAG:上着下汚れ & 64)
	PRINTFORM （%NAME:TARGET%が着ていた
	CALL PRINT_CLOTHTYPE_MAIN_BOTTOM
	PRINTW は廃棄処分にされました）
	IF CFLAG:上着タイプ >= 201
		CFLAG:上着タイプ = 0
		SIF CFLAG:着衣状態 & 4
			CFLAG:着衣状態 -= 4
		SIF CFLAG:着衣状態 & 8
			CFLAG:着衣状態 -= 8
		SIF CFLAG:着衣状態 & 16
			CFLAG:着衣状態 -= 16
	ELSE
		CFLAG:上着下状態 = -2
		SIF CFLAG:着衣状態 & 8
			CFLAG:着衣状態 -= 8
		SIF CFLAG:着衣状態 & 16
			CFLAG:着衣状態 -= 16
	ENDIF
	TFLAG:上着下汚れ = 0
	S = 1
ELSEIF CFLAG:上着タイプ && (TFLAG:上着下汚れ & 3) && CFLAG:上着下状態 == 0
	PRINTFORM （%NAME:TARGET%が着ていた
	CALL PRINT_CLOTHTYPE_MAIN_BOTTOM
	PRINTW を洗濯に出しました）
	IF CFLAG:上着タイプ >= 201
		CFLAG:上着上状態 = 3
		CFLAG:上着下状態 = 3
		SIF CFLAG:着衣状態 & 4
			CFLAG:着衣状態 -= 4
		SIF CFLAG:着衣状態 & 8
			CFLAG:着衣状態 -= 8
		SIF CFLAG:着衣状態 & 16
			CFLAG:着衣状態 -= 16
	ELSE
		CFLAG:上着下状態 = 3
		SIF CFLAG:着衣状態 & 8
			CFLAG:着衣状態 -= 8
		SIF CFLAG:着衣状態 & 16
			CFLAG:着衣状態 -= 16
	ENDIF
	TFLAG:上着下汚れ = 0
	S = 1
ENDIF
;パンツ
IF (TFLAG:下着着衣下が穿けない状態になっている & 64)
	PRINTFORMW （%NAME:TARGET%の下着は廃棄処分にされました）
	CFLAG:パンツ状態 = -2
	SIF CFLAG:着衣状態 & 1
		CFLAG:着衣状態 -= 1
	TFLAG:下着着衣下が穿けない状態になっている = 0
	S = 1
ELSEIF (TFLAG:下着着衣下が穿けない状態になっている & 3) && CFLAG:パンツ状態 == 0
	PRINTFORMW （%NAME:TARGET%の下着を洗濯に出しました）
	CFLAG:パンツ状態 = 2
	SIF CFLAG:着衣状態 & 1
		CFLAG:着衣状態 -= 1
	TFLAG:下着着衣下が穿けない状態になっている = 0
	S = 1
ENDIF

;上下ともダメになった衣装は削除
IF CFLAG:上着タイプ
	SIF CFLAG:上着上状態 < 0 && CFLAG:上着下状態 < 0
		CFLAG:上着タイプ = 0
	;ふんどし用処理
	SIF CFLAG:上着タイプ == 192 && CFLAG:上着下状態 < 0
		CFLAG:上着タイプ = 0
ENDIF
;ダメになった特別コスは削除
IF CFLAG:特別コスタイプ
	SIF CFLAG:特別コス状態 < 0
		CFLAG:特別コスタイプ = 0
ENDIF

RETURN 1

;-------------------------------------------------
;衣類の再着衣
;-------------------------------------------------
@RE_CLOTHED
;従順＋露出癖3以上なら、主人に脱がされた状態を維持
;それ以外なら、貞操帯を除いて着られる衣類は全部身に着ける
IF ABL:信頼 + ABL:露出癖 < 3
	C = CFLAG:着衣状態
	;特別コスが貞操帯で外した状態かチェック
	D = 0
	SIF CFLAG:特別コスタイプ == 79 && (CFLAG:着衣状態 & 64) == 0
		D = 1
	CALL WEARING_CLOTH_ABLE
	SIF D
		CFLAG:着衣状態 -= CFLAG:着衣状態 & 64
	IF CFLAG:着衣状態 > C
		PRINTFORML （%CALLNAME:TARGET%は、脱がされていた衣装を再び身に付けました）
		WAIT
	ENDIF
ENDIF

RETURN 1

;-------------------------------------------------
;汚れた衣類の洗濯
;-------------------------------------------------
@WASHING_CLOTH
S = 0
REPEAT CHARANUM
	TARGET = COUNT
	;全身衣装の場合、洗濯の進行状況は下半身部分にあわせる
	SIF CFLAG:上着タイプ >= 200 && CFLAG:上着上状態 > 0 && CFLAG:上着下状態 > 0
		CFLAG:上着上状態 = CFLAG:上着下状態
	;下着の洗濯
	IF CFLAG:パンツ状態 > 0
		CFLAG:パンツ状態 -= 1
		IF CFLAG:パンツ状態 == 0
			PRINTFORML （%NAME:TARGET%の下着が洗濯から戻ってきました）
			S = 1
		ENDIF
	ENDIF
	;通常コスの洗濯
	IF CFLAG:上着下状態 > 0
		CFLAG:上着下状態 -= 1
		IF CFLAG:上着下状態 == 0 && CFLAG:上着タイプ
			PRINTFORM （%NAME:TARGET%の
			CALL PRINT_CLOTHTYPE_MAIN_BOTTOM
			PRINTFORML が洗濯から戻ってきました）
			S = 1
			SIF CFLAG:上着タイプ >= 200 && CFLAG:上着上状態 >= 0
				CFLAG:上着上状態 = 0
		ENDIF
	ENDIF
	;特別コスの洗濯
	IF CFLAG:特別コス状態 > 0
		CFLAG:特別コス状態 -= 1
		IF CFLAG:特別コス状態 == 0 && CFLAG:特別コスタイプ
			PRINTFORM （%NAME:TARGET%の
			CALL PRINT_CLOTHTYPE_SPECIAL
			PRINTFORML が洗濯から戻ってきました）
			S = 1
		ENDIF
	ENDIF

	;パンツのはき古し進行もここで
	SIF (CFLAG:着衣状態 & 1)
		CFLAG:パンツ継続日数 += 1

REND
IF S
	WAIT
	DRAWLINE
ENDIF

RETURN 1

