ELSEIF CFLAG:靴タイプ == 3 && CFLAG:靴下タイプ != 99
	PRINTFORM パンプス
ELSEIF CFLAG:靴タイプ == 4 && CFLAG:靴下タイプ != 99
	PRINTFORM ハイヒール
ELSEIF CFLAG:靴タイプ == 5 && CFLAG:靴下タイプ != 99
	PRINTFORM サンダル
ELSEIF CFLAG:靴タイプ == 6 && CFLAG:靴下タイプ != 99
	PRINTFORM ビーチサンダル
ELSEIF CFLAG:靴タイプ == 7 && CFLAG:靴下タイプ != 99
	PRINTFORM ナースサンダル
ELSEIF CFLAG:靴タイプ == 8 && CFLAG:靴下タイプ != 99
	PRINTFORM パールピンクミュール
ELSEIF CFLAG:靴タイプ == 9 && CFLAG:靴下タイプ != 99
	PRINTFORM 下駄
ELSEIF CFLAG:靴タイプ == 10 && CFLAG:靴下タイプ != 99
	PRINTFORM もこもこスリッパ
ELSEIF CFLAG:靴タイプ == 11 && CFLAG:靴下タイプ != 99
	PRINTFORM なぜか便所スリッパ
ELSEIF CFLAG:靴タイプ == 12 && CFLAG:靴下タイプ != 99
	PRINTFORM 編み上げブーツ
ELSEIF CFLAG:靴タイプ == 13 && CFLAG:靴下タイプ != 99
	PRINTFORM カウボーイブーツ
ELSEIF CFLAG:靴タイプ == 14 && CFLAG:靴下タイプ != 99
	PRINTFORM ピンヒールニーハイブーツ
ELSEIF CFLAG:靴タイプ == 15 && CFLAG:靴下タイプ != 99
	PRINTFORM クロックス
ENDIF

IF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 == 0
	PRINTFORM 
ELSEIF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 != 0
	PRINTFORM 
ELSE
	PRINTFORM を履いた
ENDIF
