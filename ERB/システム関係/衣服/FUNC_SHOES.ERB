@PRINT_SHOESTYPE
IF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ != 0
	PRINTFORM 素足
ELSEIF CFLAG:靴下タイプ == 1
	PRINTFORM ショートソックス
ELSEIF CFLAG:靴下タイプ == 2
	PRINTFORM 学校指定ソックス
ELSEIF CFLAG:靴下タイプ == 3
	PRINTFORM ハイソックス
ELSEIF CFLAG:靴下タイプ == 4
	PRINTFORM ルーズソックス
ELSEIF CFLAG:靴下タイプ == 5
	PRINTFORM ニーソックス
ELSEIF CFLAG:靴下タイプ == 6
	PRINTFORM 黒ストッキング
ELSEIF CFLAG:靴下タイプ == 7
	PRINTFORM ガーターストッキング
ELSEIF CFLAG:靴下タイプ == 8
	PRINTFORM ニーハイネットストッキング
ELSEIF CFLAG:靴下タイプ == 9
	PRINTFORM 白ストッキング
ELSEIF CFLAG:靴下タイプ == 99
	PRINTFORM ガラスのニーソックス
ELSE
	PRINTFORM
ENDIF

IF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 == 0
	PRINTFORM
ELSEIF CFLAG:靴下タイプ != 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 == 0
	PRINTFORM
ELSEIF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 != 0
	PRINTFORM
ELSEIF CFLAG:靴下タイプ == 99 && CFLAG:着衣状態 != 0
	PRINTFORM
ELSEIF CFLAG:靴下タイプ != 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 != 0
	PRINTFORM
ELSE
	PRINTFORM に
ENDIF

IF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 == 0
	PRINTFORM 
ELSEIF CFLAG:靴下タイプ == 0 && CFLAG:靴タイプ == 0 && CFLAG:着衣状態 != 0
	PRINTFORM 裸足
ELSEIF CFLAG:靴タイプ == 1 && CFLAG:靴下タイプ != 99
