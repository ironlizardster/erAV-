I = COUNT
REPEAT 800
	IF T:COUNT != 100
		S *= T:COUNT
		S /= 100
	ENDIF
REND
COUNT = I

;主人・助手の中の商売人をチェック（本人が売却対象の場合は外す）
O:1 = 100
M = 0
I = COUNT
REPEAT CHARANUM
	IF TALENT:COUNT:交渉上手 && ISASSI:COUNT && ABL:COUNT:話術 && COUNT != TARGET
		SIF ABL:COUNT:話術 > ABL:M:話術
			M = COUNT
	ENDIF
REND
COUNT = I
SIF M
	O:1 = 100 + ABL:M:話術 * 2
S *= O:1
S /= 100

S /= 10

;売却不可フラグ
SIF TALENT:売却不可
