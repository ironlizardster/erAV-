;面接サブルーチン
@INTERVIEW
#DIM 性別
#DIM TMP_CHARA

TMP_CHARA = TARGET

;面接相手の性別
PRINTL
PRINTL 今回面接するのは……
PRINTL [0] - オンナ
;PRINTL [1] - オトコ
;PRINTL [2] - 男の娘

CALL INPUTF, 0, 1, 2

;性別を決定(0=オンナ、1=オトコ、2=男の娘)
性別 = RESULT

CALL RANDOM_INTERVIEW(性別)

;ターゲットを戻す
TARGET = TMP_CHARA

RETURN RESULT

@RANDOM_INTERVIEW(性別)
#DIM 性別
#DIM COMMAND_NEXT_CANDIDATE = 8

CALL キャラパラメータ決定(性別)
CALL DISPLAY_TARGET_INTERVIEW

PRINTL 
PRINTFORML 300Pで契約しますか？
CALL PRINT_BUTTON("[はい]",1,COMMAND_はい,SHOWLENGTH / 4)
CALL PRINT_BUTTON("[いいえ]",1,COMMAND_いいえ,SHOWLENGTH / 4)
CALL PRINT_BUTTON("[次の人]",1,COMMAND_NEXT_CANDIDATE)

CALL INPUTF, COMMAND_はい, COMMAND_いいえ, COMMAND_NEXT_CANDIDATE

IF RESULT == COMMAND_はい
	CFLAG:値段 = 300
	購入値段 = 300
	RETURN COMMAND_はい
ELSEIF RESULT == COMMAND_いいえ
	DELCHARA CHARANUM - 1
	RETURN COMMAND_いいえ
ELSEIF RESULT == COMMAND_NEXT_CANDIDATE
	;面接相手やり直し
	DELCHARA CHARANUM - 1

	PRINTL 
	DRAWLINE
	PRINT 前の相手が帰ってから、
	IF RAND:3 == 2
		PRINTL あまり時間置かずすぐに、
	ELSEIF RAND:2 == 1
		PRINTL 何分経ったか…
		PRINTL コーヒーを５杯目に口をつけたころになってやっと、
	ELSE
		PRINT しばらく待つと、
	ENDIF
	
	RESTART
ENDIF

RETURN COMMAND_いいえ


