@TARGET体重設定()
#DIM 脂肪
#DIM 成熟
#DIM 身長係数
#DIM 筋肉
;**********************************************************************
;ここから新規分の体重＆3サイズ計算
;**********************************************************************

;体脂肪(1000分率)
	脂肪 = RAND:101 - RAND:101
	IF CFLAG:性別 == 0
		;女性:8歳=17%～16歳=25%
		IF BASE:実年齢 > 16
			脂肪 = 16 - 8
		ELSE
			脂肪 = BASE:実年齢 - 8
		ENDIF
		脂肪 = 160 + (脂肪 * 12) + RAND:121 - RAND:121
	ELSEIF CFLAG:性別 == 1
		;男性:16.5%で一定
		脂肪 = 165 + RAND:121 - RAND:121
	ELSEIF CFLAG:性別 == 2
		;男の娘(女性を参考に年齢増減をやや少なめ)
		IF BASE:実年齢 > 16
			脂肪 = 16
		ELSE
			脂肪 = BASE:実年齢 - 8
		ENDIF
		脂肪 = 160 + (脂肪 * 8)
;		脂肪 += RAND:121 - RAND:121
	ENDIF
	;痩せすぎ、肥満気味補正
	;体型 ： 肥満ランク、マイナスは痩せ
	IF CFLAG:体型 == 0
		脂肪 += RAND:11
		脂肪 -= RAND:11
	ELSEIF CFLAG:体型 > 0
		脂肪 += CFLAG:体型 * 20 + RAND:20
	ELSEIF CFLAG:体型 < 0
		脂肪 -= CFLAG:体型 * 20 + RAND:20
	ENDIF


;成熟度
	IF CFLAG:性別 == 0 || CFLAG:性別 == 2
		IF BASE:実年齢 < 10
			;10歳未満
			IF RAND:20 == 0
				;1/20で最大60の成熟度に達する稀有な存在(･･･が大半は10止まり)
				成熟 = 10 + RAND:51 - RAND:51
				SIF 成熟 < 10
					成熟 = 10
			ELSE
				成熟 = RAND:11 - RAND:11
				SIF 成熟 < 10
					成熟 = 10
			ENDIF
			身長係数 = 100
		ELSEIF (BASE:実年齢 > 10 && BASE:実年齢 < 16)
			;11歳～15歳
			IF RAND:10 == 0
				;1/10の確率でほぼ成長していない相手
				成熟 = 10 + (BASE:実年齢 - 10) * 1
			ELSE
				成熟 = 10 + (BASE:実年齢 - 10) * 15
			ENDIF
			成熟 = 成熟 + RAND:31 - RAND:31
			SIF 成熟 < 15
				成熟 = 15
			;SIF 成熟 > 100
			;	成熟 = 100
			;100オーバーしたらしたでいいや
			;成長度の差で身長係数が±3割増減
			身長係数 = 100 + ( 30 * ( 成熟 - ( 10 + (BASE:実年齢 - 10) * 15 ) ) / 100 )
		ELSE
			;16歳以上
			IF RAND:10 == 0
				;1/10の確率でで成長していない相手
				成熟 = 15
			ELSE
				成熟 = 85
			ENDIF
			成熟 = 成熟 + RAND:31 - RAND:31
			SIF 成熟 < 15
				成熟 = 15
			;SIF 成熟 > 100
			;	成熟 = 100
			;100オーバーしたらしたでいいや
			;成長度の差で身長係数が±3割増減
			身長係数 = 100 + ( 30 * ( 成熟 - 85 ) / 100 )
		ENDIF
	ELSEIF CFLAG:性別 == 1
		IF BASE:実年齢 < 12
			;10歳未満
			成熟 = RAND:11 - RAND:11
			SIF 成熟 < 10
				成熟 = 10
			身長係数 = 100
		ELSEIF (BASE:実年齢 > 12 && BASE:実年齢 < 17)
			;12歳～17歳
			成熟 = 10 + (BASE:実年齢 - 10) * 15
			成熟 = 成熟 + RAND:31 - RAND:31
			SIF 成熟 < 15
				成熟 = 15
			;成長度の差で身長係数が±3割増減
			身長係数 = 100 + ( 30 * ( 成熟 - ( 10 + (BASE:実年齢 - 10) * 15 ) ) / 100 )
		ELSE
			;17歳以上
			成熟 = 85
			成熟 = 成熟 + RAND:31 - RAND:31
			SIF 成熟 < 15
				成熟 = 15
			;成長度の差で身長係数が±3割増減
			身長係数 = 100 + ( 30 * ( 成熟 - 85 ) / 100 )
		ENDIF
	ENDIF

;筋肉度(＆体力加算)
	IF CFLAG:性別 == 0		;女子
		;結果：筋肉=10～60(最多:30～40[同数:11パターン])
		筋肉 = 30 + RAND:31 - RAND:21
	ELSEIF CFLAG:性別 == 1		;男子
		;結果：筋肉=15～95(最多:45～65[同数:21パターン])
		筋肉 = 45 + RAND:51 - RAND:31
	ELSEIF CFLAG:性別 == 2		;男の娘
		;結果：筋肉=10～70(最多:26～45[同数:11パターン])
		筋肉 = 35 + RAND:36 - RAND:26
	ENDIF
	MAXBASE:体力 = MAXBASE:体力 + ( 筋肉 - 50 ) * 10
	BASE:体重 += BASE:身長 * 筋肉 / 100	;体重筋肉分加算(指数1に対して身長の2％で設定)

;成熟度で身長を補正
BASE:身長 = BASE:身長 * 身長係数 / 100

;基準体重(筋肉･脂肪抜き):身長(cm)*身長(cm)*身長(cm)*係数(4.7)
BASE:体重 = BASE:身長 * BASE:身長 * BASE:身長 * 47 / 100 / 100 / 100 / 10
;女性と男の娘は基礎体重に8割掛け
SIF CFLAG:性別 == 0 || CFLAG:性別 == 2
	BASE:体重 = BASE:体重 * 8 / 10

;体重の加算(筋肉量):(15(%)+筋肉(%))*身長(筋肉)*係数(0.23)
BASE:体重 += (15 + 筋肉) * BASE:身長 * 23 / 100 / 100

;体重の加算(体脂肪):体脂肪(%)*身長(筋肉)*係数(0.5)
BASE:体重 += 脂肪 * 50 * BASE:身長 / 1000 / 100




;ウェストのサイズを登録 身長*0.38基準 体重影響
;身長×(基礎値:32％+脂肪(脂肪):[体脂肪1%あたり0.1%] + 筋肉(筋肉):[筋肉=1あたり0.1%]
;女性:38%[ウエスト基準] = 32% + 2.5%[体脂肪平均値250] + 3.5[筋力平均35]
	身長係数 = BASE:身長 * (3200 + 脂肪 + (筋肉 * 10) )
	身長係数 = 身長係数 / 100 * 10 / 100
	BASE:ウェスト = 身長係数 / 10
	身長係数 = 身長係数 + BASE:身長 * (RAND:16 - RAND:16) / 100
	BASE:ヒップ = 身長係数 / 10
