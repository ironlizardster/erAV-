@キャラパラメータ決定(性別)
#DIM 性別
#DIM 職業
#DIM 髪の色
#DIM バストサイズ
#DIM 身長係数
#DIM 空想喪失
#DIM 性格

;キャラを追加
ADDVOIDCHARA

;作成したキャラをターゲットにする
TARGET = CHARANUM - 1

;NOの設定
FLAG:汎用女優カウント ++
NO = FLAG:汎用女優カウント + MIN_NO_RANDOM_CHARA

;初期体力を格納
MAXBASE:体力 = 1500
;初期気力を格納
MAXBASE:気力 = 1500
MAXBASE:魅力値 = 80

CFLAG:性別 = 性別

;性別を登録
CALL SET_SEXUALITY(TARGET,性別_女)
IF CFLAG:性別 == 1
	CALL SET_SEXUALITY(TARGET,性別_オトコ)
ELSEIF CFLAG:性別 == 2
	CALL SET_SEXUALITY(TARGET,性別_オトコ)
	TALENT:オトコの娘 = 1
ENDIF

;基本的なプロフィールを登録
BASE:実年齢 = 年齢決定()
BASE:誕生月 = 誕生月決定()
BASE:誕生日 = 誕生日決定(BASE:誕生月)
BASE:血液型 = 血液型決定()

;性別による体力補正
IF CFLAG:性別 == 1
	;男は体力はあるが気力は低い
	MAXBASE:気力 -= 100
	SIF BASE:実年齢 > 12
		MAXBASE:体力 += 100	;女性との差=200
	SIF BASE:実年齢 > 15
		MAXBASE:体力 += 200	;さらに加算
ENDIF

;職業を決定(15歳までなら学生固定)
職業 = 職業決定(BASE:実年齢)

;職業を登録(ついでに衣装も登録)
;小●生は子供服、気力、体力ダウン
IF 職業 == 0 && BASE:実年齢 <= 12
	TALENT:小学生 = 1
	CFLAG:上着タイプ = 22
	MAXBASE:体力 -= 100
	MAXBASE:気力 -= 100
;中●生は制服
ELSEIF 職業 == 0 && BASE:実年齢 >= 13 && BASE:実年齢 <= 15
	TALENT:中学生 = 1
	CFLAG:上着タイプ = 18
;ナースはナース服、体力アップ
ELSEIF 職業 == 1
	TALENT:ナース = 1
	CFLAG:上着タイプ = 24
	MAXBASE:体力 += 100
;教師はスーツ、気力アップ
ELSEIF 職業 == 2
	TALENT:教師 = 1
	CFLAG:上着タイプ = 21
	MAXBASE:気力 += 100
;メイドはメイド服
ELSEIF 職業 == 3
	TALENT:メイド = 1
	CFLAG:上着タイプ = 25
;声優は普段着、気力アップ
ELSEIF 職業 == 4
	TALENT:声優 = 1
	CFLAG:上着タイプ = 1
	MAXBASE:体力 += 100
;風俗嬢はカクテルドレス
ELSEIF 職業 == 5
	TALENT:風俗嬢 = 1
	CFLAG:上着タイプ = 203
;アイドルはステージ衣装、気力アップ
ELSEIF 職業 == 6
	TALENT:アイドル = 1
	CFLAG:上着タイプ = 30
	MAXBASE:体力 += 100
	MAXBASE:魅力値 += 30
;主婦は普段着にエプロン
ELSEIF 職業 == 7
	TALENT:人妻 = 1
	CFLAG:上着タイプ = 101
	CFLAG:特別コスタイプ = 1
;ファッションモデルはブランド服
ELSEIF 職業 == 8
	TALENT:ファッションモデル = 1
	CFLAG:上着タイプ = 23
	MAXBASE:魅力値 += 20
;フリーターはキャミとデニムショート、体力アップ
ELSEIF 職業 == 9
	TALENT:アルバイト = 1
	CFLAG:上着タイプ = 140
	MAXBASE:体力 += 100
;無職はジャージ
ELSEIF 職業 == 10
	TALENT:ニート = 1
	CFLAG:上着タイプ = 103
ENDIF
IF CFLAG:性別 == 1
;男はとりあえずジャージ
	CFLAG:上着タイプ = 103
ENDIF

;髪色を決定
髪の色 = 髪色決定()

IF 髪の色 < 11
	CFLAG:髪色 = 髪の色
	CFLAG:染髪済 = 0
	CFLAG:地毛色 = 髪の色
ELSE
	CFLAG:髪色 = (髪の色 - 11)
	CFLAG:染髪済 = 1
	CFLAG:地毛色 = RAND:11
ENDIF

;髪型を登録
;確率の調整のため母数を大きくしている。
CFLAG:髪型 = 髪型決定(BASE:実年齢)

;胸のサイズを決定
バストサイズ = バストサイズ決定()

;身長で大柄|小柄を決定
身長係数 = 身長決定()

;肥満、痩せ設定
CFLAG:体型 = RAND:6 - RAND:6

;バストサイズの素質を登録
IF バストサイズ == 1
	TALENT:絶壁 = 1
ELSEIF バストサイズ == 2
	TALENT:貧乳 = 1
ELSEIF バストサイズ == 3
	TALENT:巨乳 = 1
ELSEIF バストサイズ == 4
	TALENT:爆乳 = 1
ENDIF

CALL TARGET身体設定(身長係数)
CALL TARGET体重設定()

;身長の素質を登録
;身長が150cm未満で[小柄]、170cm以上で[大柄]に
IF BASE:身長 < 150
	TALENT:小柄 = 1
ELSEIF BASE:身長 >= 170
	TALENT:大柄 = 1
ENDIF

BASE:体力 = MAXBASE:体力
BASE:気力 = MAXBASE:気力
BASE:魅力値 = MAXBASE:魅力値

;恋人を登録
LOCAL = RAND:10
IF LOCAL >= 2 
	IF LOCAL <= 6
		TALENT:彼氏あり = 1
		CFLAG:彼氏有 = 1
	ENDIF
	CFLAG:彼氏タイプ = LOCAL
ENDIF

;NTR進行度の上限を登録
CFLAG:NTR完了値 = 100 + RAND:100

CALL 初体験決定

;性格を決定
性格 = RAND:19

;性格を登録
SIF 性格 >= 10
	TALENT:(性格 - 10 + [[TALENT:臆病]]) = 1

;陰毛状態を決定
LOCAL = 陰毛決定(BASE:実年齢)
;陰毛を登録
IF LOCAL <= 1
	CFLAG:陰毛状態 = -2 
ELSEIF LOCAL >= 2 && LOCAL <= 5
	CFLAG:陰毛状態 = -1
ELSEIF LOCAL >= 6 && LOCAL <= 7
	CFLAG:陰毛状態 = 15
ELSEIF LOCAL >= 8
	CFLAG:陰毛状態 = 12
ENDIF

;乙女心系素質を決定
LOCAL = 乙女心決定()
;乙女心系素質を登録
SIF LOCAL >= 30
	TALENT:(LOCAL - 30 + [[TALENT:貞操観念]]) = 1

;苦痛適正で痛みに弱い|痛みに強いを決定
LOCAL = 苦痛適正決定()
;痛みに弱い|痛みに強いを登録
SIF LOCAL > 0
	TALENT:(LOCAL) = 1

;濡れ適正で濡れやすい|濡れにくいを決定
LOCAL = 濡れ適正決定()

;濡れやすい|濡れにくいを登録
SIF LOCAL > 0
	TALENT:(LOCAL) = 1

;ギャラ要求額を設定
;話術レベルで補正をつける？
BASE:ギャラ = (RAND:10 + 1) *100

;名前を決定する
DO
	LOCALS = %FAMILY_NAME_JAPAN()%
	LOCALS:1 = %LAST_NAME_JAPAN_FEMALE()%
	TEMP_STR '= LOCALS + LOCALS:1
[IF_DEBUG]
	DEBUGPRINTFORML %TEMP_STR%:{FINDCHARA(NAME, TEMP_STR)}
	PRINTFORMW %TEMP_STR%
