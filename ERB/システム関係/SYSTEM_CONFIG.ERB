		FLAG:薬品自動投与 -= 4
	ELSE
		FLAG:薬品自動投与 |= 4
	ENDIF
	RESTART
ELSEIF RESULT == 3
	IF (FLAG:薬品自動投与 & 8)
		FLAG:薬品自動投与 -= 8
	ELSE
		FLAG:薬品自動投与 |= 8
	ENDIF
	RESTART
ELSEIF RESULT != 999
	RESTART
ENDIF

RETURN 0

