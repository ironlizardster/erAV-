		GOTO INPUT_LOOP
	ENDIF

	;着衣フラグ初期化
	REPEAT CHARANUM
		CFLAG:COUNT:着衣状態 = 0
		CFLAG:COUNT:上着タイプ = 0
		CFLAG:COUNT:特別コスタイプ = 0
		CFLAG:COUNT:パンツ状態 = 0
		CFLAG:COUNT:ブラジャー状態 = 0
		CFLAG:COUNT:上着上状態 = 0
		CFLAG:COUNT:上着下状態 = 0
