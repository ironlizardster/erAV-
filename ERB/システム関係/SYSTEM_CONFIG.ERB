;---------------------------------------------------------
;キャラに陰毛を生やす FLAG:36使用
;---------------------------------------------------------
@CONFIG_PUBLICHAIR
IF FLAG:陰毛を生やす
	FLAG:陰毛を生やす = 0
ELSE
	FLAG:陰毛を生やす = 1

	;初期状態のキャラがいたら陰毛を設定する
	REPEAT CHARANUM
		;既に生えてるならそのまま
		SIF CFLAG:COUNT:陰毛状態 != 0
			CONTINUE
		

		;パイパンなら-1に設定
		IF TALENT:COUNT:パイパン
			CFLAG:COUNT:陰毛状態 = -1
		;幼稚・未熟なら7に設定
		ELSEIF TALENT:COUNT:幼稚 || TALENT:COUNT:未熟
			CFLAG:COUNT:陰毛状態 = 7
		;中学生なら7に設定
		ELSEIF TALENT:COUNT:中学生
			CFLAG:COUNT:陰毛状態 = 7
		;高校生なら9に設定
		ELSEIF TALENT:COUNT:高校生
			CFLAG:COUNT:陰毛状態 = 9
		;それ以外なら12に設定
		ELSE
			CFLAG:COUNT:陰毛状態 = 12
		ENDIF
	REND
ENDIF

RETURN 0

;---------------------------------------------------------
