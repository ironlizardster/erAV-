	SIF STAIN:COUNT & 2
		PRINT <Ｐ>
	SIF STAIN:COUNT & 4
		PRINT <精>
	SIF STAIN:COUNT & 8
		PRINT <Ａ>
	SIF STAIN:COUNT & 16
		PRINT <乳>
	SIF STAIN:COUNT & 32
		PRINT <尿>
	PRINTL 
REND

REPEAT 6
	SIF ASSI < 0
		BREAK
	SIF COUNT == 2 && TALENT:ASSI:ふたなり == 0 && TALENT:ASSI:オトコ == 0
		CONTINUE
	SIF COUNT == 3 && TALENT:ASSI:オトコ
		CONTINUE
	SIF COUNT == 5 && TALENT:ASSI:オトコ
		CONTINUE
	PRINTS NAME:ASSI
	IF COUNT == 0
