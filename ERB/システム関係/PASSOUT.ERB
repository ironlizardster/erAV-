			CALL PASSOUT_MESSAGE
		;失神回復時口上の呼び出し TFLAG:200が中身違うのでこれもスルーする
		;TFLAG:屈服刻印2 = 12
		;CALL SELF_KOJO(TARGET)
		;PRINTL 
	ELSE
		PRINTFORML 
		PRINTFORML %CALLNAME:TARGET%はまだ目を覚まさない
	ENDIF
ENDIF

