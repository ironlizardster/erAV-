@MISSION_CALC_130
; 失敗なら0, 成功なら1, 大成功なら2をRETURNで戻す
LOCAL:1 = 0
LOCAL:11 = 0

SIF NO:TARGET == [[キャラ:新島みひろ]]
	LOCAL:1++
SIF ABL:レズっ気 <= 2
	LOCAL:1++
SIF ABL:話術 >= 8
	LOCAL:1++
SIF MAXBASE:体力 >= 3500
	LOCAL:1++
SIF MAXBASE:気力 >= 2500
	LOCAL:1++
SIF EXP:売春経験 == 0
	LOCAL:1++
SIF MARK:屈服刻印 == 3
	LOCAL:1++
SIF TALENT:ＡＶ女優 == 0
	LOCAL:1++
SIF TALENT:淫乱 == 0
	LOCAL:1++
SIF EXP:Ａ経験 <= 20
	LOCAL:1++
SIF EXP:薬物経験 == 0
	LOCAL:1++
SIF EXP:フェラ経験 <= 10
	LOCAL:1++
SIF EXP:性交経験 <= 20
	LOCAL:1++
SIF ABL:技巧 >= 5
	LOCAL:1++
SIF ABL:奉仕技術 >= 5
	LOCAL:1++
SIF ABL:欲望 >= 5
	LOCAL:1++
SIF ABL:奉仕精神 >= 5
	LOCAL:1++
SIF TALENT:淫乱 == 0
	LOCAL:1++
SIF TALENT:淫魔 == 0
	LOCAL:1++

SIF TALENT:処女 == 1
	LOCAL:11++
SIF TALENT:アナル処女 == 1
	LOCAL:11++
SIF CFLAG:初キス相手 == -1
	LOCAL:11++
SIF CFLAG:処女喪失相手 == 0
	LOCAL:11++


IF LOCAL:1 == 19
	IF LOCAL:11 == 4
		RETURN 2
	ELSE
		RETURN 1
	ENDIF
ELSE
	RETURN 0
ENDIF

