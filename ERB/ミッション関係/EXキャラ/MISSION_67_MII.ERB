;=================================================
; ミッション受領時処理
;--------------------------------------------
; キャラクターの追加など、
; ミッションの受領時に行う処理を記述する
;=================================================
@MISSION_RECEIVE_67
; キャラクターを選択
CALL MISSION_LIFELIST_DECIDE([[キャラ:宮間未依]])
CALL MISSION_LIFELIST_PARTNER([[キャラ:須野郁美]])
; 即座に報告処理を呼び出す
CALL MISSION_EXEC_REPORT(67)

;=================================================
; ミッション達成判定計算
;--------------------------------------------
; ミッションの成否判定を記述する
;=================================================
@MISSION_CALC_67
; 失敗なら0, 成功なら1, 大成功なら2をRETURNで戻す
LOCAL:1 = 0
;未依分:合計11
SIF NO:TARGET == [[キャラ:宮間未依]]
	LOCAL:1++
SIF ABL:信頼 >= 10
	LOCAL:1++
SIF ABL:欲望 >= 10
	LOCAL:1++
SIF ABL:性交技術 >= 10
	LOCAL:1++
SIF ABL:精液中毒 >= 5
	LOCAL:1++
SIF EXP:Ｖ拡張経験 >= 5
	LOCAL:1++
SIF EXP:Ａ拡張経験 >= 5
	LOCAL:1++
SIF TALENT:淫魔 == 0
	LOCAL:1++
SIF TALENT:天使 == 0
	LOCAL:1++

;郁美分：合計6
SIF NO:ASSI == [[キャラ:須野郁美]]
	LOCAL:1++
SIF ABL:ASSI:信頼 >= 5
	LOCAL:1++
SIF ABL:ASSI:技巧 >= 10
	LOCAL:1++
SIF ABL:ASSI:サドっ気 >= 3
	LOCAL:1++
SIF MAXBASE:ASSI:体力 >= 3000
	LOCAL:1++
SIF MAXBASE:ASSI:気力 >= 2000
	LOCAL:1++

SIF LOCAL:1 == 15
	RETURN 2
RETURN 0

;=================================================
; ミッション報告後処理
;--------------------------------------------
; 経験や素質の変化など、
; ミッション報告後に行う処理を記述する
;--------------------------------------------
; 引数
;  ARG 成否判定の結果(0:失敗, 1:成功, 2:大成功)
;=================================================
@MISSION_REPORT_67(ARG)

TALENT:ASSI:童貞 = 1
TALENT:ASSI:ふたなり = 1
BASE:ASSI:ペニスサイズ = 15 + RAND:5
CFLAG:ASSI:包皮状態 = 3


IF TALENT:処女 == 1
	CALL 処女喪失実行(TARGET,NO:ASSI + 1,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_地下室,初状況_輪姦)
ENDIF

IF TALENT:Ａ処女 == 1
	CALL Ａ処女喪失実行(TARGET,NO:MASTER + 1,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_地下室,初状況_輪姦)
ENDIF

IF !CFLAG:初キス相手
	CALL 初キス喪失実行(TARGET,NO:ASSI + 1,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_地下室,初状況_男性器)
ENDIF

TCVAR:絶頂経験 += 180 + RAND:31
TCVAR:Ｖ経験 += 70 + RAND:31
TCVAR:Ａ経験 += 50 + RAND:31
TCVAR:性交経験 += TCVAR:Ｖ経験 + TCVAR:Ａ経験 - 30 - RAND:51
TCVAR:精液経験 += 60 + RAND:56
TCVAR:フェラ経験 += 50 + RAND:31
TCVAR:異常経験 += 5 + RAND:6
TCVAR:Ｖ拡張経験 += 1 + RAND:5
TCVAR:Ａ拡張経験 += 1 + RAND:5
SIF TALENT:ふたなり || TALENT:オトコ
		TCVAR:射精経験 += TCVAR:絶頂経験 + RAND:6

TCVAR:ASSI:絶頂経験 += 70 + RAND:31
TCVAR:ASSI:射精経験 += TCVAR:TARGET:精液経験
TCVAR:ASSI:性交経験 += TCVAR:TARGET:性交経験
TCVAR:ASSI:異常経験 += 2 + RAND:4
SIF TALENT:ASSI:ふたなり || TALENT:ASSI:オトコ
		TCVAR:ASSI:精液経験 += TCVAR:ASSI:射精経験 / 2

CALL MISSION_ADD_EXP(TARGET)

BASE:魅力値 -= 50 + RAND:11
SIF BASE:魅力値 <= 0
	BASE:魅力値 = 0

CALL MISSION_ADD_EXP(ASSI)

IF ARG == 0
	MAXBASE:体力 -= 3000
	BASE:体力 -= 3000
	SIF BASE:体力 <= 500
		BASE:体力 = 500
	MAXBASE:気力 -= 3000
	BASE:気力 -= 3000
	SIF BASE:気力 <= 500
		BASE:気力 = 500
	MAXBASE:魅力値 -= 100
	BASE:魅力値 -= 100
		SIF BASE:魅力値 <= 0
			BASE:魅力値 = 0
ELSE
	TALENT:マゾ = 1
ENDIF
