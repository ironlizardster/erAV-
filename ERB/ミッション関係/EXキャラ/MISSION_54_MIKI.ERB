;=================================================
; ミッション受領時処理
;--------------------------------------------
; キャラクターの追加など、
; ミッションの受領時に行う処理を記述する
;=================================================
@MISSION_RECEIVE_54
$KANADE_RECEIVE
; キャラクターを選択
CALL MISSION_LIFELIST_DECIDE([[キャラ:飯野美樹]])
; 即座に報告処理を呼び出す
CALL MISSION_EXEC_REPORT(54)

;=================================================
; ミッション達成判定計算
;--------------------------------------------
; ミッションの成否判定を記述する
;=================================================
@MISSION_CALC_54
; 失敗なら0, 成功なら1, 大成功なら2をRETURNで戻す
LOCAL:1 = 0
LOCAL:11 = 0
SIF NO:TARGET == [[キャラ:飯野美樹]]
	LOCAL:1++
SIF EXP:Ｖ経験 <= 20
	LOCAL:1++
SIF EXP:Ａ経験 <= 50
	LOCAL:1++
SIF EXP:異常経験 <= 2
	LOCAL:1++
SIF EXP:放尿経験 >= 50
	LOCAL:1++
SIF ABL:欲望 >= 5
	LOCAL:1++
SIF ABL:露出癖 >= 5
	LOCAL:1++
SIF ABL:マゾっ気 >= 2
	LOCAL:1++
SIF ABL:被写技能 >= 3
	LOCAL:1++
SIF TALENT:淫乱 == 0
	LOCAL:1++
SIF TALENT:マゾ == 0
	LOCAL:1++
SIF TALENT:淫魔 == 0
	LOCAL:1++
SIF MAXBASE:気力 >= 2000
	LOCAL:1++
SIF MAXBASE:体力 >= 2500
	LOCAL:1++

SIF IS処女(TARGET)
	LOCAL:11++
SIF ISＡ処女(TARGET)
	LOCAL:11++
IF LOCAL:1 >= 14
	SIF LOCAL:11 == 2
		RETURN 2
	RETURN 1
ENDIF

RETURN 0

;=================================================
; ミッション報告後処理
;--------------------------------------------
; 経験や素質の変化など、
; ミッション報告後に行う処理を記述する
;--------------------------------------------
; 引数
;  ARG 成否判定の結果(0:失敗, 1:成功, 2:大成功)
;=================================================
@MISSION_REPORT_54(ARG)

IF IS処女(TARGET)
	CALL 処女喪失実行(TARGET,初相手_接待客,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_学校,初状況_輪姦,"柴田智久（クラスメイト）")
ENDIF
IF !CFLAG:初キス相手
	CALL 初キス喪失実行(TARGET,初相手_接待客,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_学校,初状況_口,"浅田一貴（クラスメイト）")
ENDIF
IF ISＡ処女(TARGET)
	CALL 処女喪失実行(TARGET,初相手_接待客,BASE:実年齢,DAY:日付_月,DAY:日付_日,初場所_学校,初状況_輪姦,"木幡基樹（クラスメイト）")
ENDIF
TCVAR:絶頂経験 += 15 + RAND:11
TCVAR:Ｖ経験 += 15 + RAND:11
TCVAR:Ａ経験 += 10 + RAND:16
TCVAR:性交経験 += TCVAR:Ｖ経験 + TCVAR:Ａ経験
TCVAR:精液経験 += 50 + RAND:21
TCVAR:フェラ経験 += 20 + RAND:16
TCVAR:異常経験 += 1 + RAND:4
TCVAR:放尿経験 += TCVAR:性交経験 + RAND:6
BASE:魅力値 -= 30 + RAND:11
SIF TALENT:ふたなり || TALENT:オトコ 
	TCVAR:射精経験 += TCVAR:絶頂経験 + RAND:6

IF ARG == 0
	CALL ADD_CATASTROPH(TARGET)
ELSEIF ARG == 1
	TALENT:プライド高い = 0
	TALENT:プライド低い = 1
	SIF ABL:性交中毒 < 3
		ABL:性交中毒 = 3
ELSEIF ARG == 2
	TALENT:プライド高い = 0
	TALENT:プライド低い = 1
	SIF ABL:性交中毒 < 3
		ABL:性交中毒 = 3
	TALENT:淫乱 = 1
	TALENT:マゾ = 1
ENDIF


