ENDIF
;最低でも1回・1個は必要
SIF A < 1
	A = 1
SIF B < 1
	B = 1
SIF C < 1
	C = 1

;異常経験が不足
IF D > EXP:異常経験
		I |= 2
		J |= 2
ENDIF

;レズっ気がレズ中毒＋１レベルでないと能力不足
IF ABL:レズっ気 < ABL:レズ中毒 + 1
	I |= 4
	J |= 4
ENDIF

;快Ｃの珠が不足
SIF JUEL:快Ｃ < B
	I |= 1
;欲情の珠が不足
SIF JUEL:欲情 < A
	I |= 1
;屈服の珠が不足
SIF JUEL:屈服 < A
	I |= 1
;レズ経験が不足
SIF EXP:レズ経験 < C
	I |= 2

IF I == 0
	RETURN 1
ELSE
	RETURN 0
ENDIF
