IF CFLAG:靴タイプ != 0
	PRINTFORM ひとまず%CALLNAME:MASTER%は%CALLNAME:TARGET%に履いている
	CALL PRINT_SHOESTYPE_MAIN2
	PRINTFORM を脱いでこちらに来るように促した……
ENDIF

RETURN 1
