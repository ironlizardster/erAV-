;-------------------------------------------------
;調教開始時（2回目以降、39をキャラ番号に変更する） CFLAGなし 
;-------------------------------------------------
@K39_KOJO2
;反発刻印Lv3
IF MARK:反発刻印 == 3 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「扱いは悪いし、お金はあんまりくれない…、最悪です…！」
	RETURN 1

;屈服刻印Lv0
ELSEIF MARK:屈服刻印 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「お金のために、一生懸命がんばりますです！」
	RETURN 1

;屈服刻印Lv1
ELSEIF MARK:屈服刻印 == 1 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORML 「監督さん！労働者のお給料が下がるとますます景気が悪くなるんですよ！？」
	PRINTFORMW 「この国の景気のためにも、是非ご検討してくださいです～！」」
	RETURN 1

;屈服刻印Lv2
ELSEIF MARK:屈服刻印 == 2 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORML 「(私はここにお金を稼ぎに来ただけ…)」
	PRINTFORMW 「(それ以外に価値の無い女なんだよ……)」
	RETURN 1

;屈服刻印Lv3＋愛無し
ELSEIF MARK:屈服刻印 == 3 && TALENT:TARGET:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORML 「辛くたって、苦しくったって…！」
	PRINTFORMW 「主張し続けます！…愛を！…夢を！……賃上げを！」
	RETURN 1

;恋心
;基本的に銭ゲバなところはあんまり変わらない
ELSEIF TALENT:TARGET:恋心 == 1 && FLAG:口上使用 == 2
	DRAWLINE
	;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
	IF RAND:3 == 0
		PRINTFORML 「え～？今日はちょっと行かないといけないところが…」
		PRINTFORMW 「…この後ごちそうしてくれるんですか！？それを早く言ってくださいよ～！」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「監督さん！今日も未来の億万長者を目指して…がんばりましょ～！」
	ELSE
		PRINTFORML 「今日みたいに天気の良い日は、無性に悲しくなるんです…」
		PRINTFORMW 「心でも、身体でもいいですから…慰めてくださいです…」
	ENDIF
	RETURN 1
ENDIF
RETURN 0

;-------------------------------------------------
;EVENTEND関係（39をキャラ番号に置換） CFLAG 211～220を使用
;調教終了時のセリフ
;-------------------------------------------------
@EVENTEND
SIF FLAG:口上使用 <= 0
	RETURN 0
SIF NO:TARGET != [[キャラ:小日向詩織]]
	RETURN 0

;キャラ死亡時は口上をスキップ
SIF BASE:体力 <= 0
	RETURN 0

;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
;反発刻印Lv3+愛なし
IF MARK:反発刻印 == 3 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORMW 「歌音さんに言いつけてやる、です…！」
	RETURN 1

;屈服刻印Lv1以下+愛なし
ELSEIF MARK:屈服刻印 <= 1 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORMW 「はぁ…はぁ…、こ、これでお給料アップは間違いなしですね…！」
	RETURN 1

;屈服刻印Lv2+愛なし
ELSEIF MARK:屈服刻印 == 2 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORML 「(こんなことして何になるんだろ…)」
	PRINTFORMW 「(ううん、違う。私はこんなことしかできないんだ…！)」
	RETURN 1

;屈服刻印Lv3+愛なし
ELSEIF MARK:屈服刻印 == 3 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORML 「今日はなんだか気持ちよかったです…！」
	PRINTFORML 「(…なんでだろ、顔は本当、全然似てないのにな～…)」
	RETURN 1

;愛(体力500以上)
ELSEIF TALENT:TARGET:恋心 == 1 && BASE:体力 >= 500
	DRAWLINE
	IF RAND:3 == 0
		PRINTFORMW 「…え？もう終わりですか？…こんなんで売り物になるんですか～？」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「たまには…デートとかしませんか…？」
	ELSE
		PRINTFORMW 「お金も、愛も、もっといっぱい欲しいです…！」
	ENDIF
	RETURN 1
;愛(体力500未満)
ELSEIF TALENT:TARGET:恋心 == 1 && BASE:体力 <= 500
	DRAWLINE
	IF RAND:3 == 0
		PRINTFORML 「つ、疲れました…！」
		PRINTFORMW 「私、結構、がんばりましたよね…？」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「こ、これ、で…おきゅ～りょ～ｱﾌﾟ…zzz...」
	ELSE
		PRINTFORML 「は、激しすぎです…！死んじゃいます～…！」
	ENDIF
	RETURN 1
ENDIF
RETURN 0

;--------------------------------------------------
;@KOJO_MESSAGE_COM関係（39をキャラ番号に置換）
;コマンド実行時に出力されます
;--------------------------------------------------
@KOJO_MESSAGE_COM_39
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0 
;ボールギャグ着用時には口上をスキップする
SIF TEQUIP:ボールギャグ && SELECTCOM != [[TRAIN:ボールギャグ]]
	RETURN 0

;-------------------------------------------------
;コマンド実行時のセリフ CFLAG 301～400を使用
;-------------------------------------------------
;-------------------------------------------------
;愛撫 CFLAG:301
;-------------------------------------------------
IF SELECTCOM == [[TRAIN:愛撫]]
	;初めて
	IF CFLAG:301 == 0
		;屈服刻印Lv2以上
		IF MARK:屈服刻印 >= 2
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
		CFLAG:301 = 1
		RETURN 0
	;二回目以降
	ELSE
		;愛
		IF TALENT:TARGET:恋心 == 1 && (CFLAG:301 <= 4 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 5
		;屈服刻印Lv3
		ELSEIF MARK:屈服刻印 == 3 && (CFLAG:301 <= 3 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 4
		;屈服刻印Lv2
		ELSEIF MARK:屈服刻印 == 2 && (CFLAG:301 <= 2 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 3
		;それ以外
		ELSEIF MARK:屈服刻印 <= 1 && (CFLAG:301 <= 1 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 2
		ENDIF
		RETURN 0
	ENDIF
ENDIF

;-------------------------------------------------
;クンニ CFLAG:302
;-------------------------------------------------
IF SELECTCOM == [[TRAIN:クンニ]]
	;初めて
	IF CFLAG:302 == 0
		;処女
