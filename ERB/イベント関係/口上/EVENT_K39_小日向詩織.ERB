			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
		CFLAG:302 = 1
		RETURN 0
	;二回目以降
	ELSE
		;愛
		IF TALENT:TARGET:恋心 == 1 && (CFLAG:302 <= 3 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:302 = 4
		;屈服刻印Lv3
		ELSEIF MARK:屈服刻印 == 3 && (CFLAG:302 <= 2 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:302 = 3
		;それ以外（屈服刻印Lv3未満）
		ELSEIF CFLAG:302 <= 1 || FLAG:口上使用 == 2
			PRINTFORMW 
			CFLAG:302 = 2
		ENDIF
		RETURN 0
	ENDIF
ENDIF

;-------------------------------------------------
;アナル愛撫 CFLAG:303
;-------------------------------------------------
IF SELECTCOM == [[TRAIN:アナル愛撫]]
	;初めて
	IF CFLAG:303 == 0
		PRINTFORMW 
		CFLAG:TARGET:303 = 1
		RETURN 0
	;二回目以降
