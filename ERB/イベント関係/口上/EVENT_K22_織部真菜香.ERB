	DRAWLINE
	PRINTFORMW 
	DRAWLINE
ENDIF

;-------------------------------------------------
;妊娠発覚 CFLAG:271
;-------------------------------------------------
IF TFLAG:口上番号 == 11
	SIF CFLAG:271 >= 1
		PRINTFORMW 「おにーさん、またデキちゃったよ？」
		PRINTFORMW 「もー、どう責任取るのー？　ばかぁｖ」
		RETURN 0
	;崩壊してしまった場合
	IF TALENT:崩壊 == 1
		PRINTFORMW 「おか……あさん……？　%CSTR:一人称%……が？」
		PRINTFORMW %CALLNAME:TARGET%の瞳から、光が失われていった……。
	;父親が主人で母親が愛持ち
	ELSEIF TALENT:恋心 && CFLAG:妊娠精子 == 1
		PRINTFORMW 「おにーさんの赤ちゃん……ｖ」
		PRINTFORMW 「えへへ……うれしい……ｖ」
		PRINTFORMW 「まだ中学生だしちょっと不安だけど……お兄さんがいてくれるから、頑張って育てるよ」
	;その他
	ELSE
		PRINTFORMW 「……赤ちゃんデキちゃったよ」
		PRINTFORMW 「ねえ、%CSTR:一人称%まだ中学生なんだよ？」
		PRINTFORMW 「せめておにーさんの赤ちゃんならよかったのに……」
	ENDIF
	CFLAG:271 = 1
ENDIF

;-------------------------------------------------
;出産 CFLAG:272
;-------------------------------------------------
IF TFLAG:口上番号 == 12
	SIF CFLAG:272 >= 1
		RETURN 0
	;崩壊している場合
	IF TALENT:崩壊 == 1
		PRINTFORMW 
	;父親が主人で母親が愛持ち
	ELSEIF TALENT:恋心 && CFLAG:妊娠精子 == 1
		PRINTFORMW 「えへへ……おにーさんと%CSTR:一人称%の赤ちゃんだよｖ」
		PRINTFORMW 「ほら、パパがきてくれましたよー……うふふ、嬉しそうに笑ってる」
	;その他
	ELSE
		PRINTFORMW 「誰がパパかもわからない赤ちゃんだけど、この子に罪はないよね……」
	ENDIF
	CFLAG:272 = 1
ENDIF

;-------------------------------------------------
;死亡　このバリアントで死んだことないな
;-------------------------------------------------
IF TFLAG:口上番号 == 999
	;愛
	IF TALENT:恋心
		PRINTFORMW 「おにーさんに殺されるなら……いいかな……」
	;それ以外
	ELSE
		PRINTFORMW 「おにーさん……激しすぎ……」
	ENDIF
ENDIF

;-------------------------------------------------
;寿命による消滅
;-------------------------------------------------
IF TFLAG:口上番号 == 998
	;愛
	IF TALENT:恋心
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF

