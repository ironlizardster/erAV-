;消費アイテム入手判定
@EGG_ITEM_USE
SELECTCASE RAND:2
CASE 0
	IF ITEM:コンドーム >= 99
		PRINTFORML %ITEMNAME:[[ITEM:コンドーム]]%が入っていた
		ITEM:コンドーム += 1
	ELSE
		PRINTFORML 小さな宝石が入っていた
		MONEY += 500
	ENDIF
CASEELSE
	PRINTFORML 小さな宝石が入っていた
	MONEY += 500
ENDSELECT