	IF ABL:技巧 >= 5 && ABL:奉仕技術 >= 5 && EXP:フェラ経験 >= 1000 && TALENT:舌使い == 0
		PRINTFORMW %CALLNAME:TARGET%の奉仕もかなりのものとなってきた……
		PRINTFORMW %CALLNAME:TARGET%は【%TALENTNAME:[[TALENT:舌使い]]%】を得た
		TALENT:舌使い = 1
	ENDIF
ENDIF

