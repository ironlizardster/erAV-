@NAKADASHI_CHECK
;中出しされた精子量に応じて確率処理

;妊娠フィルタ部分
SIF FLAG:妊娠なし == 1
	RETURN 0
SIF CFLAG:T:33 != 0
	RETURN 0

;チェックするフラグ設定
F = 101 + P
SIF P == 1
	F = 101
SIF P == 8
	F = 112

;対象が男か未熟なら関数終了
SIF TALENT:T:オトコ || TALENT:T:未熟
	RETURN 0

						;****あなた妊娠パッチ追加ここから****
						;****追加ここから****(加筆)

;対象がアンドロイド、淫魔、避妊治療済なら関数終了
IF !CAN_GET_PREGNANT(T)
	CFLAG:T:F = 0
	RETURN 0
ENDIF

;獣姦で対象が動物耳じゃないなら関数終了
IF (P == 5 && TALENT:T:動物耳 == 0) && (CFLAG:T:排卵促進剤使用 == 0 || ミッション結果:303 < 3)
	CFLAG:T:F = 0
	RETURN 0
ENDIF
						;****追加ここまで****(加筆)
;中だしされてないなら関数終了
SIF CFLAG:T:F == 0
	RETURN 0

;妊娠が既に確定している場合は関数終了
IF CFLAG:T:出産日 > 0
	CFLAG:T:F = 0
	RETURN 0
ENDIF

;妊娠中もしくは育児中は関数終了
						;****スライムパッチ追加ここから****
IF TALENT:T:妊娠 || TALENT:T:育児中 || TALENT:T:抱卵
						;****スライムパッチ追加ここまで****
	CFLAG:T:F = 0
	RETURN 0
ENDIF

IF CFLAG:T:排卵促進剤使用 == -1
	CFLAG:T:F = 0
	RETURN 0
ENDIF
						;****あなた妊娠パッチ追加ここまで****
;排卵剤の有無による定数設定
C = 3 - (CFLAG:T:排卵促進剤使用 * 2)
SIF C < 0
	C = 1

IF CFLAG:T:F >= 30
	SIF RAND:((1 + TALENT:T:小柄 * 2) * C) == 0
		CFLAG:T:妊娠精子 = P
ELSEIF CFLAG:T:F >= 20
	SIF RAND:((2 + TALENT:T:小柄 * 2) * C) == 0
		CFLAG:T:妊娠精子 = P
ELSEIF CFLAG:T:F >= 15
	SIF RAND:((3 + TALENT:T:小柄 * 2) * C) == 0
		CFLAG:T:妊娠精子 = P
ELSEIF CFLAG:T:F >= 10
	SIF RAND:((4 + TALENT:T:小柄 * 2) * C) == 0
		CFLAG:T:妊娠精子 = P
ELSEIF CFLAG:T:F >= 5
	SIF RAND:((5 + TALENT:T:小柄 * 2) * C) == 0
		CFLAG:T:妊娠精子 = P
ELSEIF CFLAG:T:F >= 1
	SIF RAND:((6 + TALENT:T:小柄 * 2) * C) == 0
		CFLAG:T:妊娠精子 = P
ENDIF

;判定を行った膣射のリセット
CFLAG:T:F = 0

@CAN_GET_PREGNANT(ARG)
#FUNCTION
RETURNF !TALENT:ARG:妊娠 && !TALENT:ARG:抱卵 && !TALENT:ARG:育児中 && CFLAG:ARG:危険日 == 1 && CFLAG:ARG:排卵促進剤使用 != -1 && (!(TALENT:ARG:アンドロイド || TALENT:ARG:淫魔 || TALENT:ARG:避妊治療済 || TALENT:ARG:性魔術 || TALENT:ARG:異界知識 || TALENT:ARG:最先端科学 || TALENT:ARG:天使 || TALENT:ARG:幽霊) || (CFLAG:ARG:排卵促進剤使用 > 0 && ミッション結果:303 >= 3))

