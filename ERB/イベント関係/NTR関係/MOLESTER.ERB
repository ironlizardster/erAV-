;===========================================================
;痴漢発生の処理
;===========================================================
@EVENT_MOLESTER

SIF FLAG:治安悪化レベル == 0
	RETURN 1

REPEAT CHARANUM
	IF COUNT == 0
		CONTINUE
						;****痴漢誘惑追加ここから****
	ELSEIF NO:COUNT == [[キャラ:宮間かなで]] && NO:MASTER == [[キャラ:あなた]] && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF NO:COUNT == [[キャラ:佐々木美乃里]] && NO:MASTER == [[キャラ:あなた]] && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF NO:COUNT == [[キャラ:須野郁美]] && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF IS_MALE(COUNT)
		CONTINUE
	ELSEIF CFLAG:COUNT:入寮中
		CONTINUE
	ELSEIF TALENT:COUNT:極道の娘 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:護身術 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:アンドロイド && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:淫魔 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:警察官 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:天使 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:極道の妻 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:探偵 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:Triptych && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:幽霊 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF TALENT:COUNT:[[TALENT:ONLY ONE EMPRESS]] && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF CFLAG:COUNT:護衛 && CFLAG:COUNT:痴漢誘惑 == 0
		CONTINUE
	ELSEIF CFLAG:COUNT:着衣状態 >= 64 && CFLAG:COUNT:特別コスタイプ == 79
		CONTINUE
						;****痴漢誘惑追加ここまで****
	ELSE
		LOCAL = RAND:100
		IF TALENT:COUNT:臆病
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:大人しい
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:解放
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:恥じらい
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:即落ち
			LOCAL += 50
		ENDIF
		IF TALENT:COUNT:淫乱
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:魅力
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:心弱い
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:地味
			LOCAL += RAND:10
		ENDIF
		IF TALENT:COUNT:ナマイキ
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:気丈
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:活発
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:貞操観念
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:抵抗
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:恥薄い
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:ボーイッシュ
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:男性不信
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:カリスマ
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:逆襲
			LOCAL -= RAND:10
		ENDIF
		IF TALENT:COUNT:彼氏あり
			LOCAL -= 50
		ENDIF
		IF TALENT:COUNT:運動部
			LOCAL -= RAND:10
		ENDIF
					;****痴漢誘惑追加ここから****
		IF CFLAG:COUNT:痴漢誘惑
			LOCAL += BASE:COUNT:魅力値
		ENDIF
					;****痴漢誘惑追加ここまで****
		LOCAL += 10 * FLAG:治安悪化レベル
		IF LOCAL >= 90
			CALL MOLESTER_BRUNCH(COUNT)
			FLAG:治安悪化 += RAND:5 + 1
			LOCAL = 0
		ELSE
			LOCAL = 0
		ENDIF
	ENDIF
REND