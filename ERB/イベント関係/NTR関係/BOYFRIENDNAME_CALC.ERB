;===========================================================
;名前ランダム決定。
;ARG=キャラ番号,ARG:1=モード(0ならBF,1ならSF)
;===========================================================
@DECIDE_BOY_NAME,ARG,ARG:1
#DIM LNAMENO,1
#DIM FNAMENO,1


SELECTCASE ARG:1
	CASE 0
		LNAMENO = [[CSTR:彼氏姓]]
		FNAMENO = [[CSTR:彼氏名]]
	CASE 1
		LNAMENO = [[CSTR:セフレ姓]]
		FNAMENO = [[CSTR:セフレ名]]
ENDSELECT

