		PRINTFORMW %PALAMNAME:[[PALAM:不快]]%の珠＋2000
		JUEL:ARG:不快 += 2000
	ENDIF
	EXP:ARG:精液経験 += 2
	EXP:ARG:痴漢経験 += 1
