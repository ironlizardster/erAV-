;===========================================================
;痴漢発生の処理
;===========================================================
@MOLESTER_BRUNCH(ARG)

LOCAL += RAND:6
IF LOCAL <= 1
	TFLAG:口上番号 = 16
ELSEIF LOCAL == 2
	TFLAG:口上番号 = 17
ELSEIF LOCAL == 3
	TFLAG:口上番号 = 18
ELSEIF LOCAL == 4
	TFLAG:口上番号 = 19
ELSEIF LOCAL == 5
	TFLAG:口上番号 = 20
ENDIF

CALL SELF_KOJO(ARG)
PRINTFORML %CALLNAME:MASTER%が事務所を出ようとした時、すでに帰宅したはずの%CALLNAME:ARG%から%CALLNAME:MASTER%の携帯電話に着信があった。
PRINTFORMW 何事かと思い慌てて%CALLNAME:MASTER%が電話に出ると、\@ TALENT:ARG:淫乱 ? 艶っぽい声で # 涙声で \@帰りの電車で痴漢に遭ったことを報告してきた……
PRINTFORM %CALLNAME:MASTER%が詳しい話を%CALLNAME:ARG%に聞くと、
IF LOCAL <= 1
	PRINTFORML 尻や胸をイヤらしい手つきで撫で回され、勃起した痴漢のペニスを無理やり握らされたようだ。
	PRINTFORMW そしてそのまま大量のザーメンを全身にぶっかけられたらしい……	  
	PRINTFORML %EXPNAME:[[EXP:精液経験]]%+2
	PRINTFORML %PALAMNAME:[[PALAM:恥情]]%の珠＋500
	PRINTFORML %PALAMNAME:[[PALAM:恐怖]]%の珠＋100
	IF TALENT:ARG:淫乱 == 0
		PRINTFORMW %PALAMNAME:[[PALAM:不快]]%の珠＋1000
		JUEL:ARG:不快 += 1000
	ENDIF
	EXP:ARG:精液経験 += 2
	EXP:ARG:痴漢経験 += 1
	JUEL:ARG:恥情 += 500
	JUEL:ARG:恐怖 += 100
	LOCAL = 0
	BASE:ARG:魅力値 -= 2
ELSEIF LOCAL == 2
	PRINTFORML 尻や胸をイヤらしい手つきで撫で回され、勃起した痴漢のペニスを無理やり握らされたようだ。
	PRINTFORMW 手コキを強要されながら%CALLNAME:ARG%は下着越しにクリトリスや乳首を執拗にいじくり回され%CALLNAME:ARG%が絶頂すると同時に痴漢が大量のザーメンを%CALLNAME:ARG%の手のひらに射精したらしい…
	PRINTFORML %EXPNAME:[[EXP:絶頂経験]]%+1
	PRINTFORML %EXPNAME:[[EXP:精液経験]]%+2
	PRINTFORML %PALAMNAME:[[PALAM:恥情]]%の珠＋800
	PRINTFORML %PALAMNAME:[[PALAM:恐怖]]%の珠＋200
	IF TALENT:ARG:淫乱 == 0
		PRINTFORMW %PALAMNAME:[[PALAM:不快]]%の珠＋1500
		JUEL:ARG:不快 += 1500
	ENDIF
	EXP:ARG:精液経験 += 2
	EXP:ARG:痴漢経験 += 1
	EXP:ARG:絶頂経験 += 1
	JUEL:ARG:恥情 += 800
	JUEL:ARG:恐怖 += 200
	LOCAL = 0
	BASE:ARG:魅力値 -= 3
ELSEIF LOCAL == 3
	PRINTFORML 尻や胸をイヤらしい手つきで撫で回され、勃起した痴漢のペニスを無理やり握らされたようだ。
	PRINTFORMW 手コキを強要されながら%CALLNAME:ARG%は下着の中に手を突っ込まれ尻穴を指で弄られたらしい…
	PRINTFORM %CALLNAME:ARG%が
	IF TALENT:ARG:尻穴狂い
		PRINTFORM とろけた表情で痴漢の指をアヌスでくわえ込み、嬌声をあげて絶頂すると同時に
	ELSEIF ABL:ARG:Ａ感覚 >= 3
		PRINTFORM 絶頂すると同時に
	ELSEIF EXP:ARG:Ａ経験 == 0
		PRINTFORM 初めての異物感に抵抗している姿に興奮した
	ELSE
		PRINTFORM 異物感に抵抗している姿に興奮した
	ENDIF
	PRINTFORMW 痴漢が大量のザーメンを%CALLNAME:ARG%の手のひらに射精したとのことだ……
	IF ABL:ARG:Ａ感覚 >= 3
		PRINTFORML %EXPNAME:[[EXP:絶頂経験]]%+1
		EXP:ARG:絶頂経験 += 1
	ENDIF
	PRINTFORML %EXPNAME:[[EXP:Ａ経験]]%+1
	PRINTFORML %EXPNAME:[[EXP:精液経験]]%+2
	PRINTFORML %PALAMNAME:[[PALAM:恥情]]%の珠＋1000
	PRINTFORML %PALAMNAME:[[PALAM:恐怖]]%の珠＋300
