	PRINTFORML %PALAMNAME:[[PALAM:恐怖]]%の珠＋300
	IF TALENT:ARG:淫乱 == 0
		PRINTFORMW %PALAMNAME:[[PALAM:不快]]%の珠＋2000
		JUEL:ARG:不快 += 2000
	ENDIF
	EXP:ARG:精液経験 += 2
	EXP:ARG:痴漢経験 += 1
	EXP:ARG:Ｖ経験 += 2
	JUEL:ARG:恥情 += 1000
	JUEL:ARG:恐怖 += 300
