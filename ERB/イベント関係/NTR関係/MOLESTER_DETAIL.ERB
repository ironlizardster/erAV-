					PRINTFORMW %PALAMNAME:[[PALAM:不快]]%の珠＋2000
				ENDIF
			EXP:COUNT:精液経験 += 2
			EXP:COUNT:痴漢経験 += 1
			EXP:COUNT:Ｖ経験 += 2
				IF ABL:COUNT:Ｖ感覚 >= 3
					EXP:COUNT:絶頂経験 += 1
				ENDIF
			JUEL:COUNT:恥情 += 1000
			JUEL:COUNT:恐怖 += 300
				IF TALENT:COUNT:淫乱 == 0
					JUEL:COUNT:不快 += 2000
				ENDIF
			LOCAL = 0
			BASE:COUNT:魅力値 -= 15
		ENDIF
	DRAWLINE
