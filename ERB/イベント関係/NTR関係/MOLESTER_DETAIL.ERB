LOCAL += RAND:6
IF LOCAL == 0
	TFLAG:口上番号 = 16
ELSEIF LOCAL == 1
	TFLAG:口上番号 = 16
ELSEIF LOCAL == 2
	TFLAG:口上番号 = 17
ELSEIF LOCAL == 3
	TFLAG:口上番号 = 18
ELSEIF LOCAL == 4
