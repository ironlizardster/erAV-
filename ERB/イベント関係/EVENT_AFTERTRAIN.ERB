	A += 1
ELSEIF TALENT:快感の否定
	A -= 2
ENDIF

;淫乱によるボーナス
SIF TALENT:淫乱
	A += 1

;回数が0以下になっていたら終了
SIF A <= 0
	RETURN 0

DRAWLINE
