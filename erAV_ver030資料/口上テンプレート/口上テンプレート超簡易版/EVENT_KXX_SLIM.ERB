;=================================================
;erapoke専用口上テンプレート（超簡易版）
;=================================================
;まずX1とX2をキャラ番号に置換し、Y1を助手のキャラ番号に置換してください
;ただし、キャラ番号が一桁のキャラの場合、X1,Y1は1、X2は001のように変える必要があります
;また、キャラ番号が二桁のキャラの場合、X1,Y1は21、X2は021のように変える必要があります
;コマンド内の各条件の下にあるPRINTFORMWの後ろに、半角スペースを置いてセリフを追加してください。
;口上を追加しないコマンドには行頭に『;』をつけたほうが、
;予期せぬ不具合などが起こるのをある程度防げるかもしれません
;最後になりますが、read meもできるかぎり読んで下さい
;eratohoU専用口上テンプレートを改造して流用
;-------------------------------------------------

;-------------------------------------------------
;○○口上簡易版（キャラクターの名前を入れる）
;（コンセプトなどがあればここに書いておくと、加筆する際の目安になるでしょう）
;
;-------------------------------------------------

;==================================================
;EVENTTRAIN関係（X1をキャラ番号に置換）
;調教開始時のセリフ CFLAG 201～210を使用
;==================================================
@EVENTTRAIN
SIF FLAG:口上使用 <= 0
	RETURN 0
SIF NO:TARGET != X1
	RETURN 0

;-------------------------------------------------
;初調教時 CFLAG:201
;-------------------------------------------------
IF CFLAG:201 == 0
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 1
	RETURN 1

;-------------------------------------------------
;屈服刻印（各Lv一回のみ） CFLAG:201
;-------------------------------------------------
;屈服刻印Lv1
ELSEIF CFLAG:201 < 2 && MARK:屈服刻印 == 1
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 2
	RETURN 1

;屈服刻印Lv2
ELSEIF CFLAG:201 < 3 && MARK:屈服刻印 == 2
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 3
	RETURN 1

;屈服刻印Lv3＋愛無し
ELSEIF CFLAG:201 < 4 && MARK:屈服刻印 == 3 && TALENT:恋心 == 0
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 4
	RETURN 1

;愛
ELSEIF CFLAG:201 < 5 && TALENT:恋心
	DRAWLINE
	PRINTFORMW 
	CFLAG:201 = 5
	RETURN 1

;助手の有無をチェック（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI < 0 
	CALL KX1_KOJO2

;-------------------------------------------------
;簡易助手口上 CFLAG:202～210
;Ynを調教対象X1との会話が発生する助手のキャラ番号に変更する
;会話が発生する助手を2人以上に増やす場合は、
;コピー＆ペーストをしてCFLAGの数を203、204と増やす（210が上限）
;-------------------------------------------------
;助手○○（○○にY1のキャラ名を入れる）
;ELSEIF NO:ASSI == Y1
;	DRAWLINE
;	;初めて
;	IF CFLAG:202 == 0
;		PRINTFORMW 
;		CFLAG:202 = 1
;		RETURN 1
;	;二回目以降
;	ELSE CFLAG:202 == 1 && FLAG:口上使用 ==2
;		PRINTFORMW 
;		RETURN 1
;	ENDIF
;助手○○（○○にY2のキャラ名を入れる）
;ELSEIF NO:ASSI == Y2
;	DRAWLINE
;	;初めて
;	IF CFLAG:203 == 0
;		PRINTFORMW 
;		CFLAG:203 = 1
;		RETURN 1
;	;二回目以降
;	ELSE CFLAG:203 == 1 && FLAG:口上使用 ==2
;		PRINTFORMW 
;		RETURN 1
;	ENDIF
;口上のある助手が居ない場合は、通常の二回目以降の口上へ飛ぶ
ELSE
	CALL KX1_KOJO2
ENDIF

;==================================================
;調教開始時（2回目以降、X1をキャラ番号に変更する） CFLAGなし 
;==================================================
@KX1_KOJO2
;反発刻印Lv3
IF MARK:反発刻印 == 3 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv0
ELSEIF MARK:屈服刻印 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv1
ELSEIF MARK:屈服刻印 == 1 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv2
ELSEIF MARK:屈服刻印 == 2 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv3＋愛無し
ELSEIF MARK:屈服刻印 == 3 && TALENT:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;愛
ELSEIF TALENT:恋心 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1
ENDIF
RETURN 0

;==================================================
;EVENTEND関係（X1をキャラ番号に置換） CFLAG 211～219を使用
;調教終了時のセリフ
;==================================================
@EVENTEND
SIF FLAG:口上使用 <= 0
	RETURN 0
SIF NO:TARGET != X1
	RETURN 0

;キャラ死亡or瀕死時は口上をスキップ
SIF BASE:体力 < 500
	RETURN 0

;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
;反発刻印Lv3+愛なし
IF MARK:反発刻印 == 3 && TALENT:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv1以下+愛なし
ELSEIF MARK:屈服刻印 <= 1 && TALENT:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv2+愛なし
ELSEIF MARK:屈服刻印 == 2 && TALENT:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv3+愛なし
ELSEIF MARK:屈服刻印 == 3 && TALENT:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;愛
ELSEIF TALENT:恋心 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 
	RETURN 1
ENDIF
RETURN 0

;==================================================
;@KOJO_MESSAGE_COM関係（X1をキャラ番号に置換）
;コマンド実行時に出力されます
;==================================================
@KOJO_MESSAGE_COM_X1
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0 
;ボールギャグ着用時には口上をスキップする
SIF TEQUIP:ボールギャグ && SELECTCOM != [[TRAIN:ボールギャグ]]
	RETURN 0

;-------------------------------------------------
;コマンド実行時のセリフ CFLAG 301～500を使用
;-------------------------------------------------
;CSVフォルダ内のTrain.csvに記述されている調教コマンド番号を
;下の汎用調教コマンドテンプレートに記述されているxxとzzに入れ
;調教コマンド番号に+1した数字をyyに入れる
;・ただし、yyに入れる数字が１桁の場合は01や02のように最初に0を付ける
;　例:愛撫の場合 xx → 0, yy → 01
;　　 正常位の場合 xx → 20, yy → 21
;・ただし、触手調教の場合のみはzzの値を100にする
;
;～・～・～・～・～・～・～・～・～・～・～・～・～
;汎用調教コマンドテンプレート
;～・～・～・～・～・～・～・～・～・～・～・～・～
;-------------------------------------------------
;調教コマンド名 CFLAG:3yy 
;-------------------------------------------------
;IF SELECTCOM == xx
;	;初めて
;	IF CFLAG:3yy == 0
;		PRINTFORMW 
;		CFLAG:TARGET:3yy = 1
;		RETURN 0
;	;二回目以降
;	ELSE
;		PRINTFORMW 
;		CFLAG:3yy = 2
;		RETURN 0
;	ENDIF
;ENDIF
;～・～・～・～・～・～・～・～・～・～・～・～・～
;開始時と終了時で口上を変える必要のあるコマンド
;対象となるコマンド
;バイブ、アナルバイブ、クリキャップ、ニプルキャップ、搾乳器、オナホール
;アイマスク、縄、ボールギャグ、浣腸セット、アナル電極、
;ビデオ撮影、野外プレイ、羞恥プレイ、お風呂場プレイ、新妻プレイ、
;獣姦プレイ、触手調教（触手調教の場合 zz には100を入れる）
;～・～・～・～・～・～・～・～・～・～・～・～・～
;-------------------------------------------------
;調教コマンド名 CFLAG:3yy 　CFLAG:4yy
;-------------------------------------------------
;開始時
;IF SELECTCOM == xx && TEQUIP:zz
;	;初めて
;	IF CFLAG:3yy == 0
;		PRINTFORMW 
;		CFLAG:3yy = 1
;		RETURN 0
;	;二回目以降
;	ELSEIF CFLAG:3yy == 1 || FLAG:口上使用 == 2
;		PRINTFORMW 
;		CFLAG:3yy = 2
;		RETURN 0
;	ENDIF
;終了時
;ELSEIF SELECTCOM == xx && TEQUIP:zz == 0
;	;初めて
;	IF CFLAG:4yy == 0
;		PRINTFORMW 
;		CFLAG:4yy = 1
;		RETURN 0
;	;二回目以降
;	ELSEIF CFLAG:4yy == 1 || FLAG:口上使用 == 2
;		PRINTFORMW 
;		CFLAG:4yy = 2
;		RETURN 0
;	ENDIF
;ENDIF
;～・～・～・～・～・～・～・～・～・～・～・～・～

;-------------------------------------------------
;キスする CFLAG:307
;-------------------------------------------------
IF SELECTCOM == [[TRAIN:キスする]]
	;初キスで愛かつ主人
	IF CFLAG:307 == 0 && TFLAG:初キス && TALENT:恋心 && ASSIPLAY == 0
		PRINTFORMW 
	ENDIF
	CFLAG:307 = 1
	RETURN 0
ENDIF

;-------------------------------------------------
;会話する CFLAG:357
;-------------------------------------------------
IF SELECTCOM == [[TRAIN:会話する]]
	;初めて
	IF CFLAG:357 == 0
		PRINTFORMW 
		CFLAG:357 = 1
	;二回目以降
	ELSEIF CFLAG:357 == 1
		;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
		IF RAND:3 == 0
			PRINTFORMW 
		ELSEIF RAND:2 == 0
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


;==================================================
;@KOJO_MESSAGE_PALAMCNG関係（X1をキャラ番号に置換）
;パラメータ変動をトリガーにした口上
;パラメータ変動後に口上を発動します
;==================================================
@KOJO_MESSAGE_PALAMCNG_X1
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0 
;ボールギャグ着用時には口上をスキップする
SIF TEQUIP:ボールギャグ
	RETURN 0
;-------------------------------------------------
;パラメータ変動時のセリフ CFLAG 221～260を使用
;-------------------------------------------------
;-------------------------------------------------
;処女喪失(処女のみ) CFLAG:229
;-------------------------------------------------
A = UP:[[UP:反感]] + UP:[[UP:不快]]
IF TFLAG:処女喪失 == 1 && CFLAG:229 == 0
	;【愛持ちで】主人による処女喪失・かつ反発刻印取得せず
	IF TFLAG:主人による処女喪失 == 1 && TALENT:恋心 == 1 && (A < 500 || TFLAG:反発刻印の回避 == 1)
		PRINTFORMW 
	;それ以外の処女喪失
	ELSE
		PRINTFORMW 
	ENDIF
	CFLAG:TARGET:229 = 1
ENDIF

;-------------------------------------------------
;@KOJO_MESSAGE_MARKCNG関係（X1をキャラ番号に置換）
;刻印変動をトリガーにした口上
;刻印変動後に口上を発動します
;-------------------------------------------------
@KOJO_MESSAGE_MARKCNG_X1
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0 
;ボールギャグ着用時には口上をスキップする（OFFだと口を塞いでるのに喋りまくる）
SIF TEQUIP:ボールギャグ
	RETURN 0

;-------------------------------------------------
;苦痛刻印Lv3取得 CFLAG:297
;-------------------------------------------------
IF TFLAG:苦痛刻印 == 3 && CFLAG:297 == 0
	PRINTFORMW 
	CFLAG:297 = 1
ENDIF

;-------------------------------------------------
;快楽刻印Lv3取得 CFLAG:298
;-------------------------------------------------
IF TFLAG:快楽刻印 == 3 && CFLAG:298 == 0
	PRINTFORMW 
	CFLAG:298 = 1
ENDIF

;-------------------------------------------------
;屈服刻印Lv3取得 CFLAG:299
;-------------------------------------------------
IF TFLAG:屈服刻印 == 3 && CFLAG:299 == 0
	PRINTFORMW 
	CFLAG:299 = 1
ENDIF

;-------------------------------------------------
;反発刻印Lv3取得 CFLAG:300
;-------------------------------------------------
IF TFLAG:反発刻印 == 3 && CFLAG:300 == 0
	PRINTFORMW 
	CFLAG:300 = 1
ENDIF


;==================================================
;イベント口上集（X1、X2をキャラ番号に置換する） CFLAG 261～296
;==================================================
@SELF_KOJO_KX1
;-------------------------------------------------
;調教後自慰 CFLAG:261
;-------------------------------------------------
IF TFLAG:初キス == 1
	IF CFLAG:261 < 1 || FLAG:口上使用 == 2
		PRINTFORMW 
		CFLAG:261 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;特定の相手との調教後レズプレイ発生 CFLAG:262
;Ynを調教対象X1との会話が発生する助手のキャラ番号に変更する
;-------------------------------------------------
;IF TFLAG:初キス == 2 && ASSI > 0
;	;助手○○（○○にY1のキャラ名を入れる）
;	IF NO:ASSI == Y1 && (CFLAG:262 < 1 || FLAG:口上使用 == 2)
;		PRINTFORMW 
;		CFLAG:262 = 1
;	;助手○○（○○にY2のキャラ名を入れる）
;	ELSEIF NO:ASSI == Y2 && (CFLAG:262 < 1 || FLAG:口上使用 == 2)
;		PRINTFORMW 
;		CFLAG:262 = 1
;	ENDIF
;ENDIF

;-------------------------------------------------
;朝フェラ CFLAG:263
;-------------------------------------------------
IF TFLAG:初キス == 3
	IF CFLAG:263 < 1 || FLAG:口上使用 == 2
		PRINTFORMW 
		CFLAG:263 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;主人との調教後セックス CFLAG:264
;-------------------------------------------------
IF TFLAG:初キス == 4
	IF CFLAG:264 < 1 || FLAG:口上使用 == 2
		PRINTFORMW 
		CFLAG:264 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;夜這い CFLAG:265
;-------------------------------------------------
IF TFLAG:初キス == 5
	IF CFLAG:265 < 1 || FLAG:口上使用 == 2
		PRINTFORMW 
		CFLAG:265 = 1
	ENDIF
ENDIF

;-------------------------------------------------
;売却
;-------------------------------------------------
IF TFLAG:初キス == 6
	;愛+反発刻印Lv3未満
	IF TALENT:恋心 && MARK:反発刻印 < 3
		PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
;ガンダーロボ戦に参加
;-------------------------------------------------
IF TFLAG:初キス == 9
	DRAWLINE
	PRINTFORMW 
	DRAWLINE
ENDIF

;-------------------------------------------------
;ガンダーロボ戦に参加して勝利
;-------------------------------------------------
IF TFLAG:初キス == 10
	DRAWLINE
	PRINTFORMW 
	DRAWLINE
ENDIF

;-------------------------------------------------
;妊娠発覚 CFLAG:271
;-------------------------------------------------
IF TFLAG:初キス == 11
	SIF CFLAG:271 >= 1
		RETURN 0
	;※崩壊してしまった場合は非表示
	IF TALENT:崩壊 == 1
		RETURN 0
	;父親が主人で母親が愛持ち
	ELSEIF TALENT:恋心 && CFLAG:妊娠精子 == 1
		PRINTFORMW 
	ENDIF
	CFLAG:271 = 1
ENDIF

;-------------------------------------------------
;出産 CFLAG:272
;-------------------------------------------------
IF TFLAG:初キス == 12
	SIF CFLAG:272 >= 1
		RETURN 0
	;※崩壊してしまった場合は非表示
	IF TALENT:崩壊 == 1
		RETURN 0 
	;父親が主人で母親が愛持ち
	ELSEIF TALENT:恋心 && CFLAG:妊娠精子 == 1
		PRINTFORMW 
	ENDIF
	CFLAG:272 = 1
ENDIF

;-------------------------------------------------
;死亡
;-------------------------------------------------
IF TFLAG:初キス == 999
	;愛
	IF TALENT:恋心
		PRINTFORMW 
	ENDIF
ENDIF

;--------------------------------------------------
;フラグ初期化
;--------------------------------------------------
TFLAG:初キス = 0

RETURN 0


