	;初めて
	IF CFLAG:301 == 0
		;屈服刻印Lv2以上
		IF MARK:屈服刻印 >= 2
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
		CFLAG:301 = 1
		RETURN 0
	;二回目以降
	ELSE
		;愛
		IF TALENT:TARGET:恋心 == 1 && (CFLAG:301 <= 4 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 5
		;屈服刻印Lv3
		ELSEIF MARK:屈服刻印 == 3 && (CFLAG:301 <= 3 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 4
		;屈服刻印Lv2
		ELSEIF MARK:屈服刻印 == 2 && (CFLAG:301 <= 2 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 3
		;それ以外
		ELSEIF MARK:屈服刻印 <= 1 && (CFLAG:301 <= 1 || FLAG:口上使用 == 2)
			PRINTFORMW 
			CFLAG:301 = 2
		ENDIF
		RETURN 0
	ENDIF
