;	DRAWLINE
;	;初めて
;	IF CFLAG:202 == 0
;		;既に愛持ちで愛取得時初口上（陥落イベント）が発生済み
;		IF TALENT:TARGET:恋心 == 1 && CFLAG:201 >= 5
;			PRINTFORMW 
;		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;		CFLAG:202 = 1
;		RETURN 1
;	;二回目以降
;	ELSE CFLAG:202 == 1 && FLAG:口上使用 ==2
;		;愛
;		IF TALENT:TARGET:恋心 == 1
;			PRINTFORMW 
;		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;		RETURN 1
;	ENDIF
;助手○○（○○にY2のキャラ名を入れる）
;ELSEIF NO:ASSI == Y2
;	DRAWLINE
;	;初めて
;	IF CFLAG:203 == 0
;		;既に愛持ちで愛取得時初口上（陥落イベント）が発生済み
;		IF TALENT:TARGET:恋心 == 1 && CFLAG:201 >= 5
;			PRINTFORMW 
