;		ELSE
;			PRINTFORMW 
;		ENDIF
;		CFLAG:203 = 1
;		RETURN 1
;	;二回目以降
;	ELSE CFLAG:203 == 1 && FLAG:口上使用 ==2
;		;愛
;		IF TALENT:TARGET:恋心 == 1
;			PRINTFORMW 
;		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;		RETURN 1
;	ENDIF
;口上のある助手が居ない場合は、通常の二回目以降の口上へ飛ぶ
ELSE
	CALL KX1_KOJO2
ENDIF


;-------------------------------------------------
;調教開始時（2回目以降、X1をキャラ番号に変更する） CFLAGなし 
;-------------------------------------------------
@KX1_KOJO2
;反発刻印Lv3
IF MARK:反発刻印 == 3 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「……こっち来ないで。顔も見たくないの」
	RETURN 1

;屈服刻印Lv0
ELSEIF MARK:屈服刻印 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「今日はどんなことをするの？」
	RETURN 1

;屈服刻印Lv1
ELSEIF MARK:屈服刻印 == 1 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「今日はどんなことをするのかな……」
	RETURN 1

;屈服刻印Lv2
ELSEIF MARK:屈服刻印 == 2 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「今日はどんなことをするの？」
	RETURN 1

;屈服刻印Lv3＋愛無し
ELSEIF MARK:屈服刻印 == 3 && TALENT:TARGET:恋心 == 0 && FLAG:口上使用 == 2
	DRAWLINE
	PRINTFORMW 「今日はどんなことをするの♪」
	RETURN 1

;愛
ELSEIF TALENT:TARGET:恋心 == 1 && FLAG:口上使用 == 2
	DRAWLINE
	;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
	IF RAND:3 == 0
		PRINTFORMW 
	ELSEIF RAND:2 == 0
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1
ENDIF
RETURN 0

;-------------------------------------------------
;EVENTEND関係（X1をキャラ番号に置換） CFLAG 211～220を使用
;調教終了時のセリフ
;-------------------------------------------------
@EVENTEND
SIF FLAG:口上使用 <= 0
	RETURN 0
SIF NO:TARGET != X1
	RETURN 0

;キャラ死亡時は口上をスキップ
SIF BASE:体力 <= 0
	RETURN 0

;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
;反発刻印Lv3+愛なし
IF MARK:反発刻印 == 3 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv1以下+愛なし
ELSEIF MARK:屈服刻印 <= 1 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv2+愛なし
ELSEIF MARK:屈服刻印 == 2 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;屈服刻印Lv3+愛なし
ELSEIF MARK:屈服刻印 == 3 && TALENT:TARGET:恋心 == 0
	DRAWLINE
	PRINTFORMW 
	RETURN 1

;愛(体力500以上)
ELSEIF TALENT:TARGET:恋心 == 1 && BASE:体力 >= 500
	DRAWLINE
	PRINTFORMW 
	RETURN 1
;愛(体力500未満)
ELSEIF TALENT:TARGET:恋心 == 1 && BASE:体力 <= 500
	DRAWLINE
	PRINTFORMW 
	RETURN 1
ENDIF
RETURN 0

